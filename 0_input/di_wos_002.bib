@article{ ISI:000417281000003,
Author = {Brail, Shauna},
Title = {{Promoting innovation locally: Municipal regulation as barrier or boost?}},
Journal = {{GEOGRAPHY COMPASS}},
Year = {{2017}},
Volume = {{11}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{This paper examines a seemingly abrupt series of shifts that have the
   potential to completely transform cities as a direct result of a rising
   digital economy. It takes into account the convergence of research from
   three areas of study: (a) urban concentrations of innovative activities,
   (b) the platform economy, and (c) municipal regulation. A case study
   discussion about the role of municipal regulation in ride-hailing, with
   particular emphasis on Uber, highlights debates regarding the challenges
   faced by municipalities amidst the prospect of renewed innovative
   activities and concomitant economic growth, alongside unknown risks. The
   paper concludes with a discussion about questions and approaches to
   understanding innovative activities in light of the emerging platform
   economy, and reviews key questions regarding the role that municipal
   regulatory decisions play in shaping the twenty-first century city.}},
