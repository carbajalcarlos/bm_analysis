@article{ ISI:000424603100002,
Author = {Dias Caetano, Luis Miguel and Nogueira do Nascimento, Marcia Mychelle},
Title = {{Integration of Digital Resources in Elementary Education}},
Journal = {{REVISTA EDUCAONLINE}},
Year = {{2017}},
Volume = {{11}},
Number = {{3}},
Pages = {{15-32}},
Month = {{SEP-DEC}},
Abstract = {{The use of technologies in pedagogical practices is an issue of
   fundamental importance, once we experience the constant presence of
   technologies in all spheres of society. Thus, education needs to adapt
   to this scenario and insert technological resources into its objectives
   as a way of enabling education that is in tune with the interests of
   society. Thus, the present work aimed to understand the use of digital
   resources in the pedagogical practices of elementary school teachers,
   identifying the difficulties in using them, as well as knowing some
   digital resources for use in elementary education. This study followed a
   qualitative methodology, of a bibliographic nature, with an exploratory
   investigative character, in the sense that, it was proposed to
   investigate on the use of digital resources in pedagogical practices. He
   used as a corpus, questionnaires that were applied to elementary school
   teachers of the Padre Cosme State School, located in the municipality of
   Sao Miguel / RN. The results allowed us to identify the digital
   resources provided by the Ministry of Education and with enormous
   pedagogical potential to favor the development of innovative teaching
   and learning strategies. However, teachers present as the main
   difficulty to use the technologies in the construction of pedagogical
   practices, the need for technical and mainly pedagogical training and
   consequently their use in the educational context.}},
