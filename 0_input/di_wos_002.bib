@article{ ISI:000404306600003,
Author = {Ozkil, Ali Gurcan},
Title = {{Collective design in 3D printing: A large scale empirical study of
   designs, designers and evolution}},
Journal = {{DESIGN STUDIES}},
Year = {{2017}},
Volume = {{51}},
Pages = {{66-89}},
Month = {{JUL}},
Abstract = {{This paper provides an empirical study of a collective design platform
   (Thingiverse); with the aim of understanding the phenomenon and
   investigating how designs concurrently evolve through the large and
   complex network of designers. The case study is based on the meta-data
   collected from 158 489 designs and 247 768 users; and it reveals that
   (i) Designs can be shared and quickly evolved into other designs through
   a distributed network of designers, (ii) only a small portion of the
   users are designers and (iv) collective design has deep and strong
   evolutionary roots. Better understanding of collective design platforms
   can help design practitioners to identify lead users in their respective
   domains and to discover latent needs that stem from different
   sub-communities or geographic regions. (C) 2017 Elsevier Ltd. All rights
   reserved.}},
