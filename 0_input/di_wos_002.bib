@article{ ISI:000404231300011,
Author = {Bucci, Giovanni and Ciancetta, Fabrizio and Fiorucci, Edoardo and
   Ometto, Antonio},
Title = {{Survey about Classical and Innovative Definitions of the Power
   Quantities Under Nonsinusoidal Conditions}},
Journal = {{INTERNATIONAL JOURNAL OF EMERGING ELECTRIC POWER SYSTEMS}},
