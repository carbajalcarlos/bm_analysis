@article{ ISI:000414911000024,
Author = {Xu, Jiachen and Liu, Xiao and Ma, Ming and Liu, Anfeng and Wang, Tian
   and Huang, Changqin},
Title = {{Intelligent Aggregation Based on Content Routing Scheme for Cloud
   Computing}},
