@article{ ISI:000404237800001,
Author = {Garcia Aretio, Lorenzo},
Title = {{Distance and virtual education: quality, disruption, adaptive learning
   and mobile learning}},
Journal = {{RIED-REVISTA IBEROAMERICANA DE EDUCACION A DISTANCIA}},
Year = {{2017}},
Volume = {{20}},
Number = {{2}},
Pages = {{9-25}},
Month = {{JUL}},
Abstract = {{Although faced with resistances, distance and digital education is
   overtaking face-to-face formats. When pedagogical designs are
   successful, there is evidence of the quality of digital learning. The
   effectiveness of these systems is at least similar to that of
   face-to-face proposals. This paper introduces the debate on innovations
   and disruptive technologies, arguing that digital learning (as a new
   version of distance education) is an educational disruption both because
   it brings about a drastic change of media and methods and because it is
   continually gaining ground from conventional formats. Finally, it is
   claimed that this trend is not exhausted, but continues to penetrate and
   deepen the break with new technologies, such as learning analytics,
   adaptive learning and mobile learning.}},
