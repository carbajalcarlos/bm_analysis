@article{ ISI:000407493600008,
Author = {Kreiss, Daniel and Saffer, Adam J.},
Title = {{Networks and Innovation in the Production of Communication: Explaining
   Innovations in US Electoral Campaigning From 2004 to 2012}},
Journal = {{JOURNAL OF COMMUNICATION}},
Year = {{2017}},
Volume = {{67}},
Number = {{4}},
Pages = {{521-544}},
Month = {{AUG}},
Abstract = {{We outline a network analytic framework for analyzing the production of
   communication. In our framework, individualsin part the products of the
   history of their social and professional tiesmerge from various fields
   and a medley of prior production experiences within organizations to
   produce communicative innovations. Organizations with individuals who
   have diverse backgrounds and significant overlap in work experiences
   will be more innovative. We demonstrate this through a network analysis
   of the professional biographies of 629 staffers on U.S. presidential
   campaigns from 2004 to 2012. Democratic staffers came from more diverse
   organizations and shared significant overlap in prior experiences than
   their Republican counterparts. Through interview data, we argue that
   this in part explains Democratic innovativeness in technology during
   this period.}},
