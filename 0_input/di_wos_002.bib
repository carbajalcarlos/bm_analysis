Title = {{Circuit and System Designs of Ultra-Low Power Sensor Nodes With
   Illustration in a Miniaturized GNSS Logger for Position Tracking: Part
   II-Data Communication, Energy Harvesting, Power Management, and Digital
   Circuits}},
Journal = {{IEEE TRANSACTIONS ON CIRCUITS AND SYSTEMS I-REGULAR PAPERS}},
Year = {{2017}},
Volume = {{64}},
