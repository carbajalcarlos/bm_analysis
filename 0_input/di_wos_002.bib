@article{ ISI:000423922400006,
Author = {Shareski, Dean},
Title = {{Instilling Joy in a Digital Age}},
Journal = {{LEARNING LANDSCAPES}},
Year = {{2017}},
Volume = {{11}},
Number = {{1}},
Pages = {{31-36}},
Month = {{FAL}},
Abstract = {{In this interview, digital consultant and author Dean Shareski talks
   about the fundamental aspects of authentic learning: joy, wonder, play,
   and community. He believes that technology can bring a sense of fun and
   curiosity to the classroom by allowing students to do things they
   couldn't do before, like editing photographs or meeting a scientist from
   another part of the world. School communities can now share their
   innovations with the community at large and also with schools in
   different countries. Teachers, he says, should see themselves as artists
   and not be afraid to play and explore new opportunities. As final advice
   to teachers and parents, he suggests that we let kids be kids and
   experience the joy of childhood.}},
