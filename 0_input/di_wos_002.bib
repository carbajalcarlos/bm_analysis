@article{ ISI:000416790500001,
Author = {Gaiani, Marco and Apollonio, Fabrizio Ivan and Ballabeni, Andrea and
   Remondino, Fabio},
Title = {{Securing Color Fidelity in 3D Architectural Heritage Scenarios}},
Journal = {{SENSORS}},
Year = {{2017}},
Volume = {{17}},
Number = {{11}},
Month = {{NOV}},
Abstract = {{Ensuring color fidelity in image-based 3D modeling of heritage scenarios
   is nowadays still an open research matter. Image colors are important
   during the data processing as they affect algorithm outcomes, therefore
   their correct treatment, reduction and enhancement is fundamental. In
   this contribution, we present an automated solution developed to improve
   the radiometric quality of an image datasets and the performances of two
   main steps of the photogrammetric pipeline (camera orientation and dense
   image matching). The suggested solution aims to achieve a robust
   automatic color balance and exposure equalization, stability of the
   RGB-to-gray image conversion and faithful color appearance of a
   digitized artifact. The innovative aspects of the article are: complete
   automation, better color target detection, a MATLAB implementation of
   the ACR scripts created by Fraser and the use of a specific weighted
   polynomial regression. A series of tests are presented to demonstrate
   the efficiency of the developed methodology and to evaluate color
   accuracy (color characterization').}},
