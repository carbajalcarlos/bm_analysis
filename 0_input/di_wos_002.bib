@article{ ISI:000416225500005,
Author = {Wilson, Rob and Baines, Sue and Martin, Mike and Richter, Paul and
   McLoughlin, Ian and Maniatopoulos, Gregory},
Title = {{Who is telecaring whom? Exploring the total social organisation of care
   work in an Italian municipality}},
Journal = {{NEW TECHNOLOGY WORK AND EMPLOYMENT}},
