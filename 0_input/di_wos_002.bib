@article{ ISI:000413319600029,
Author = {Tafazoli, Mehdi and Shakeri, Mohsen and Baniassadi, Majid and Babaei,
   Alireza},
Title = {{Investigation of the geometric property hull for infiltrated solid oxide
   fuel cell electrodes}},
