@article{ ISI:000403488300009,
Author = {Smith, K. C. and Gordon, A. P.},
Title = {{Mechanical Characterization of Prosthetic Feet and Shell Covers Using a
   Force Loading Apparatus}},
Journal = {{EXPERIMENTAL MECHANICS}},
Year = {{2017}},
Volume = {{57}},
Number = {{6}},
Pages = {{953-966}},
Month = {{JUL}},
Abstract = {{To assist in the redirection of kinetic energy many modern prosthetic
   feet often utilize a so called energy storing and return (ESAR) design
   that is achieved through dorsiflexion and elastic bending to facilitate
   forward propulsion during push-off. Consequently, the proper selection
   of the foot stiffness and mechanical response for an individual amputee
   is significant; however, the component stiffness and mechanical
