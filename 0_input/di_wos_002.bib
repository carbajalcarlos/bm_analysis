@article{ ISI:000407564300018,
Author = {Mosconi, Gaia and Korn, Matthias and Reuter, Christian and Tolmie, Peter
   and Teli, Maurizio and Pipek, Volkmar},
Title = {{From Facebook to the Neighbourhood: Infrastructuring of Hybrid Community
   Engagement}},
Journal = {{COMPUTER SUPPORTED COOPERATIVE WORK-THE JOURNAL OF COLLABORATIVE
   COMPUTING}},
Year = {{2017}},
Volume = {{26}},
