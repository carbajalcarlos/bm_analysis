@article{ ISI:000406894100004,
Author = {Boldt, Johannes and Vuck, Alexander and Horvath, Sebastian},
Title = {{Possibilities of digital workflows for single-tooth implant restorations}},
Journal = {{IMPLANTOLOGIE}},
Year = {{2017}},
Volume = {{25}},
Number = {{2}},
Pages = {{147-161}},
Month = {{JUN}},
Abstract = {{Digital technologies are increasingly used in implant dentistry. Every
   treatment step can be planned digitally, which results in a treatment
   outcome that is less invasive and more precise and efficient. Innovative
   computer-aided design/computer-assisted manufacturing (CAD/CAM)
   materials for the chairside application expand the digital workflow from
   restoration planning to fabrication and enable a predictable and
   reproducible result for practitioners in private practice. This virtual
   treatment procedure is beneficial for both the patient and dentist
   because it reduces the number of mistakes and results in material
   savings, fewer costs, and a more efficient allocation of time. This
   article provides an overview of digital workflows in implantology and
   implant prosthodontics and discusses available materials displayed with
   clinical examples.}},
