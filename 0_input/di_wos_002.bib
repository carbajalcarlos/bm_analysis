@article{ ISI:000404620800007,
Author = {Richardson, Lizzie},
Title = {{Sharing as a postwork style: digital work and the co-working office}},
Journal = {{CAMBRIDGE JOURNAL OF REGIONS ECONOMY AND SOCIETY}},
