@article{ ISI:000403471100004,
Author = {Hien Thu Tran and Santarelli, Enrico},
Title = {{Spatial heterogeneity, industry heterogeneity, and entrepreneurship}},
Journal = {{ANNALS OF REGIONAL SCIENCE}},
