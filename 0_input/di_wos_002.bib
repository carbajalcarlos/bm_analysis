@article{ ISI:000404281400005,
Author = {Philipp, Patrick and Maleshkova, Maria and Rettinger, Achim and Katic,
   Darko},
Title = {{A SEMANTIC FRAMEWORK FOR SEQUENTIAL DECISION MAKING FOR JOURNAL OF WEB
   ENGINEERING}},
Journal = {{JOURNAL OF WEB ENGINEERING}},
