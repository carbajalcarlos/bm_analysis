@article{ ISI:000408788000011,
Author = {De Santis, Stefano and Ceroni, Francesca and de Felice, Gianmarco and
   Fagone, Mario and Ghiassi, Bahman and Kwiecien, Arkadiusz and Lignola,
   Gian Piero and Morganti, Mattia and Santandrea, Mattia and Valluzzi,
   Maria Rosa and Viskovic, Alberto},
Title = {{Round Robin Test on tensile and bond behaviour of Steel Reinforced Grout
   systems}},
