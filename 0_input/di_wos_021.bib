@article{ ISI:000259575200029,
Author = {Hill, Douglas and Hamilton, Rhonda and Fung, Tak},
Title = {{Assessment of techniques to reduce sclerosant foam migration during
   ultrasound-guided sclerotherapy of the great saphenous vein}},
Journal = {{JOURNAL OF VASCULAR SURGERY}},
Year = {{2008}},
Volume = {{48}},
Number = {{4}},
Pages = {{934-939}},
