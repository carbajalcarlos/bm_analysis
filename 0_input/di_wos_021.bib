@inproceedings{ ISI:000254688900012,
Author = {Mora, Javier and Lee, Won-Sook},
Editor = {{Coquillart, S and Sturzlinger, W and Kiyokawa, K}},
Title = {{Real-time 3D fluid interaction with a haptic user interface}},
