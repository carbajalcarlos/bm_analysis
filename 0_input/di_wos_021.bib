@article{ ISI:000251947400005,
Author = {Angrisani, Leopoldo and Battaglia, Luigi and Napolitano, Aniello and Lo
   Moriello, Rosario Schiano},
Title = {{On the suitability of GRID Service Technology for distributed
   measurements based on complex digital signal processing algorithms}},
Journal = {{IEEE TRANSACTIONS ON INSTRUMENTATION AND MEASUREMENT}},
