@inproceedings{ ISI:000258885400046,
Author = {Verdegem, Pieter and De Marez, Lieven},
Editor = {{Filipe, J and Marca, DA and Shishkov, B and VanSinderen, M}},
Title = {{Conditions for technology acceptance - Broadening the scope of
   determinants of ICT appropriation}},
Booktitle = {{ICE-B 2008: PROCEEDINGS OF THE INTERNATIONAL CONFERENCE ON E-BUSINESS}},
Year = {{2008}},
Pages = {{292-299}},
