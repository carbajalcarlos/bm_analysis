@article{ ISI:000261362500008,
Author = {Adomavicius, Gediminas and Bockstedt, Jesse C. and Gupta, Alok and
   Kauffman, Robert J.},
Title = {{MAKING SENSE OF TECHNOLOGY TRENDS IN THE INFORMATION TECHNOLOGY
   LANDSCAPE: A DESIGN SCIENCE APPROACH}},
Journal = {{MIS QUARTERLY}},
