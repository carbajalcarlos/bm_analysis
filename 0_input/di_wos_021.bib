@inproceedings{ ISI:000258907700031,
Author = {Chiaroni, Davide and Chiesa, Vittorio},
Book-Group-Author = {{IEEE}},
Title = {{The co-evolution of strategic alliances in the ICT industry: the case of
   HP}},
Booktitle = {{IEMC - EUROPE 2008: INTERNATIONAL ENGINEERING MANAGEMENT CONFERENCE,
   EUROPE, CONFERENCE PROCEEDINGS: MANAGING ENGINEERING, TECHNOLOGY AND
   INNOVATION FOR GROWTH}},
