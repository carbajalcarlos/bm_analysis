@article{ ISI:000260578500019,
Author = {Brown, A. G.},
Title = {{Geoarchaeology, the four dimensional (4D) fluvial matrix and climatic
   causality}},
Journal = {{GEOMORPHOLOGY}},
Year = {{2008}},
Volume = {{101}},
Number = {{1-2, SI}},
