Editor = {{Vladareanu, L and Chiroiu, V and Bratu, P and Magheti, I}},
Title = {{Ways to improve sales management for Romanian retailers using
   Geographical Information Systems}},
Booktitle = {{PROCEEDINGS OF THE 9TH WSEAS INTERNATIONAL CONFERENCE ON MATHEMATICS \&
   COMPUTERS IN BUSINESS AND ECONOMICS (MCBE `08): MATHEMATICS AND
   COMPUTERS IN BUSINESS AND ECONOMICS}},
