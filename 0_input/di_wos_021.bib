@article{ ISI:000263168000006,
Author = {Paskaleva-Shapira, Krassimira and Azorin, Jose and Chiabai, Aline},
Title = {{Enhancing digital access to local cultural heritage through
   e-governance: innovations in theory and practice from Genoa, Italy}},
