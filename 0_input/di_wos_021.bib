@article{ ISI:000250812600003,
Author = {Krieger, Gerhard and Moreira, Alberto and Fiedler, Hauke and Hajnsek,
   Irena and Werner, Marian and Younis, Marwan and Zink, Manfred},
Title = {{TanDEM-X: A satellite formation for high-resolution SAR interferometry}},
