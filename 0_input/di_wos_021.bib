@inproceedings{ ISI:000261605800010,
Author = {Karadimas, Nikolaos V. and Papatzelou, Katerina and Papantoniou,
   Agisilaos N.},
Editor = {{Louca, LS and Chrysanthou, Y and Oplatkova, Z and AlBegain, K}},
Title = {{M-GOVERNMENT SERVICES IN GREECE}},
Booktitle = {{22ND EUROPEAN CONFERENCE ON MODELLING AND SIMULATION, PROCEEDINGS}},
Year = {{2008}},
Pages = {{71+}},
Note = {{22nd European Conference on Modelling and Simulation (ECMS 2008), Univ
   Cyprus, Nicosia, CYPRUS, JUN 03-06, 2008}},
Organization = {{European Council Modelling \& Simulat; IEEE; German Speaking Simuat Soc;
   Federat European Simulat Soc; Polish Soc Comp Simulat; Latvian Simuat
   Soc}},
Abstract = {{With the advances in Information Communication Technology (ICT) and the
   demand for more efficient and effective government, governments in
   various countries have implemented e-government strategies to offer more
   information and online services to citizens, government agencies,
   businesses, and others. E-government initiatives aim to benefit from the
   use of most innovative forms of information technologies, mainly
   web-based Internet applications, in improving governments' fundamental
   functions. It is considered the preferred tool to enhance government
   services among its customers and government agencies. However, an
   explosion in the use of wireless internet devices, including
   internet-ready mobile phones and Personal Digital Assistants (PDAs) in
   combination with the increasing mobility of people in industrial
   economies is forcing governments to prepare themselves to transition
   from e-government to m-government. This paper examines the development
   of in-government in Greece, which is in an early stage in comparison
   with other countries all over the world. More specifically, m-government
   services that are currently used are mentioned. In addition,
