Title = {{Introducing Free Surface Scanning, an optical, non-contact technique for
   recording the deformed free surface in towing tank}},
Booktitle = {{MARITIME INDUSTRY, OCEAN ENGINEERING AND COASTAL RESOURCES, VOLS 1 AND 2}},
Series = {{Proceedings and Monographs in Engineering Water and Earth Sciences}},
Year = {{2008}},
Volume = {{1-2}},
