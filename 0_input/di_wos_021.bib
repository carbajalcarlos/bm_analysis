@inproceedings{ ISI:000260353300139,
Author = {Ng, Richard Yew Fatt and Tay, Yong Haur and Mok, Kai Ming},
Editor = {{Zaman, HB and Sembok, TMT and VanRijsbergen, K and Zadeh, L and Buza, P and Shih, T and Taib, MN}},
Title = {{Iris Recognition Algorithms Based on Texture Analysis}},
Booktitle = {{INTERNATIONAL SYMPOSIUM OF INFORMATION TECHNOLOGY 2008, VOLS 1-4,
   PROCEEDINGS: COGNITIVE INFORMATICS: BRIDGING NATURAL AND ARTIFICIAL
   KNOWLEDGE}},
Year = {{2008}},
Pages = {{904-908}},
Note = {{International Symposium on Information Technology, Univ Kebangsaan, Fac
   Informat Sci \& Technol, Kuala Lumpur, MALAYSIA, AUG 26-29, 2008}},
Organization = {{IEEE}},
Abstract = {{Iris recognition has become a popular research in recent years due to
   its reliability and nearly perfect recognition rates. It-is recognition
   system has three main stages: image preprocessing, feature extraction
   and template matching. An innovative method is proposed to extract iris
   features based on texture analysis. Iris textures are analyzed to
   capture the discriminating frequency information. Specific filters with
   different center frequency are applied to three different zones to
   extract the texture of the iris. Different weightings are given to each
   zone depending on its contribution to the recognition. The encoded
   binary templates are compact in size and can avoid the visibility of the
   individual iris images. The templates are suitable for implementing iris
   recognition devices using DSP (Digital Signal Processor). The proposed
   method was evaluated using CASIA iris image database version 1.0 {[}1].
   Experimental results show that the proposed approach has achieved high
   accuracy of 98.62\%.}},
