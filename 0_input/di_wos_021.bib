@article{ ISI:000251745500075,
Author = {Van Renterghem, Koen and Pletinckx, Jo and Vandewege, Jan and
   Ternmennan, Serge},
Title = {{A reconfigurable digital platform for the real-time emulation of
   broadband copper access networks}},
Journal = {{IEEE TRANSACTIONS ON INSTRUMENTATION AND MEASUREMENT}},
Year = {{2007}},
Volume = {{56}},
Number = {{6}},
Pages = {{2731-2738}},
Month = {{DEC}},
Abstract = {{This paper presents a reconfigurable platform for the real-time digital
   emulation of broadband copper access networks. The instrument, which
   uses hard real-time digital-signal-processing techniques on Xilinx
   Virtex II field-programmable gate arrays, is capable of accurately
   reproducing the physical layer of a digital subscriber line (DSL). The
   magnitude and the true phase of the insertion and return loss of the
   twisted pair access loop, which consists of cables with various
   characteristics and length, are digitally emulated over the full DSL
   bandwidth. The innovative character and advantages of digital emulation
   techniques over conventional analog loop emulation are demonstrated, and
   its performance is assessed using several test cases.}},
