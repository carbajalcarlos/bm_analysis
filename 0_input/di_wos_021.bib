@inproceedings{ ISI:000257882400010,
Author = {Msanjila, Simon Samwel and Afsarmanesh, Hamideh},
Editor = {{Azevedo, A}},
Title = {{Automating trust assessment for configuration of temporary partnerships}},
Booktitle = {{INNOVATION IN MANUFACTURING NETWORKS}},
