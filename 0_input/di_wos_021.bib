@article{ ISI:000264754400009,
Author = {Retalis, Symeon},
Title = {{Creating Adaptive e-Learning Board Games for School Settings Using the
   ELG Environment}},
Journal = {{JOURNAL OF UNIVERSAL COMPUTER SCIENCE}},
Year = {{2008}},
