@inproceedings{ ISI:000258743700119,
Author = {Whyte, Jennifer K.},
Editor = {{Zavadskas, EK and Kaklauskas, A and Skibniewski, MJ}},
Title = {{Reaching for the skies: Time and turbulence in digital technologies and
   practices}},
Booktitle = {{25TH INTERNATIONAL SYMPOSIUM ON AUTOMATION AND ROBOTICS IN CONSTRUCTION
   - ISARC-2008}},
Year = {{2008}},
Pages = {{836-847}},
