@article{ ISI:000269841700002,
Author = {Jahanshahi, Mohammad R. and Kelly, Jonathan S. and Masri, Sami F. and
   Sukhatme, Gaurav S.},
Title = {{A survey and evaluation of promising approaches for automatic
   image-based defect detection of bridge structures}},
Journal = {{STRUCTURE AND INFRASTRUCTURE ENGINEERING}},
