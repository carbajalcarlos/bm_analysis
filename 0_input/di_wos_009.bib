@inproceedings{ ISI:000417330202024,
Author = {Urbano-Lopez de Meneses, Beatriz and Gomez-Barrios, Xiomar A. and
   Martinez-Moran, Olegario and Elena Sanchez-Moran, Marta and Moran,
   Antonio and Gonzalez-Andres, Fernando},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{THE USE OF ICT AND THE FLIPPED LEARNING METHODOLOGY TO CLOSE THE
   SUBJECT'S PRACTICES TO THE LABOUR MARKET}},
Booktitle = {{ICERI2016: 9TH INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND
   INNOVATION}},
