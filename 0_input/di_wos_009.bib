@article{ ISI:000360313200002,
Author = {Ghazawneh, Ahmad and Henfridsson, Ola},
Title = {{A paradigmatic analysis of digital application marketplaces}},
Journal = {{JOURNAL OF INFORMATION TECHNOLOGY}},
