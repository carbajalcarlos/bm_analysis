@inproceedings{ ISI:000417330208068,
Author = {Walker, Richard},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{TECHNOLOGY ADOPTION TRENDS AND EDUCATIONAL CHANGE WITHIN UK HIGHER
   EDUCATION: REFLECTIONS ON THE UCISA SURVEY DATA (2001-2016)}},
