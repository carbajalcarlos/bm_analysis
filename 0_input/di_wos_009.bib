Title = {{The Effect of Local Vibration in Cross Country Skiing (Diagonal Stride)}},
Booktitle = {{SOCIETY, INTEGRATION, EDUCATION, VOL. III}},
Series = {{Sabiedriba Integracija Izglitiba-Society Integration Education}},
Year = {{2016}},
Pages = {{434-441}},
