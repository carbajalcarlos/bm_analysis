@article{ ISI:000362975000010,
Author = {Liu, Jun and Kauffman, Robert J. and Ma, Dan},
Title = {{Competition, cooperation, and regulation: Understanding the evolution of
   the mobile payments technology ecosystem}},
Journal = {{ELECTRONIC COMMERCE RESEARCH AND APPLICATIONS}},
Year = {{2015}},
