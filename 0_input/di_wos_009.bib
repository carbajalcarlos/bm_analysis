@inproceedings{ ISI:000417330201033,
Author = {Luojus, Satu and Kauppinen, Sami and Lahti, Janne},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{UNIVERSITY - AN ESSENTIAL PART OF THE OPEN INNOVATION FRAMEWORK?}},
Booktitle = {{ICERI2016: 9TH INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND
   INNOVATION}},
