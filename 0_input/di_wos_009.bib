@inproceedings{ ISI:000417330206041,
Author = {Giaever, Tonje H. and Mifsud, Louise and Gjolstad, Eli},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{TEACHERS' UNDERSTANDING AND PRACTICE OF CYBER ETHICS IN THE CLASSROOM}},
