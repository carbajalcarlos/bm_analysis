@inproceedings{ ISI:000382051400081,
Author = {Gattaz, Cristiane Chaves and Cruvinel, Paulo Estevao and Bernardes,
   Roberto Carlos},
Book-Group-Author = {{IEEE}},
Title = {{Leveraging Digital Knowledge Ecosystem Framework Implementation Case
   Study: Aligning Knowledge Management and Innovation Goals for
   Agricultural Aerial Pest Control}},
Booktitle = {{2016 IEEE TENTH INTERNATIONAL CONFERENCE ON SEMANTIC COMPUTING (ICSC)}},
