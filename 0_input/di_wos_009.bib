@inproceedings{ ISI:000376433200024,
Author = {Hossfeld, Steffen},
Book-Group-Author = {{Univ Latvia}},
Title = {{OPTIMIZATION ON DECISION MAKING DRIVEN BY DIGITALIZATION}},
Booktitle = {{NEW CHALLENGES OF ECONOMIC AND BUSINESS DEVELOPMENT - 2016}},
