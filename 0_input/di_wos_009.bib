@article{ ISI:000359640600010,
Author = {del Rosario Neira-Pineiro, Maria},
Title = {{Reading and writing about literature on the Internet. Two innovative
   experiences with blogs in higher education}},
Journal = {{INNOVATIONS IN EDUCATION AND TEACHING INTERNATIONAL}},
Year = {{2015}},
Volume = {{52}},
Number = {{5}},
Pages = {{546-557}},
Month = {{SEP 3}},
Abstract = {{This paper describes two innovative experiences with ICT in teacher
   training by means of the development of two blogs about children's and
   young adults' literature. The study of both activities shows the
   usefulness of blogs in higher education and their contribution to the
   linguistic and literary training of future teachers. The results show
   that the creation of a literature blog enhances communicative skills in
   a digital environment, enlarges the students' literary competence,
   improves the future teachers' didactic training, increases the
   participants' motivation and encourages independent learning and shared
   knowledge building by means of peer interaction.}},
