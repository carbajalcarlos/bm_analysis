@article{ ISI:000362517300016,
Author = {Cai, Le and Zhang, Suoming and Miao, Jinshui and Yu, Zhibin and Wang,
   Chuan},
Title = {{Fully Printed Foldable Integrated Logic Gates with Tunable Performance
   Using Semiconducting Carbon Nanotubes}},
