@article{ ISI:000365553500029,
Author = {Rabbolini, S. and Beretta, S. and Foletti, S. and Cristea, M. E.},
Title = {{Crack closure effects during low cycle fatigue propagation in line pipe
   steel: An analysis with digital image correlation}},
