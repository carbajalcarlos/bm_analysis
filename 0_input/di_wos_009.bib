@article{ ISI:000395259800001,
Author = {Giselle Maldonado de Leon, Maria and Vela Nava, Alma Silvia and Garza
   Guzman, Maria Rosalia},
Title = {{Impact of the use of the Smart Board Interactive whiteboard as an ICT
   tool on the English language learning of sixth grade students of a
   public primary school in Asturias, Spain}},
Journal = {{REVISTA DE INVESTIGACION EDUCATIVA DE LA ESCUELA DE GRADUADOS EN
   EDUCACION}},
Year = {{2015}},
Volume = {{6}},
Number = {{11}},
Pages = {{2-8}},
Month = {{JUL-DEC}},
Abstract = {{This article provides an analysis of the most relevant aspects relating
   to the pedagogic integration of the Information and Communication
   Technology (ICT), especially the Smart Board Interactive whiteboard
   (SBIW) in the teaching and learning of English. The objective was to
   describe and evaluate how the application of ICT as a technological tool
   affects English language acquisition among sixth grade students of a
   public primary school in Asturias, Spain. Following a qualitative
   ethnographic methodology, this study revealed descriptive information
   about the participants and the impact of the technology on the education
   process. The data collection was achieved via different qualitative
   techniques: direct observation, journals of the class and semi
   structured interviews with the teacher and the sixth grade students. An
   analysis of the results revealed elements, aspects and situations in
   which the use of these technological tools benefits the teaching
   methodology and the acquisition of a second language by the students.
   The conclusions indicate that the SBIW and the ICT in the classroom
   generate educational innovations applicable to language teaching
   methodologies, as well as directly influencing and enhancing the
   acquisition of language skills in the students.}},
