@article{ ISI:000367115100020,
Author = {Gimenez, Salvador Pinillos and Alati, Daniel Manha},
Title = {{Electrical behavior of the Diamond layout style for MOSFETs in X-rays
   ionizing radiation environments}},
Journal = {{MICROELECTRONIC ENGINEERING}},
Year = {{2015}},
Volume = {{148}},
Number = {{SI}},
Pages = {{85-90}},
Month = {{DEC 1}},
Abstract = {{This paper aims to describe some innovative layout styles, which are
   capable to boost the electrical performance and, in the same time, the
   Total Ionizing Dose (TID) tolerance of Metal-Oxide-Semiconductor (MOS)
   Field Effect Transistors (MOSFET), without burdening the current planar
   Complementary MOS (CMOS) Integrated Circuits (ICs) manufacturing
   processes. To illustrate the potential use of these new alternative
   devices in analog and digital CMOS ICs applications, this work focuses
   on the Diamond layout style for MOSFET that presents hexagonal gate
   geometry. The new effects associated to this innovative transistor
   structure and its modeling are presented and discussed in detail. Some
   experimental results are illustrated to evidence its use mainly in space
   and medical CMOS ICs applications. (C) 2015 Elsevier B.V. All rights
   reserved.}},
