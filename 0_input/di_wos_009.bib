@article{ ISI:000358648200070,
Author = {Chiang, Cheng-Ta and Lin, Jian-Xiang and Liu, Lin},
Title = {{Design of a CMOS Calibrated Monolithic Illumination Meter for Monitoring
   Solar Radiation of Tomato Crops}},
Journal = {{IEEE SENSORS JOURNAL}},
