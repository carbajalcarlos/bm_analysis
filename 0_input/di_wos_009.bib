@article{ ISI:000363044300002,
Author = {Scott, Kimberly A. and Sheridan, Kimberly M. and Clark, Kevin},
Title = {{Culturally responsive computing: a theory revisited}},
Journal = {{LEARNING MEDIA AND TECHNOLOGY}},
Year = {{2015}},
Volume = {{40}},
Number = {{4}},
Pages = {{412-436}},
Month = {{OCT 2}},
Abstract = {{Despite multiple efforts and considerable funding, historically
   marginalized groups (e.g., racial minorities and women) continue not to
   enter or persist in the most lucrative of fields - technology.
   Understanding the potency of culturally responsive teaching (CRT), some
   technology-enrichment programs modified CRP principles to establish a
   culturally responsive computing (CRC) experience for disenfranchised
   groups. We draw from our respective praxes developing two such
   initiatives and reconceptualize CRC as a heuristic. In this theoretical
   article, we offer a more nuanced vision of CRC considering
   intersectionality, innovations, and technosocial activism. Implications
   for the newly defined tenets consider programmatic, theoretical, and
   methodological concerns.}},
