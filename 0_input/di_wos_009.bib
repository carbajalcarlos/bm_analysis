@inproceedings{ ISI:000417330200107,
Author = {Scaradozzi, D. and Screpanti, L. and Cesaretti, L. and Mazzieri, E. and
   Storti, M. and Brandoni, M. and Longhi, A.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{``RETHINK LORETO: WE BUILD OUR SMART CITY!{''} A STEM EDUCATION
   EXPERIENCE FOR INTRODUCING SMART CITY CONCEPT WITH THE EDUCATIONAL
   ROBOTICS}},
Booktitle = {{ICERI2016: 9TH INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND
   INNOVATION}},
