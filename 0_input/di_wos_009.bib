@article{ ISI:000380161100003,
Author = {Edirisinghe, Ruwini and Lingard, Helen},
Title = {{Exploring the potential for the use of video to communicate safety
   information to construction workers: case studies of organizational use}},
Journal = {{CONSTRUCTION MANAGEMENT AND ECONOMICS}},
Year = {{2016}},
Volume = {{34}},
Number = {{6}},
Pages = {{366-376}},
Abstract = {{Legislation requires employers to provide health and safety (H\&S)
   training to workers and communicate safety rules, policies and
   procedures to them in a way they understand. As the construction
   industry has an increasingly multicultural and multilingual workforce,
   conventional written and verbal safety communication is of limited
   effectiveness. CodeSafe Solutions, has developed an innovative method of
   communicating H\&S information to field-based workers using digital
   media. The potential of the system was explored and two organizations in
   the Australian construction industry were used. Qualitative data from
   in-depth interviews of safety managers and workers involved in system
   implementation and quantitative data such as incident reports and video
   usage were analysed. Managers perceived the CodeSafe system to be
   beneficial and well received by workers. In one organization, the
   introduction of the CodeSafe system coincided with a reduction in injury
   rates. However, causal inferences about the impact of the system cannot
   be made. Workers involved in making the films believed films would be an
   effective communication tool. However, barriers to implementation of the
   technology include workers' reluctance to use personal smart phones,
   limited internet connectivity, and organizational and national
   regulations on mobile phone use on construction sites.}},
