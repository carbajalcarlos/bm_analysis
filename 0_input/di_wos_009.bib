@article{ ISI:000362336400006,
Author = {Li, Shu-Chu Sarrina},
Title = {{Lifestyles and the adoption of information versus entertainment
   technologies: An examination on the adoption of six new technologies in
   Taiwan}},
Journal = {{NEW MEDIA \& SOCIETY}},
Year = {{2015}},
Volume = {{17}},
Number = {{10}},
Pages = {{1696-1714}},
Month = {{NOV}},
Abstract = {{Using Rogers' diffusion of innovation model, this study examined the
   relationships between lifestyles and the adoption of six new
   technologies in Taiwan. Three of the six technologiesnotebooks,
   netbooks, and tablet PCsare information technologies, while the
   remaining three technologiesWii, Kinect, and digital videos (DVs)are
   entertainment technologies. A telephone survey was adopted to obtain a
   representative sample, which obtained 1157 valid calls with a response
   rate of 53.1\%. Two conclusions are drawn from the data analysis. First,
   lifestyles did play a significant role in predicting the adoption of the
   six technologies. Second, this study found that demographics and mass
   media use were more predictive of the adoption of information
   technologies than that of the adoption of entertainment technologies.}},
