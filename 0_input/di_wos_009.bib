@article{ ISI:000358136200004,
Author = {Mejia, Robert},
Title = {{A Pressure Chamber of Innovation: Google Fiber and Flexible Capital}},
Journal = {{COMMUNICATION AND CRITICAL-CULTURAL STUDIES}},
