@inproceedings{ ISI:000381861800037,
Author = {Sanchez, Eider and Toro, Carlos and Grana, Manuel},
Editor = {{Chen, YW and Torro, C and Tanaka, S and Howlett, RJ and Jain, LC}},
Title = {{Integrating Electronic Health Records in Clinical Decision Support
   Systems}},
