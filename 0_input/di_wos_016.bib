@article{ ISI:000209372700004,
Author = {Prins, J. E. J. and Broeders, Dennis and Griffioen, H. M.},
Title = {{iGovernment: A new perspective on the future of government digitisation}},
Journal = {{COMPUTER LAW \& SECURITY REVIEW}},
Year = {{2012}},
Volume = {{28}},
Number = {{3}},
Pages = {{273-282}},
Month = {{JUN}},
Abstract = {{Innovative use of ICT applications is rapidly becoming a cornerstone of
   modern government policy in every area of service, care and control.
   Looking beyond the individual applications and layers of digitisation,
   we find a hodgepodge of information flows running within and between the
   various public authorities, policy domains, and crossing the
   public/private boundary. This has consequences for the relation between
   government and citizens. Step by step, decision by decision, the
   everyday work of government is changing `the rules of the game' and
   giving rise to ``information Government{''} (iGovernment), without this
   being based on any overall strategic agenda or awareness among political
   decision-makers. This article places this development in a new framework
   and suggests a perspective on a necessary paradigm shift. (C) 2012
   J.E.J. Prins, D. Broeders, H.M. Griffioen. Published by Elsevier Ltd.
   All rights reserved.}},
