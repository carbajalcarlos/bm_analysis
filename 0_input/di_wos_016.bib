@inproceedings{ ISI:000316362900052,
Author = {Tiwari, Ashish and Sahu, Anil Kumar},
Book-Group-Author = {{IEEE}},
Title = {{An Innovative Approach of Computational Fault Detection using Design for
   Testability of CP-PLL}},
Booktitle = {{2012 NATIONAL CONFERENCE ON COMPUTING AND COMMUNICATION SYSTEMS (NCCCS)}},
Year = {{2012}},
Pages = {{279-284}},
Note = {{National Conference on Computing and Communication Systems (NCCCS),
   Durgapur, INDIA, NOV 21-22, 2012}},
Organization = {{Tata Consultancy Serv; Dept Atom Energy; State Bank India; Def Res \&
   Dev Org; Council Sci \& Ind Res; Infosys India Pvt Ltd; Dr B C Roy Engn
   Coll Soc; Dr B C Roy Engn Coll, Dept Comp Sci \& Engn; Dr B C Roy Engn
   Coll, Dept Informat Technol}},
Abstract = {{This paper presents an innovative approach to detect computational fault
   using design for testability (DFT) of CP-PLL (charge pump phase locked
   loop) to allow simple digital testing. The proposed structure is useful
   in mixed signal IC (incorporating both analog and digital block on the
   same chip) testing. With increasing complexity of mixed signal IC the
   demand of preparing the low cost testing circuitry also gets increased.
   Here CP-PLL is taken as the mixed signal IC wherein the proposed method
   uses the charge pump as stimulus generator and the VCO (voltage
   controlled oscillator) as measuring device for testing the CP-PLL. It
   avoids the need of interfacing any foreign component and decreases the
   area overhead of whole IC. Moreover, testing circuitry is applied at the
   digital part of the CP-PLL i.e. PFD (phase frequency detector) and the
   analog part i.e. charge pump; loop filter and VCO are controlled by PFD
   only. Consequently the efficiency of the testing process avoiding the
   loading effect at analog node is increased. Fault simulation results
   indicate that the proposed structure posses high fault coverage of
   98.2\% and less area overhead of about 3.025\%.}},
