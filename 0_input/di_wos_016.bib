@inproceedings{ ISI:000320481100020,
Author = {Cui, Weili and Miles, Ronald N. and Su, Quang and Homentcovsci, Dorel},
Book-Group-Author = {{ASME}},
Title = {{A BIO-INSPIRED MINIATURE COMB SENSE DIFFERENTIAL MICROPHONE DIAPHRAGM}},
