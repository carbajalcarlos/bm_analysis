@article{ ISI:000306757800016,
Author = {Collin, Antoine and Hench, James L.},
Title = {{Towards Deeper Measurements of Tropical Reefscape Structure Using the
   WorldView-2 Spaceborne Sensor}},
Journal = {{REMOTE SENSING}},
