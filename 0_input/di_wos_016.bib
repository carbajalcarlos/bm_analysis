@article{ ISI:000311533200017,
Author = {Chen, Albert Y. and Pena-Mora, Feniosky and Plans, Albert P. and Mehta,
   Saumil J. and Aziz, Zeeshan},
Title = {{Supporting Urban Search and Rescue with digital assessments of
   structures and requests of response resources}},
Journal = {{ADVANCED ENGINEERING INFORMATICS}},
