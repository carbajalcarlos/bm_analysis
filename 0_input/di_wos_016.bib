@inproceedings{ ISI:000351496100060,
Author = {Kristensen, Ena Lloret and Gramazio, Fabio and Kohler, Matthias and
   Langenberg, Silke},
Editor = {{Stouffs, R and Janssen, P and Roudavski, S and Tuncer, B}},
Title = {{COMPLEX CONCRETE CONSTRUCTIONS Merging existing casting techniques with
   digital fabrication}},
Booktitle = {{PROCEEDINGS OF THE 18TH INTERNATIONAL CONFERENCE ON COMPUTER-AIDED
   ARCHITECTURAL DESIGN RESEARCH IN ASIA (CAADRIA 2013): OPEN SYSTEMS}},
Year = {{2013}},
