Editor = {{Beldhuis, H}},
Title = {{Rethinking Creativity in Learning and Teaching With Technology in
   Romanian Higher Education}},
Booktitle = {{PROCEEDINGS OF THE 11TH EUROPEAN CONFERENCE ON E-LEARNING}},
