@article{ ISI:000307873600059,
Author = {Liu, Xiang and Chandrasekhar, S. and Winzer, P. J. and Chraplyvy, A. R.
   and Tkach, R. W. and Zhu, B. and Taunay, T. F. and Fishteyn, M. and
   DiGiovanni, D. J.},
Title = {{Scrambled coherent superposition for enhanced optical fiber
   communication in the nonlinear transmission regime}},
Journal = {{OPTICS EXPRESS}},
Year = {{2012}},
Volume = {{20}},
Number = {{17}},
Pages = {{19088-19095}},
Month = {{AUG 13}},
Abstract = {{Coherent superposition of light waves has long been used in various
   fields of science, and recent advances in digital coherent detection and
   space-division multiplexing have enabled the coherent superposition of
   information-carrying optical signals to achieve better communication
   fidelity on amplified-spontaneous-noise limited communication links.
   However, fiber nonlinearity introduces highly correlated distortions on
   identical signals and diminishes the benefit of coherent superposition
   in nonlinear transmission regime. Here we experimentally demonstrate
   that through coordinated scrambling of signal constellations at the
   transmitter, together with appropriate unscrambling at the receiver, the
   full benefit of coherent superposition is retained in the nonlinear
   transmission regime of a space-diversity fiber link based on an
   innovatively engineered multi-core fiber. This scrambled coherent
   superposition may provide the flexibility of trading communication
   capacity for performance in future optical fiber networks, and may open
   new possibilities in high-performance and secure optical communications.
   (C) 2012 Optical Society of America}},
