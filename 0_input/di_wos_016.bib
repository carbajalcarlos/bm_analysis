@article{ ISI:000310086500008,
Author = {Amit, Sharon and Yaron, Avraham},
Title = {{Novel systems for in vivo monitoring and microenvironmental
   investigations of diabetic neuropathy in a murine model}},
Journal = {{JOURNAL OF NEURAL TRANSMISSION}},
