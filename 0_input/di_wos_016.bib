@inproceedings{ ISI:000368423200027,
Author = {Cardaci, A. and Versaci, A.},
Editor = {{Grussenmeyer, P}},
Title = {{IMAGE-BASED 3D MODELING VS LASER SCANNING FOR THE ANALYSIS OF MEDIEVAL
   ARCHITECTURE: THE CASE OF ST. CROCE CHURCH IN BERGAMO}},
