@article{ ISI:000306189800023,
Author = {Chang, Fuh-Yu and Liu, Chia-Ming and Chen, Tse-Min and Chen, Chia-Ming
   and Lin, Yu-Hsien and Huang, Shu-Jiuan},
Title = {{Three-Dimensional Force Sensing Device Using Carbon Nanofiber Polymer
   Composites: Design and Fabrication}},
Journal = {{JAPANESE JOURNAL OF APPLIED PHYSICS}},
Year = {{2012}},
Volume = {{51}},
Number = {{6, 2, SI}},
Month = {{JUN}},
Abstract = {{We propose an innovative three-dimensional force sensing device
   fabricated with carbon nanofiber (CNF) polymer composites. The device
   has four piezoresistive strain sensors made onto a polyimide substrate
   using surface patterning treatment and tilted-drop process with CNF
   polymer solutions. The proposed design and fabrication process is
   simpler than that of other three-dimensional force sensors and the
   device is suitable for mass production. The fabricated strain sensor
   properties using CNF polymer solutions with different composition ratios
   were investigated. An equation was derived using simple percolation
   theory to predict the conductivity of CNF polymer composites. The
   measured gauge factors were in the 4.84 to 17.68 range for CNF polymer
   composites with CNF 8.85-45.2 wt \%. A programmable system on chip
   (PSoC) with built-in operational (OP) amplifier, analog-to-digital (AD)
   converter and multiplexer was used to develop a scanning and analyzing
   circuit for the three-dimensional force sensing device. The proposed
   integrated system was successfully applied to control a computer screen
   cursor. (C) 2012 The Japan Society of Applied Physics}},
