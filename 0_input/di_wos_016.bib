@article{ ISI:000308442600006,
Author = {Wharam, J. Frank and Weiner, Jonathan P.},
Title = {{The Promise and Peril of Healthcare Forecasting}},
Journal = {{AMERICAN JOURNAL OF MANAGED CARE}},
Year = {{2012}},
Volume = {{18}},
Number = {{3}},
Pages = {{E82-E85}},
Month = {{MAR}},
Abstract = {{Health plans and physician groups increasingly use sophisticated tools
   to predict individual patient outcomes. Such analytics will accelerate
   as US medicine enters the digital age. Promising applications of
   forecasting include better targeting of disease management as well as
   innovative patient care approaches such as personalized health insurance
   and clinical decision support systems. In addition, stakeholders will
   use predictions to advance their organizational agendas, and unintended
   consequences could arise. Forecasting-based interventions might have
   uncertain effectiveness, focus on cost savings rather than long-term
   health, or specifically exclude disadvantaged populations. Policy
   makers, health plans, and method developers should adopt strategies that
   address these concerns in order to maximize the benefit of healthcare
   forecasting on the long-term health of patients.}},
