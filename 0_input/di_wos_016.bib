@article{ ISI:000307191400015,
Author = {Ye, Zhaohui and Hua, Chengying},
Title = {{An Innovative Method of Teaching Electronic System Design With PSoC}},
Journal = {{IEEE TRANSACTIONS ON EDUCATION}},
