@inproceedings{ ISI:000326239300108,
Author = {Rosly, J. and Kadri, Shahrul A. and Roszairi, H. and Shaharudin, A. and
   Adli, Zul W. M. and Samad, Razak Y.},
Editor = {{Chova, LG and Torres, IC and Martinez, AL}},
Title = {{THE UTILIZATION OF A MULTI-FRAME VIDEO SYSTEM IN THE TEACHING AND
   LEARNING OF SCIENCE}},
Booktitle = {{EDULEARN12: 4TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
Series = {{EDULEARN Proceedings}},
Year = {{2012}},
Pages = {{833-838}},
Note = {{4th International Conference on Education and New Learning Technologies
   (EDULEARN), Barcelona, SPAIN, JUL 02-04, 2012}},
Abstract = {{In this paper, we describe an innovative teaching strategy utilizing a
   multi-frame video system for science experiments. This system uses four
   Charge Coupled Device (CCD) cameras to record the activities, the meter
   readings and the experimental set-up. We edit the multi-frame video
   recordings and later integrate it with other digitalized materials. We
   called the video clip, Visualization Approach Teaching Aid (VATA).
   Students can use the VATA video clips in its original form or integrate
   it with the physics content using the Microsoft's Power-point. There are
   three ways to use the VATA videos namely: student-centered learning
   module, teacher-centered teaching module and virtual laboratory. When
   used in a student-centered module, students observe the playback and
   take responsibility of their learning. The teacher helps only when asked
   to. When the VATA video clips are used in a teacher-centered module, the
   teacher play-back the VATA video clips as the teaching aids during
   teaching. During which time, the teacher explain or ask questions to
   evaluate students' understanding about experiment and its science.
   Finally, when used as a virtual laboratory, we first list the
   apparatuses in the experiment. As students observe the play-back to the
   VATA video clips, they interact with the instructions just like in any
   real-time experiment. These include collecting data, tabulating data,
   analyzing data and making the appropriate conclusion about the result.
   Students also write reports and present the experimental results.}},
