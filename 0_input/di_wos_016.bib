@article{ ISI:000307182800005,
Author = {Vickery, Lindsay},
Title = {{The Evolution of Notational Innovations from the Mobile Score to the
   Screen Score}},
Journal = {{ORGANISED SOUND}},
Year = {{2012}},
Volume = {{17}},
Number = {{2}},
Pages = {{128-136}},
Month = {{AUG}},
Abstract = {{This article examines the evolution of music notational practices from
   avant-garde-era experiments in `mobility' to the advent of the digital
   `screen score'. It considers the varied goals of the composers who
   initiated these developments and the dissonance between these goals and
   the practical possibilities actually afforded by the paper score.
   The advent of graphical computing is charted along with the consequent
   expansion of possibilities afforded by screening the score from a
   platform that also provides the potential for performer coordination,
   sound synthesis and transformation. The performative, interactive and
   formal implications of these possibilities are considered.}},
