@article{ ISI:000305497300008,
Author = {White, Jeffrey W. and Andrade-Sanchez, Pedro and Gore, Michael A. and
   Bronson, Kevin F. and Coffelt, Terry A. and Conley, Matthew M. and
   Feldmann, Kenneth A. and French, Andrew N. and Heun, John T. and
