@article{ ISI:000223966800025,
Author = {Zhang, HG and Jiang, TP and Gu, ZQ and Zheng, SB},
Title = {{Design and implementation of broadcast file system based on DSM-CC data
   carousel protocol}},
Journal = {{IEEE TRANSACTIONS ON CONSUMER ELECTRONICS}},
Year = {{2004}},
Volume = {{50}},
Number = {{3}},
Pages = {{929-933}},
Month = {{AUG}},
Abstract = {{This paper presents a solution for the broadcast file system based on
   Digital Storage Media Command and Control (DSM-CC) data carousel
   protocol. Given the complexity of the object carousel protocol, we
   innovatively modify the data carousel protocol so that the hierarchical
   directory structure can be transmitted. The corresponding software has
   been implemented in set-top boxes. With the help of the Virtual
   Filesystem Switch (VFS) mechanism, the broadcast file system can be
   accessed through unified interfaces. Considering the differences between
   the broadcastfile system and the traditional file system, this paper
   also proposes the optimized strategies while receiving and caching data
   from the network in order to show better performance.}},
