@inproceedings{ ISI:000239563900019,
Author = {Estalayo, Enrique and Salgado, Luis and Jaureguizar, Fernando and
   Garcia, Narciso},
Editor = {{Sadjadi, FA}},
Title = {{Efficient image stabilization and automatic target detection in aerial
   FLIR sequences}},
Booktitle = {{AUTOMATIC TARGET RECOGNITION XVI}},
