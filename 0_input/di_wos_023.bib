@article{ ISI:000230504300011,
Author = {Chochliouros, IP and Spiliopoulou-Chochliourou, AS},
Title = {{Content regulatory aspects in broadcasting activities and modern
   electronic communications}},
Journal = {{JOURNAL OF THE COMMUNICATIONS NETWORK}},
Year = {{2005}},
Volume = {{4}},
Number = {{2}},
Pages = {{60-67}},
Month = {{APR-JUN}},
Abstract = {{Convergence affects modern electronic communications (both networks and
   services) as well as a great variety of innovative broadcasting
   applications. It offers significant potential for growth, investments
   and productivity, thus providing opportunities for development in fully
   liberalised and competitive environments. In particular, the `proper'
   creation, use, distribution and delivery of various forms of content
   constitute major challenges for European businesses, for the benefit of
   all `actors' involved (public and private sector). The promotion and the
   endorsement of common regulatory principles and/or measures are of
   particular importance to extend the relevant novel perspectives of the
   markets. This article mainly investigates various points of view,
   originating from recent European regulatory initiatives, towards
   establishing suitable conditions for technical, business and commercial
   evolution. We assess the `core' provisions of the corresponding
   initiatives, including enhanced electronic communications, audio- and
   TV-programme related activities, electronic commerce, intellectual
   property rights and copyright, digital rights management, privacy
   issues, consumers' interests and many more. Content is a crucial factor
   in the digital economy, and plays a key role both economically and
   socially.}},
