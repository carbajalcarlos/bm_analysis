@inproceedings{ ISI:000288875601007,
Author = {Breton, Dominique and Delagnes, Eric},
Book-Group-Author = {{IEEE}},
Title = {{14-Bit and 2GS/s Low Power Digitizing Boards for Physics Experiments}},
Booktitle = {{2006 IEEE NUCLEAR SCIENCE SYMPOSIUM CONFERENCE RECORD, VOL 1-6}},
