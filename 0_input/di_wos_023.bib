@inproceedings{ ISI:000242037000022,
Author = {Egerton, E. James and Sood, Ashok K. and Bell, Robert A. and Puri, Yash
   R. and Roginsky, Jacob and Wessels, Bruce W. and Cook, Timothy and
   Ulmer, melville P.},
Editor = {{Dutta, AK and Ohishi, Y and Dutta, NK and Moerk, J}},
Title = {{Design and development of high quantum efficiency large area UV Focal
   Plane Arrays for Photon Counting Applications}},
Booktitle = {{ACTIVE AND PASSIVE OPTICAL COMPONENTS FOR COMMUNICATIONS VI}},
Series = {{Proceedings of SPIE}},
Year = {{2006}},
