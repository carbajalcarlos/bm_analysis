@inproceedings{ ISI:000238252900010,
Author = {Borisov, Ilya S. and Grygoruk, Valeriy I. and Kostyukevych, Sergey A.},
Editor = {{Bjelkhagen, HI and Lessard, RA}},
Title = {{Physical aspects of digital synthesis and reconstruction of the
   multiview diffractive images}},
Booktitle = {{PRACTICAL HOLOGRAPHY XX: MATERIALS AND APPLICATIONS}},
