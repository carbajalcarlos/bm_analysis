@article{ ISI:000226342800049,
Author = {Sansoni, G and Docchio, F},
Title = {{3-D optical measurements in the field of cultural heritage: The case of
   the Vittoria Alata of Brescia}},
Journal = {{IEEE TRANSACTIONS ON INSTRUMENTATION AND MEASUREMENT}},
Year = {{2005}},
Volume = {{54}},
Number = {{1}},
Pages = {{359-368}},
Month = {{FEB}},
Abstract = {{In this paper, we give an insight on how innovative techniques of
   optical three-dimensional (3-D) measurement can be helpful in the domain
   of the study, conservation, restoration, and presentation of artworks.
   In particular, we present the results of the 3-D acquisition of the
   ``Vittoria Alata,{''} the statue symbol of our City, using an optical
   whole-field profilometer developed in our laboratory. The study,
   originally motivated by the need to explore a new hypothesis for the
   origin of the statue, led to its complete digitization, with a global
   error of 0.5 mm, as well as to the description of the statue in terms of
   both polygonal and CAD models. Moreover, it helped the archaeologists to
