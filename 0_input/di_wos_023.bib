Title = {{The role of the optical services gateway}},
Booktitle = {{Network Architectures, Management, and Applications IV}},
Series = {{PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)}},
Year = {{2006}},
