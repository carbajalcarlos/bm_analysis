@inproceedings{ ISI:000229335900005,
Author = {Mazumder, J and Qi, H},
Editor = {{Schriempf, JT}},
Title = {{Fabrication of 3-D components by laser aided direct metal deposition}},
Booktitle = {{CRITICAL REVIEW: INDUSTRIAL LASERS AND APPLICATIONS}},
