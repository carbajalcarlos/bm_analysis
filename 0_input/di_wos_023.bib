@inproceedings{ ISI:000229337900014,
Author = {Acher, O and Pages, H and Enguehard, F and Piombini, H},
Editor = {{Zediker, MS}},
Title = {{Desktop cutting of paper using a single emitter laser diode and inkjet
   printing}},
