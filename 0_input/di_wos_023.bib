@article{ ISI:000227364600003,
Author = {Blume, H and Feldkaemper, HT and Noll, TG},
Title = {{Model-based exploration of the design space for heterogeneous Systems on
   Chip}},
Journal = {{JOURNAL OF VLSI SIGNAL PROCESSING SYSTEMS FOR SIGNAL IMAGE AND VIDEO
   TECHNOLOGY}},
