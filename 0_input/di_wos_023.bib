Author = {Dhont, D and Doumit, A and Somma, J and Luxey, P},
Book-Group-Author = {{IEEE}},
Title = {{3-D geologic modeling from remote surface information: Application to
   underground water resources in Lebanon}},
Booktitle = {{IGARSS 2005: IEEE International Geoscience and Remote Sensing Symposium,
   Vols 1-8, Proceedings}},
Series = {{IEEE International Symposium on Geoscience and Remote Sensing (IGARSS)}},
Year = {{2005}},
