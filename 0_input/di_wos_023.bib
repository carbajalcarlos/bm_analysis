@article{ ISI:000233452000009,
Author = {Gengatharen, DE and Standing, C},
Title = {{A framework to assess the factors affecting success or failure of the
   implementation of government-supported regional e-marketplaces for SMEs}},
Journal = {{EUROPEAN JOURNAL OF INFORMATION SYSTEMS}},
