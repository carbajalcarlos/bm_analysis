@inproceedings{ ISI:000238645500064,
Author = {Ariaratnam, Samuel T. and Guercio, Noel},
Editor = {{Pandey, M and Xie, WC and Xu, L}},
Title = {{In-pipe ground penetrating radar for non-destructive evaluation of PVC
   lined concrete pipe}},
