Title = {{Characterisation of high-density particle distributions for optimisation
   of laser cladding processes using digital holography}},
Booktitle = {{Optical Measurement Systems for Industrial Inspection IV, Pts 1 and 2}},
Series = {{PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)}},
