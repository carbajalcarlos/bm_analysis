@article{ ISI:000295244500003,
Author = {Flament, Axel and Frappe, Antoine and Stefanelli, Bruno and Kaiser,
   Andreas and Cathelin, Andreia and Giraud, Sylvain and Chatras, Matthieu
   and Bila, Stephane and Cros, Dominique and David, Jean Baptiste and
   Leyssenne, Laurent and Kerherve, Eric},
Title = {{A Complete UMTS Transmitter Using BAW Filters and Duplexer: A 90-nm CMOS
   Digital RF Signal Generator and a 0.25-mu m BiCMOS Power Amplifier}},
Journal = {{INTERNATIONAL JOURNAL OF RF AND MICROWAVE COMPUTER-AIDED ENGINEERING}},
Year = {{2011}},
Volume = {{21}},
Number = {{5}},
Pages = {{466-476}},
Month = {{SEP}},
Abstract = {{A complete UMTS transmitter is proposed. It is composed of a
   radiofrequency (RF) signal generator, a power stage, and a bulk acoustic
   wave (BAW) duplexer. The 90-nm CMOS digital RF signal generator is based
   on a third-order delta-sigma modulator using innovative design
   techniques to increase work frequency and a BAW filter to get rid of
   out-of-band quantization noise. The filter exhibits very high rejection,
   3 dB of insertion losses at 1.95 GHz for a 3\% fractional bandwidth. The
   0.25-mu m BiCMOS power stage feeds a BAW duplexer that allows to share a
   common between W-CDMA emission and reception. This 4 x 4 mm(2) duplexer,
   built with flip-chipped BAW on glass substrate, ensures good isolation.
   The full transmitter measurements show the compliance with respect to
   spurious emissions in the different frequency bands and an EVM
   measurement at the state of the art (< 5 \%). (C) 2011 Wiley
   Periodicals, Inc. Int J RF and Microwave CAE 21:466-476,2011.}},
