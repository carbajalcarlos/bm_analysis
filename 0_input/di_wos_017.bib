@inproceedings{ ISI:000326239302052,
Author = {Adewumi, Olusoji Olurotimi and Ozoh, Julius Momoh and Ozoh, Olufunmilola
   Omodele and Ayanwale, Ibukunolu and Lawal, Olufunke},
Editor = {{Chova, LG and Torres, IC and Martinez, AL}},
Title = {{E-LEARNING DESIGN AND DEVELOPMENT PROJECT IN THE NIGERIAN EDUCATIONAL
   SYSTEM}},
Booktitle = {{EDULEARN12: 4TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
