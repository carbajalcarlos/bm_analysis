@article{ ISI:000289396000030,
Author = {Bourgonjon, Jeroen and Valcke, Martin and Soetaert, Ronald and de Wever,
   Bram and Schellens, Tammy},
Title = {{Parental acceptance of digital game-based learning}},
Journal = {{COMPUTERS \& EDUCATION}},
Year = {{2011}},
Volume = {{57}},
Number = {{1}},
Pages = {{1434-1444}},
Month = {{AUG}},
Abstract = {{In research about digital game-based learning, the likely negative
   perceptions of parents are often enlisted as a barrier toward the
   adoption of games in classroom settings. Teachers, students and policy
   makers appear to be influenced by what parents think about games in the
   classroom. Therefore, it is important to study these parental beliefs
   about games. The present research develops and validates a path model to
   explain and predict parental acceptation of video games in the
   classrooms of their children. The hypothetical model was found reliable
   and valid, based on a survey of 858 parents with at least one child in
   secondary education. Overall, the results show that 59\% of the variance
   in parents' preference for video games can be explained by the model
   comprising hypotheses about learning opportunities, subjective norm,
   perceived negative effects of gaming, experience with video games,
   personal innovativeness, and gender. (c) 2011 Elsevier Ltd. All rights
   reserved.}},
