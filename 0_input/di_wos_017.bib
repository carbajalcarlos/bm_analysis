@inproceedings{ ISI:000380252704004,
Author = {Virani, Shamsnaz and Burnham, Iris B.},
Book-Group-Author = {{ASEE}},
Title = {{INNOVATIVE CURRICULUM FOR ENGINEERING IN HIGH SCHOOL (ICE-HS): STATUS
   UPDATE}},
