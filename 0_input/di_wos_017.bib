@article{ ISI:000305209100012,
Author = {Xiang, Dong and Arnold, Mark A.},
Title = {{CHEMICAL IMAGING WITH A SOLID-STATE NEAR INFRARED SPECTROMETER BASED ON
   A DIGITAL MICRO-MIRROR ARRAY DEVICE COUPLED WITH HADAMARD TRANSFORM
   SPECTROSCOPY}},
Journal = {{ANALYTICAL LETTERS}},
Year = {{2012}},
Volume = {{45}},
Number = {{9}},
Pages = {{1070-1078}},
Abstract = {{Chemical imaging is demonstrated by using an innovative near infrared
   spectrometer that uses a digital micro-mirror array device (DMD) to
   provide both spatial and spectral resolution. Hadamard transform
   spectroscopy is used to enhance the signal-to-noise ratio of the
   measurement compared to single point raster scanning. The concept is
   demonstrated by collecting a spectral image of a polymer sample composed
   of separate regions of polystyrene and polypropylene. The resulting
   image is composed of 63 spectral resolution elements across the first
   overtone region of the near infrared spectrum (1538-1818 nm), as well as
   65 spatial resolution elements across the longitudinal direction of the
   DMD (11.3 mm).}},
