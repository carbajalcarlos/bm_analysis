@inproceedings{ ISI:000326239304012,
Author = {Garcia Martul, David and Franco Alvarez, Guillermina},
Editor = {{Chova, LG and Torres, IC and Martinez, AL}},
Title = {{DESIGN AND IMPLEMENTATION OF AN EDUCATIONAL PLATFORM OPEN SOURCE
   COMMUNICATION FOR INFORMATION LITERACY IN WOMEN SUB-SAHARAN IMMIGRANTS}},
Booktitle = {{EDULEARN12: 4TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
