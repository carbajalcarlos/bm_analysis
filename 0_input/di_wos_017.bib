@inproceedings{ ISI:000310691600048,
Author = {Naletto, Giampiero and Cesaro, Michele and Albasini, Alessandro and
   Cremonese, Gabriele and Da Deppo, Vania and Forlani, Gianfranco and Re,
   Cristina and Roncella, Riccardo and Salemi, Giuseppe and Simioni,
   Emanuele},
Editor = {{Clampin, MC and Fazio, GG and MacEwen, HA and Oschmann, JM}},
Title = {{Innovative optical setup for testing a stereo camera for space
   applications}},
Booktitle = {{SPACE TELESCOPES AND INSTRUMENTATION 2012: OPTICAL, INFRARED, AND
   MILLIMETER WAVE}},
Series = {{Proceedings of SPIE}},
