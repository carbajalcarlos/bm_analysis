@inproceedings{ ISI:000335746300086,
Author = {Nikolaou, A. and Tsolakidis, C.},
Book-Group-Author = {{IEEE}},
Title = {{The Role of Three Dimensional Virtual Environments in the Development of
   Personal Learning Networks}},
