@article{ ISI:000295845900026,
Author = {Forno, Maria Gabriella and Lingua, Andrea and Lo Russo, Stefano and
   Taddia, Glenda},
Title = {{Improving digital tools for Quaternary field survey: a case study of the
   Rodoretto Valley (NW Italy)}},
Journal = {{ENVIRONMENTAL EARTH SCIENCES}},
Year = {{2011}},
Volume = {{64}},
Number = {{5}},
Pages = {{1487-1495}},
Month = {{NOV}},
Abstract = {{In the modern geological mapping activities geomatics technologies can
   facilitate data collection during field survey and avoid practical
   problems related to the transposition of that data onto topographic
   maps. This paper describes an innovative tool (the Solid True
   OrthoPhoto; STOP) for the structured acquisition and recording of
   geological field survey data that is immediately available in a GIS
   framework. An experimental software graphical interface has been
   developed to manage the STOP application during the survey. The benefits
   of the STOP system were tested by producing a digital map of the
   Quaternary formations and landforms in a 3-km(2) sector of the Rodoretto
   Valley, a minor tributary of the Germanasca Valley, northwestern Italy.}},
