Author = {Su, S. Z. and Li, M. Z. and Liu, C. G. and Ji, C. Q. and Setchi, R. and
   Larkiola, J. and Panteleev, I. and Stead, I. and Lopez, R.},
Editor = {{Merklein, M and Hagenah, H}},
Title = {{Flexible Tooling System Using Reconfigurable Multi-Point Thermoforming
   Technology for Manufacturing Freeform Panels}},
Booktitle = {{MATERIAL FORMING - ESAFORM 2012, PTS 1 \& 2}},
Series = {{Key Engineering Materials}},
Year = {{2012}},
Volume = {{504-506}},
Pages = {{839+}},
