@article{ ISI:000292649900010,
Author = {Prefasi, E. and Reekmans, S. and Hernandez, L. and Rombouts, P.},
Title = {{A one-path subsampling quadrature receiver based on a I I{''} pound
   modulator with distributed resonators}},
