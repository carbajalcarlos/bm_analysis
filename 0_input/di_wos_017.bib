@article{ ISI:000313926400043,
Author = {Andreopoulou, Z. and Tsekouropoulos, G. and Pavlidis, Th},
Title = {{ADOPTION AND PERSPECTIVES OF NETWORK TECHNOLOGIES AND E-MARKETING D
   SKIING CENTRES WEBSITES IN THE INTERNET}},
Journal = {{JOURNAL OF ENVIRONMENTAL PROTECTION AND ECOLOGY}},
Year = {{2012}},
Volume = {{13}},
Number = {{4}},
Pages = {{2416-2422}},
Abstract = {{Internet provides new perspectives within the social, financial and
   cultural mountainous development. In Greece, winter tourism is a fast
   growing sector and there are 23 organised skiing centres in mountainous
   forest areas. Most of them have a website in the Internet in various
   levels of innovative technologies adoption. This paper assesses skiing
   centres websites in Greece regarding the adoption of innovative network
   and Internet technologies and e-marketing services and classifies them
   in groups. Initially, websites are analysed qualitatively and
   quantitatively according to criteria described, such as innovative
   network technology incorporation, e-marketing functions, interactivity
   and various e-services provision. Then, the websites are classified in 3
   groups according to the criteria they accomplish. The `optimum group 1',
   represents 30\% and provides enhanced e-marketing services through
   advanced online multimedia digital services that allow conducting online
   transactions, such as online booking system for accommodations,
   e-ticketing, newsletters, social media, etc. The `lagging group 3'
   represents 48\%, being information platforms without online
   functionalities only seeking in a web presence. It is imperative that
   websites in group 2 and group 3 should further evolve in network
   services and e-marketing features and take advantage of innovative
   technologies, as to become more compatible.}},
