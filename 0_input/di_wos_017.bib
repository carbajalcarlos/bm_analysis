@article{ ISI:000294893800023,
Author = {Gutin, Alexey and Jacob, Philip and Chu, Michael and Belemjian, Paul M.
   and LeRoy, Mitchell R. and Kraft, Russell P. and McDonald, John F.},
Title = {{Carry Chains for Ultra High-Speed SiGe HBT Adders}},
