@incollection{ ISI:000428175800094,
Author = {Glasco, Jon E.},
Book-Group-Author = {{Informat Resources Management Assoc}},
Title = {{Web 2.0 and Government Transformation: How E-Government and Social Media
   Contribute to Innovation in Public Services}},
Booktitle = {{DIGITAL DEMOCRACY: CONCEPTS, METHODOLOGIES, TOOLS, AND APPLICATIONS, VOL
   1}},
Year = {{2012}},
Pages = {{1861-1882}},
Abstract = {{The existing body of knowledge on public sector transformation explores
   the concept that e-government contributes to improving public sector
   services and increasing the participation of citizens in government
   planning and change initiatives. This chapter provides information on
   the potential for a digital ecosystem of government, citizens and
   businesses to use Web 2.0 and social media innovation to shape
   government transformation and contribute to government's quality-of-life
   mission. The chapter includes examples of how web-centric technologies
   affect e-government innovation and proposes a new planning framework for
