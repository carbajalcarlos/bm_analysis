Title = {{The Neutrino Telescope of the KM3NeT Deep-Sea Research Infrastructure}},
Booktitle = {{PROCEEDINGS OF THE 2ND INTERNATIONAL CONFERENCE ON TECHNOLOGY AND
   INSTRUMENTATION IN PARTICLE PHYSICS (TIPP 2011)}},
Series = {{Physics Procedia}},
Year = {{2012}},
