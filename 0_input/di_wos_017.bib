@article{ ISI:000295106500039,
Author = {Stanitzki, M. and SPiDeR Collaboration},
Title = {{Advanced monolithic active pixel sensors for tracking, vertexing and
   calorimetry with full CMOS capability}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
