@inproceedings{ ISI:000326239303078,
Author = {Gomes, Natalia and Jose Hernandez, Maria},
Editor = {{Chova, LG and Torres, IC and Martinez, AL}},
Title = {{DIGITAL COMPETENCES: ARE MY STUDENTS READY FOR LEARNING 2.0?}},
