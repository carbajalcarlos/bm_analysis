Author = {Shraim, Khitam},
Book-Group-Author = {{IEEE}},
Title = {{Facilitating the Implementation of the Constructivist Approach through
   the Social Space of Facebook}},
Booktitle = {{2013 FOURTH INTERNATIONAL CONFERENCE ON E-LEARNING ``BEST PRACTICES IN
   MANAGEMENT, DESIGN AND DEVELOPMENT OF E-COURSES: STANDARDS OF EXCELLENCE
   AND CREATIVITY}},
