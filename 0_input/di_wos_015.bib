Author = {Ionita, Mirela and Asan, Doina},
Editor = {{Roceanu, I and Beligan, D and Ciolan, L and Radu, C}},
Title = {{BLENDED LEARNING IN FOREIGN LANGUAGE LEARNING: SUCCESSFUL APPROACH OR
   JUST A TRENDY ONE?}},
