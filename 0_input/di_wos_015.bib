@article{ ISI:000312456400002,
Author = {Freund, Louis E. and Spohrer, James C.},
Title = {{The human side of service engineering}},
Journal = {{HUMAN FACTORS AND ERGONOMICS IN MANUFACTURING \& SERVICE INDUSTRIES}},
Year = {{2013}},
Volume = {{23}},
Number = {{1, SI}},
Pages = {{2-10}},
Month = {{JAN-FEB}},
Abstract = {{Service engineering is concerned with applying engineering methods to
   make and scale diverse types of service systems, which range from
   transportation and finance to health care and government. What makes
   modern service engineering uniquely challenging is the need not only to
   consider the technical side of these system designs but also to focus on
   how these systems engage, relate to, and are perceived to contribute to
   the self-actualization goals of their all-too-human cocreators of value.
   Service systems must have a wide-ranging human side. In this article, we
   propose a framework to scope and better define the human side of modern
   service systems. We then explore how to impact the cocreation of value
   in modern service systems, characterized by their digital,
