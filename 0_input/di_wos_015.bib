@inproceedings{ ISI:000323440000001,
Author = {Asmus, J. and Elford, J. and Parfenov, V.},
Editor = {{Pezzati, L and Targowski, P}},
Title = {{Spin-offs from laser ablation in art conservation}},
Booktitle = {{OPTICS FOR ARTS, ARCHITECTURE, AND ARCHAEOLOGY IV}},
Series = {{Proceedings of SPIE}},
