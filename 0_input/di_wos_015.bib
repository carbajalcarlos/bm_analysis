@inproceedings{ ISI:000346699800031,
Author = {Shulby, Christopher Dane},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{TEACHER PREPARATION FOR ONLINE EDUCATION}},
