@inproceedings{ ISI:000340643600055,
Author = {Fricker, Pia and Girot, Christophe and Munkel, Georg},
Editor = {{Stouffs, R and Sariyildiz, S}},
Title = {{How to Teach `New Tools' in Landscape Architecture in the Digital
