@inproceedings{ ISI:000330839100341,
Author = {Rikakis, Thanassis and Tinapple, David and Olson, Loren},
Book-Group-Author = {{IEEE}},
Title = {{The Digital Culture Degree: a competency-based interdisciplinary program
   spanning engineering and the arts}},
Booktitle = {{2013 IEEE FRONTIERS IN EDUCATION CONFERENCE}},
