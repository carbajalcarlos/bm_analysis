@inproceedings{ ISI:000333716800051,
Author = {Pendit, Ulka Chandini and Zaibon, Syamsul Bahrin},
Editor = {{Jamaludin, Z and ChePa, N and Bakar, MSA}},
Title = {{NON-PERSONAL DIGITAL INTERPRETIVE MEDIA AT CULTURAL HERITAGE SITES}},
