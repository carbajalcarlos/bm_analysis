@inproceedings{ ISI:000339402900118,
Author = {Tmazirte, Nourdine Ait and El Najjar, Maan E. and Smaili, Cherif and
   Pomorski, Denis},
Book-Group-Author = {{IEEE}},
Title = {{Dynamical Reconfiguration Strategy of a Multi Sensor Data Fusion
   Algorithm based on Information Theory}},
