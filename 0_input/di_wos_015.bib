@inproceedings{ ISI:000333812900018,
Author = {Chan, Steve and Sala, Simone},
Book-Group-Author = {{IEEE}},
Title = {{Sensemaking and Robust Decision Engineering: Synchrophasors and their
   Application for a Secure Smart Grid}},
Booktitle = {{2013 7TH IEEE INTERNATIONAL CONFERENCE ON DIGITAL ECOSYSTEMS AND
   TECHNOLOGIES (DEST)}},
Series = {{IEEE International Conference on Digital Ecosystems and Technologies}},
