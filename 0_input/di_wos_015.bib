@inproceedings{ ISI:000346699804050,
Author = {Fondevila-Gascon, Joan-Francesc and Segura-Jimenez, Herlaynne and del
   Olmo-Arriaga, Josep-Lluis},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{DIGITIZATION AND UNIVERSITY: A COLOMBIAN EXPERIENCE}},
