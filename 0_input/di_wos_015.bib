@inproceedings{ ISI:000321909400027,
Author = {Roodt, Sumarie},
Editor = {{Blooma, J and Nkhoma, M and Leung, N}},
Title = {{Using YouTube to Support Student Engagement for the Net Generation in
   Higher Education}},
Booktitle = {{PROCEEDINGS OF THE 4TH INTERNATIONAL CONFERENCE ON INFORMATION SYSTEMS
