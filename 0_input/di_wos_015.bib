@inproceedings{ ISI:000346798306023,
Author = {Petrucco, Corrado},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{DIGITAL STORYTELLING FOR EFFECTIVE TEAM BUILDING AND COMPETENCES
   DEVELOPMENT}},
