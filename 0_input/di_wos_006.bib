@article{ ISI:000393406900003,
Author = {Perez-Llantada, Carmen},
Title = {{HOW IS THE DIGITAL MEDIUM SHAPING RESEARCH GENRES? SOME
   CROSS-DISCIPLINARY TRENDS}},
