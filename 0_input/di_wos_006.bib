@article{ ISI:000387047400011,
Author = {Allen, Timothy Craig},
Title = {{The Archives of Pathology \& Laboratory Medicine The Most Widely Read
   Pathology Journal Today}},
Journal = {{ARCHIVES OF PATHOLOGY \& LABORATORY MEDICINE}},
