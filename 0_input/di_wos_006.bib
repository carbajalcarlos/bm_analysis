Title = {{Mobility and Service Innovation: A Critical Examination of Opportunities
   and Challenges for the Canadian Public Sector}},
Journal = {{INTERNATIONAL JOURNAL OF PUBLIC ADMINISTRATION IN THE DIGITAL AGE}},
Year = {{2016}},
Volume = {{3}},
