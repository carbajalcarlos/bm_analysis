@article{ ISI:000390688900009,
Author = {Papaioannou, G. and Loukas, A. and Vasiliades, L. and Aronica, G. T.},
Title = {{Flood inundation mapping sensitivity to riverine spatial resolution and
   modelling approach}},
Journal = {{NATURAL HAZARDS}},
