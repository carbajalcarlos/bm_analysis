@article{ ISI:000390996900018,
Author = {de Azevedo, Adriana Barroso},
Title = {{Digital media in pedagogical practices in education: an
   hermeneutic-phenomenological analysis}},
Journal = {{EDUCACAO}},
Year = {{2016}},
Volume = {{41}},
Number = {{2}},
Pages = {{495-507}},
Month = {{MAY-AUG}},
Abstract = {{This article brings the results of a research done with 11 teachers,
   indicated for being innovative, creative and for the use of different
   pedagogical practices with the usage of technology. These teachers work
   on public and private schools of the cities of Sao Bernardo do Campo and
   Sao Paulo. The article was built under the concept of research-training
   (JOSSO, 2010) and seeks to promote reflection towards the
   knowledge-building about education and technology. The narratives of
   teaching experiences with the use of technology, built on virtual
   learning environments, were analyzed from a hermeneutic-phenomenological
   approach (FREIRE 2012, RICOUER 1986). The results show that the
   practices are not anchored in the use of technology itself, but the
   desire of these educators to promote a education that is more humane and
   next to the student's context.}},
