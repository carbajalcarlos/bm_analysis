@article{ ISI:000385534000038,
Author = {Sullivan, Sheila Cox and Bopp, Melinda M. and Roberson, Paula K. and
   Lensing, Shelly and Sullivan, Dennis H.},
Title = {{Evaluation of an Innovative Method for Calculating Energy Intake of
   Hospitalized Patients}},
