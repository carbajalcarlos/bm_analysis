Author = {Valls, L. and Badve, C. and Avril, S. and Herrmann, K. and Faulhaber, P.
   and O'Donnell, J. and Avril, N.},
Title = {{FDG-PET imaging in hematological malignancies}},
Journal = {{BLOOD REVIEWS}},
