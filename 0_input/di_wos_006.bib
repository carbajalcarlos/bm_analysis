@article{ ISI:000379915500003,
Author = {Gielen, Georges and Van Rethy, Jelle and Marin, Jorge and Shulaker, Max
   M. and Hills, Gage and Wong, H. -S. Philip and Mitra, Subhasish},
Title = {{Time-Based Sensor Interface Circuits in CMOS and Carbon Nanotube
   Technologies}},
