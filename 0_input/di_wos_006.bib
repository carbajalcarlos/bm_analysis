@article{ ISI:000380179300005,
Author = {Santella, Anna-Lise P.},
Title = {{THE IDEAL DICTIONARY: IMPOSSIBLE TASKS, FRANK ADJUSTMENTS, AND
   LEXICOGRAPHICAL INNOVATIONS IN THE CREATION OF GROVE MUSIC ONLINE}},
Journal = {{FONTES ARTIS MUSICAE}},
Year = {{2016}},
Volume = {{63}},
Number = {{3}},
Pages = {{213-221}},
Month = {{JUL-SEP}},
Abstract = {{Lexicography is by definition an impossible task, an activity built on
   ideals that are beyond reach. But in wrestling with competing
   requirements, lexicographers have created innovations to dictionaries
   that have changed the way scholars work. This article examines two
   innovative moments in the history of Grove Music-the publication of the
   first American supplement in 1920 and the debut of Grove Music Online in
   2001. It assesses their impact, considers the role of innovation in
   print and digital environments, and projects paths for future growth to
   both dictionaries and the field of music scholarship.}},
