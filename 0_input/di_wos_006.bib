@article{ ISI:000389751600001,
Author = {Thom, Brian and Colombi, Benedict J. and Degai, Tatiana},
Title = {{Bringing Indigenous Kamchatka to Google Earth Collaborative Digital
   Mapping with the Itelmen Peoples}},
