@article{ ISI:000376706200004,
Author = {Komorowski, Marlen and Coppens, Paulien and Van den Broeck, Wendy and
   Braet, Olivier},
Title = {{Lowering the barriers for online cross-media usage: Scenarios for a
   Belgian single sign-on solution}},
