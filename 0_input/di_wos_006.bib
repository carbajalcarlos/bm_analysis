@article{ ISI:000388997400010,
Author = {Spano, Antonia and Chiabrando, Filiberto and Dezzani, Livio and
   Prencipe, Antonio},
Title = {{DIGITAL SEGUSIO: FROM MODELS GENERATION TO URBAN RECONSTRUCTION}},
Journal = {{VIRTUAL ARCHAEOLOGY REVIEW}},
