@article{ ISI:000404136700009,
Author = {Ibba, Simona and Pani, Filippo Eros},
Title = {{Digital Libraries: The Challenge of Integrating Instagram with a
   Taxonomy for Content Management}},
Journal = {{FUTURE INTERNET}},
Year = {{2016}},
Volume = {{8}},
Number = {{2}},
Month = {{JUN}},
Abstract = {{Interoperability and social implication are two current challenges in
   the digital library (DL) context. To resolve the problem of
   interoperability, our work aims to find a relationship between the main
   metadata schemas. In particular, we want to formalize knowledge through
   the creation of a metadata taxonomy built with the analysis and the
   integration of existing schemas associated with DLs. We developed a
   method to integrate and combine Instagram metadata and hashtags. The
   final result is a taxonomy, which provides innovative metadata with
   respect to the classification of resources, as images of Instagram and
   the user-generated content, that play a primary role in the context of
   modern DLs. The possibility of Instagram to localize the photos inserted
   by users allows us to interpret the most relevant and interesting
   informative content for a specific user type and in a specific location
   and to improve access, visibility and searching of library content.}},
