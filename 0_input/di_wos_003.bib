@article{ ISI:000401237800005,
Author = {Coyle, Diane},
Title = {{PRECARIOUS AND PRODUCTIVE WORK IN THE DIGITAL ECONOMY}},
Journal = {{NATIONAL INSTITUTE ECONOMIC REVIEW}},
