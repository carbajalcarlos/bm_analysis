@article{ ISI:000403304000033,
Author = {Esenam, Aniekan},
Title = {{Overview of digital agriculture: Making growers lives more productive}},
Journal = {{INTERNATIONAL SUGAR JOURNAL}},
Year = {{2017}},
Volume = {{119}},
Number = {{1422}},
Pages = {{466-470}},
Month = {{JUN}},
Abstract = {{The problem of sustainably growing food to feed an extra 2 billion
   mouths by 2050, is receiving considerable attention, especially the
   transformation in agricultural production methods required to meet this
   unprecedented challenge. In an increasingly digitalized world, Digital
   Agriculture holds out the premise of allowing farmers to increase yields
   by extracting as much value as possible from every stage of the crop
   production cycle. This article sheds light on the digitally connected
   farming environment, and enabling technologies that make up digital
   agriculture. To better understand this technology, we follow a farmer's
   decision-making process, over a crop production cycle, integrating
   agronomic knowledge, agricultural `big data' and powerful innovative
   technologies from sensors, satellites, drones, agriculture Internet of
   Things (IoT), NDVI maps and autonomous robots. In particular, we show
   that digital agriculture is not a stand-alone solution, but a connected
   system encompassing farm based sensors, equipment and software, and
   external advisors, data services and cloud platforms. Of particular
   interest, is seeing that even in this connected data chain, human
   intervention is still necessary for key decisions. Finally, we see how
   our farmer can monitor all aspects during the production cycle, and can
   apportion the yield gains/losses to the correct drivers and be in a
   position to improve crop production performance for the following
   season.}},
