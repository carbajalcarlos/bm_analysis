@inproceedings{ ISI:000428311600003,
Author = {Khiari, Bochra and Jilani, Lamia Labed},
Book-Group-Author = {{IEEE}},
Title = {{TOWARDS a DSPL FOR CONTEXT AWARE BPM}},
Booktitle = {{2017 IEEE/ACS 14TH INTERNATIONAL CONFERENCE ON COMPUTER SYSTEMS AND
   APPLICATIONS (AICCSA)}},
