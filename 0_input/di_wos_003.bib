@inproceedings{ ISI:000429975305079,
Author = {Vassileva, Sofia and Kovatcheva, Eugenia},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{THE INNOVATIVE MODEL FOR INTERACTIVITY IN BULGARIAN MUSEUMS}},
Booktitle = {{10TH INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND INNOVATION
   (ICERI2017)}},
Series = {{ICERI Proceedings}},
Year = {{2017}},
