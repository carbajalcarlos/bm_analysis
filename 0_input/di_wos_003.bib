@inproceedings{ ISI:000432209800028,
Author = {Almishari, Sitah and Salamah, Nora and Alwan, Maram and Alkhalifa, Nada
   and Al-Wabil, Areej},
Editor = {{Meiselwitz, G}},
Title = {{The Rise of Hackathon-Led Innovation in the MENA Region: Visualizing
   Spatial and Temporal Dynamics of Time-Bounded Events}},
Booktitle = {{SOCIAL COMPUTING AND SOCIAL MEDIA: APPLICATIONS AND ANALYTICS, SCSM
   2017, PT II}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2017}},
Volume = {{10283}},
