@article{ ISI:000404520600006,
Author = {Pour, Reza Saeidi and Randelzhofer, Peter and Edelhoff, Daniel and
   Prandtner, Otto and Rafael, Caroline Freitas and Liebermann, Anja},
Title = {{Innovative Single-Tooth Replacement with an Individual Root-Analog
   Hybrid Implant in the Esthetic Zone: Case Report}},
Journal = {{INTERNATIONAL JOURNAL OF ORAL \& MAXILLOFACIAL IMPLANTS}},
Year = {{2017}},
Volume = {{32}},
Number = {{3}},
Pages = {{E153-E160}},
Month = {{MAY-JUN}},
Abstract = {{The goal of this study was to explore an innovative approach to
   single-tooth replacement using an individually custom-fabricated,
   root-analog, hybrid dental implant, in the esthetic zone, to avoid the
   microgap and micromovements between the implant and abutment. Moreover,
   the use of burs to prepare the implant recipient site is not necessary
   in this technique, reducing the bone removal, heating, and trauma. The
   process requires capturing accurate root geometry through combined
   computer-aided design/computerassisted manufacturing (CAD/CAM) and a
   three-dimensional (3D) visualization (digital volume tomography {[}DVT])
   of the tooth in situ, which might result in reduced remodeling after
   insertion. A good esthetic and functional outcome was obtained. The use
   of a root-shaped tooth analog implant might be in selected cases a
   viable alternative to current threaded cylindrical and cone-shaped
   implants. The new concept avoids the microgap between the implant and
   the abutment and reduces the trauma to the tissue and bone.}},
