@inproceedings{ ISI:000429975300064,
Author = {Svensson, Lars and Utrianien, Callisto and Winman, Thomas},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{E-QUALITY IN INFRASTRUCTURES FOR LEARNING IN HIGHER EDUCATION}},
