@inproceedings{ ISI:000426970500011,
Author = {Sydow, Sophie Landwehr and Tholander, Jakob and Jonsson, Martin},
Book-Group-Author = {{ACM}},
Title = {{``It's a Bomb!{''} - Material Literacy and Narratives of Making}},
