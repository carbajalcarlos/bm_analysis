@inproceedings{ ISI:000428140500062,
Author = {Sachdeva, Nitin and Obheroi, Rajat Kumar and Srivastava, Abhishek and
   Nehal, S. K.},
Editor = {{Khatri, SK and Kapur, RK and Rana, A and Singh, S and Kapur, PK}},
Title = {{Diffusion of Industry 4.0 in Manufacturing Sector - An Innovative
   Framework}},
Booktitle = {{2017 INTERNATIONAL CONFERENCE ON INFOCOM TECHNOLOGIES AND UNMANNED
   SYSTEMS (TRENDS AND FUTURE DIRECTIONS) (ICTUS)}},
Year = {{2017}},
Pages = {{317-321}},
Note = {{International Conference on Infocom Technologies and Unmanned Systems
   (Trends and Future Directions) (ICTUS), Amity Univ, Dubai, U ARAB
   EMIRATES, DEC 18-20, 2017}},
Organization = {{Amity Univ, Amity Directorate Engn \& Technol; IEEE UAE Sect}},
Abstract = {{With new technologies emerging out in the manufacturing sector everyday
   moving away from the traditionally adopted mechanical systems to one of
   the most sophisticated and automated assembly lines of today. To meet
   and adopt to the dynamic customer requirements, fourth generation of
   industrial manufacturing governed by smart systems and web based
   solutions called industry 4.0 is considered to be the technology for the
   future. The two prime distinguishing features of Industry 4.0 involves
   digitization by use of cyber-physical systems and manufacturing units
   enabled with human like intelligence based on the concept of ``internet
