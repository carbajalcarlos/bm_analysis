@article{ ISI:000399817800029,
Author = {Singh, Mansher and Li, Hehuan and Nuutila, Kristo and Collins, K. C. and
   Wall, Jennifer and Riviello, Robert and Weaver, Michael J. and Carty,
   Matthew J. and Pribaz, Julian and Talbot, Simon G.},
Title = {{Innovative Techniques for Maximizing Limb Salvage and Function}},
Journal = {{JOURNAL OF BURN CARE \& RESEARCH}},
Year = {{2017}},
Volume = {{38}},
Number = {{3}},
Pages = {{E670-E677}},
Month = {{MAY}},
Abstract = {{Concurrent injuries to multiple extremities present unique challenges to
   the reconstructive surgeon. The primary goal in such scenarios is to
   optimize functional outcomes. The goal of this article is to present an
   overview of various techniques necessary to provide sufficient soft
   tissue and preserve amputation limb lengths and function. The concept of
   innovative techniques for maximizing limb savage and function is
   presented using an index patient with multiple extremity third-and
   fourth-degree burn injuries resulting in nonsalvageable lower
   extremities and severe left-hand wounds. A review of other potential
   innovative techniques is discussed. The burn injury resulted in a need
   for bilateral guillotine belowknee amputations. Above-knee amputation
   was avoided in the left leg using a parascapular free fasciocutaneous
   flap, while through-knee amputation was preferred to above-knee
   amputation in the right leg. The preservation of areas with questionable
   viability resulted in salvaging the left hand of the patient using
   digital palmar flaps to resurface the dorsum with creation of a first
   web-space. Maintenance of maximal viable length of limbs and any
   residual function in the limbs can be of significant functional benefit
   to multiple limb amputation patients. Maximizing the limb length in such
   patients is critical, and typical ``rules{''} that have traditionally
   been utilized to minimize numbers of operations and optimize prosthetic
   fit may not apply.}},
