@article{ ISI:000397919500003,
Author = {Maneiro-Catoira, Roberto and Bregains, Julio C. and Garcia-Naya, Jose A.
   and Castedo, Luis and Rocca, Paolo and Poli, Lorenzo},
Title = {{Performance Analysis of Time-Modulated Arrays for the Angle Diversity
   Reception of Digital Linear Modulated Signals}},
Journal = {{IEEE JOURNAL OF SELECTED TOPICS IN SIGNAL PROCESSING}},
