@article{ ISI:000401343900010,
Author = {Turvani, G. and Riente, F. and Plozner, E. and Vacca, M. and Graziano,
   M. and Gamm, S. Breitkreutz-v},
Title = {{A pNML Compact Model Enabling the Exploration of Three-Dimensional
   Architectures}},
Journal = {{IEEE TRANSACTIONS ON NANOTECHNOLOGY}},
