@inproceedings{ ISI:000429975303034,
Author = {Wong, Joseph and Hughes, Brad},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{STUDENT OPINIONS ON DIGITAL ART MEDIA: ``FLY-OVER VIDEOS{''} IN HIGHER
   EDUCATION}},
