@article{ ISI:000433426400006,
Author = {Magal-Royo, Teresa and Garcia Laborda, Jesus and Price, Sara},
Title = {{A New m-Learning Scenario for a Listening Comprehension Assessment Test
   in Second Language Acquisition {[}SLA]}},
Journal = {{JOURNAL OF UNIVERSAL COMPUTER SCIENCE}},
