@inproceedings{ ISI:000432077000020,
Author = {Malgonde, Onkar and Hevner, Alan},
Editor = {{Maedche, A and VomBrocke, J and Hevner, A}},
Title = {{Finding Evidence for Effectual Application Development on Digital
   Platforms}},
