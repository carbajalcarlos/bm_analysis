@inproceedings{ ISI:000428513600044,
Author = {Sornalatha, R. and Janakiraman, N. and Balasharmitha, T.},
Book-Group-Author = {{IEEE}},
Title = {{Modeling of Widely Linear Digitally Controlled Self-Interference
   Canceller for Full-Duplex Transceiver}},
Booktitle = {{2017 2ND IEEE INTERNATIONAL CONFERENCE ON WIRELESS COMMUNICATIONS,
   SIGNAL PROCESSING AND NETWORKING (WISPNET)}},
Year = {{2017}},
Pages = {{229-235}},
Note = {{2nd IEEE International Conference on Wireless Communications, Signal
   Processing and Networking (WiSPNET), Chennai, INDIA, MAR 22-24, 2017}},
Organization = {{IEEE; IEEE Commun Soc; SSN; iNSERB INDIA; Aconf; ISRO Bangalore; Pearson
   India Educ Serv Private Ltd; IEEE MAS Sect; IEEE Commun Madras Chapter;
   DST SERB New Delhi}},
Abstract = {{In this paper, we describe a mathematical model of widely linear
   digitally controlled Self-Interference canceller for In-band
   full-duplex-capable transceiver. This transceiver schemes are combined
   with linear distortion over and above amplitude and phase distortion at
   transmitter and receiver IQ mixer. Here the In-band full duplex
   technology is used to maximize the throughput level and also twice the
   spectral efficiency. Normally, the inevitable self-Interference limits
   the increase in throughput owing to full duplex operation. The holistic
   SI cancellation techniques can be achieved by the novel modeling of
   these parameter evaluation methods. From the innovation of classical
   methods we can easily adapt these techniques with the new standards. The
   successful reduction of electromagnetic coupling for antenna isolation
   and the RF imperfection can be adjusted by modeling the most dominant
   impairments at the waveform level. The extensive mathematical
   calculations helps to get the clear outperforms of widely linear digital
