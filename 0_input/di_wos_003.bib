@article{ ISI:000394794500002,
Author = {Gibson, Cristina B.},
Title = {{Elaboration, Generalization, Triangulation, and Interpretation: On
   Enhancing the Value of Mixed Method Research}},
Journal = {{ORGANIZATIONAL RESEARCH METHODS}},
