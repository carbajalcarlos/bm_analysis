@inproceedings{ ISI:000427725600032,
Author = {Chandra, Yakob Utama and Ernawaty and Suryanto},
Book-Group-Author = {{IEEE}},
Title = {{Bank vs Telecommunication E-Wallet : System Analysis, Purchase, and
   Payment Method of GO-Mobile CIMB Niaga and T-Cash Telkomsel}},
Booktitle = {{PROCEEDINGS OF 2017 INTERNATIONAL CONFERENCE ON INFORMATION MANAGEMENT
   AND TECHNOLOGY (ICIMTECH)}},
Year = {{2017}},
Pages = {{165-170}},
Note = {{2nd International Conference on Information Management and Technology
   (ICIMTech), Yogyakarta, INDONESIA, NOV 15-17, 2017}},
Organization = {{Bina Nusantara Univ, Sch Informat Syst; IEEE Indonesia Sect}},
Abstract = {{Current developing technology in finance is ewallet. E-wallet is a
   program or service which allows its user to save digital money and doing
   transactions easily whenever and wherever. Many banks and nonbank
   companies are competing to create innovation in this field. Go Mobile is
   an e-wallet product of CIMB Niaga, meanwhile, T-Cash is an e-wallet
   issued by a telecommunication company, PT Telkomsel. This research aimed
   to analyze the system, purchase and payment methods in Go Mobile and
   T-Cash. This research was done in qualitative design, used secondary
   data sourced from official websites of both companies as well as related
   researches. The result showed that Go Mobile and T-Cash's systems could
   ease the user's transaction. Go Mobile and T-Cash could be used in
   purchase and payment transaction through SMS as well as Android-based
   Smartphone application. In T-Cash, users who do not own an Android
   Smartphone could enjoy this service by attaching Near Field
   Communication (NFC) sticker on their phone. This is called T-Cash Tap.
   Based on the usage and ease of use, T-Cash as a telecommunication
   e-wallet had received highest acceptance in a society based on
   Technology Acceptance Model (TAM).}},
Publisher = {{IEEE}},
