@inproceedings{ ISI:000427401301037,
Author = {Alseddiqi, Mohamed and Heranjal, Ravikiran},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{ROLE OF SOCIAL NETWORKING IN ENHANCING ENGINEERING EDUCATION COURSES}},
