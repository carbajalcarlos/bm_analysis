@article{ ISI:000396611700004,
Author = {Camilleri, Mark Anthony and Camilleri, Adriana Caterina},
Title = {{Digital Learning Resources and Ubiquitous Technologies in Education}},
Journal = {{TECHNOLOGY KNOWLEDGE AND LEARNING}},
Year = {{2017}},
Volume = {{22}},
Number = {{1}},
Pages = {{65-82}},
Month = {{APR}},
Abstract = {{This research explores the educators' attitudes and perceptions about
   their utilisation of digital learning technologies. The methodology
   integrates measures from `the pace of technological innovativeness' and
   the `technology acceptance model' to understand the rationale for
   further ICT investment in compulsory education. A quantitative study was
   carried out amongst 241 educators in Malta. It has investigated the
   costs and benefits of using digital learning resources in schools from
   the educators' perspective. A principal component analysis has indicated
   that the educators were committed to using digital technologies. In
   addition, a stepwise regression analysis has shown that the younger
   teachers were increasingly engaging in digital learning resources.
   Following this study's empirical findings educational stakeholders are
   better informed about how innovative technologies can support our
   students. In conclusion, this contribution puts forward key implications
   and recommendations for regulatory authorities and policy makers for
   better curricula and educational outcomes.}},
