@inproceedings{ ISI:000427401303068,
Author = {Yaroslavova, Elena N. and Fedotova, Marina G.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{MERGING LEARNING MANAGEMENT SYSTEMS AND PERSONAL LEARNING ENVIRONMENT IN
   FOREIGN LANGUAGE TEACHING}},
