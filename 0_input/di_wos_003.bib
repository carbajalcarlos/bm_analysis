@inproceedings{ ISI:000427401302091,
Author = {Porta, Amparo},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{OPENING A NEW CHAPTER IN MUSIC HIGH EDUCATION. THE DEBATE ABOUT
   AUDIO-VISUAL MUSIC MASSIVELY BROADCASTED}},
