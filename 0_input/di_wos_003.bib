@article{ ISI:000397443900007,
Author = {Dewald, Andreas and Seufert, Sabine},
Title = {{AFEIC: Advanced forensic Ext4 inode carving}},
Journal = {{DIGITAL INVESTIGATION}},
Year = {{2017}},
Volume = {{20}},
Number = {{S}},
Pages = {{S83-S91}},
