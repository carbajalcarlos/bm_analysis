@inproceedings{ ISI:000429975304141,
Author = {Jekov, B. and Shoikova, E. and Donchev, D. and Petkova, P.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{STUDY ON THE IOT ECOSYSTEM BUSINESS MODELS AND THE SEGMENT OF STARTUPS}},
