@inproceedings{ ISI:000429975302007,
Author = {Paneva-Marinova, Desislava and Goynov, Maxim and Pavlov, Radoslav},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{ENHANCED AND PERSONALIZED LEARNING EXPERIENCE IN DIGITAL LIBRARIES}},
