@inproceedings{ ISI:000429975304109,
Author = {Terzieva, Valentina and Pavlov, Yuri and Todorova, Katia and
   Kademova-Katzarova, Petia},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{UTILITY ASSESSMENT OF ICT-EDUCATIONAL RESOURCES IN BULGARIAN SCHOOLS}},
Booktitle = {{10TH INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND INNOVATION
   (ICERI2017)}},
