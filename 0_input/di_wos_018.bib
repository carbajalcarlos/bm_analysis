Author = {Bernard, Christian and Clermidy, Fabien},
Book-Group-Author = {{IEEE}},
Title = {{A Low-Power VLIW processor for 3GPP-LTE Complex Numbers Processing}},
Booktitle = {{2011 DESIGN, AUTOMATION \& TEST IN EUROPE (DATE)}},
