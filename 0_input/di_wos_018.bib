@article{ ISI:000295688700006,
Author = {Fedderke, Johannes and Naumann, Dietmar},
Title = {{An analysis of industry concentration in South African manufacturing,
   1972-2001}},
Journal = {{APPLIED ECONOMICS}},
Year = {{2011}},
Volume = {{43}},
Number = {{22, SI}},
Pages = {{2919-2939}},
Abstract = {{This article examines industry concentration for the South African
   manufacturing sector over the 1972-2001 period, for the three-digit
   industry classification. The article notes both the high level of
   industry concentration in South African manufacturing and a rising trend
   in concentration across a wide range of industries as measured by the
   Gini and Rosenbluth coefficients. The article examines the impact of
   concentration on investment rates using a dynamic heterogeneous panel
   estimation methodology. We find that increased concentration
   unambiguously lowers investment rates.}},
