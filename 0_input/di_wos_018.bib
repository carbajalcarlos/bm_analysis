@inproceedings{ ISI:000317080001043,
Author = {Jones, Susan and Hall, Lynne and Hilton, Janette and Fowler, Jeffrey and
   Smith, Peter and Aylett, Ruth},
Editor = {{Torres, IC and Chova, LG and Martinez, AL}},
Title = {{INVESTIGATING THE USE OF THE IPAD IN HERITAGE EDUCATION FOR CHILDREN:
   IMPACT OF TECHNOLOGY ON THE `HISTORY DETECTIVE' IN A VICTORIAN CLASSROOM
   ROLE PLAY ACTIVITY}},
Booktitle = {{2011 4TH INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND INNOVATION
   (ICERI)}},
Year = {{2011}},
Pages = {{1262-1271}},
Note = {{4th International Conference of Education, Research and Innovation
   (ICERI), Madrid, SPAIN, NOV 14-16, 2011}},
Abstract = {{The indexical properties of real photographs impart a compelling link
   with previous generations and past events in social and economic
   history. However, the fragility of photographic material, means that
   rare photographs are consigned to the archive and in the context of
   children's heritage education, which relies heavily on photographic
   resources, this materiality of the photograph is inevitably transformed
   reducing the quality of the learning experience. Ongoing innovations in
   multi-touch small screen technology are opening up new opportunities in
   the context of heritage education which give users thin and lightweight
   mobile hand held devices which are fingertip driven and ideal for
   examining and manipulating images. Intuitively, the iPad has some
   commendable design features, particularly for children, but just how
   these impact upon children's experiences and responses to viewing
   archive images has yet to be fully investigated.
   This work sets out to explore the use of the iPad within the context of
   a Victorian classroom experience at the Old Donnison School Museum in
   the Sunderland Heritage Quarter, a regeneration project in the North
   East of England. In this paper we review the broad outcomes of this
   pilot into the use of the iPad in heritage education and draw some
   conclusions as to the use of this versatile technology for the effective
   deployment of the digital archive for children.}},
