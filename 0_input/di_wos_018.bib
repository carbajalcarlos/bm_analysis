@inproceedings{ ISI:000326292904087,
Author = {Calabi, Daniela and Quaggiotto, Marco and Chiodo, Elisa},
Editor = {{Chova, LG and Belenguer, DM and Martinez, AL}},
Title = {{DESIGN RESEARCH SCENARIOS FOR TERRITORIAL IDENTITIES}},
Booktitle = {{EDULEARN11: 3RD INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
