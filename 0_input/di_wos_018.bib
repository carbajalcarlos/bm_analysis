@inproceedings{ ISI:000313751300046,
Author = {Sumathi, T. and Hemalatha, M.},
Book-Group-Author = {{IEEE}},
Title = {{A Combined Hierarchical Model for Automatic Image Annotation and
   Retrieval}},
