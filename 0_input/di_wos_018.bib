@inproceedings{ ISI:000326292903005,
Author = {de Lucas, Yolanda and Calderon, Antonio and Martinez, Pilar and de
   Lucas, Rocio and Sanz, Mar and Martin, Cristina and Rubio, Sonia and
   Olivos, Luis and San Andres, Manuel and de Lucas, Julio},
Editor = {{Chova, LG and Belenguer, DM and Martinez, AL}},
Title = {{LIBRARY AND E-LEARNING 2.0: RESPONDING TO THE NEW EUROPEAN HIGHER
   EDUCATION AREA (EHEA) INTO AN DEEP ECONOMIC CRISIS}},
Booktitle = {{EDULEARN11: 3RD INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
