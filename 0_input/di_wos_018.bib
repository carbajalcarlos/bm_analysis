@inproceedings{ ISI:000303073400013,
Author = {Nylen, Daniel and Holmstrom, Jonny},
Editor = {{Chiasson, M and Henfridsson, O and Karsten, H and DeGross, JI}},
Title = {{From Forestry Machines to Sociotechnical Hybrids: Investigating the Use
   of Digitally Enabled Forestry Machines}},
Booktitle = {{RESEARCHING THE FUTURE IN INFORMATION SYSTEMS}},
