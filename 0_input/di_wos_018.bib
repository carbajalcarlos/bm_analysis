@inproceedings{ ISI:000378523000026,
Author = {Prevost, Amy C. and Nathan, Mitchell},
Book-Group-Author = {{ASEE}},
Title = {{STEM Integration in a Pre-College Course in Digital Electronics:
   Analysis of the Enacted Curriculum}},
Booktitle = {{2011 ASEE ANNUAL CONFERENCE \& EXPOSITION}},
