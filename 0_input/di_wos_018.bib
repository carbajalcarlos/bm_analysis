@inproceedings{ ISI:000326447702136,
Author = {Hernandez-Serrano, M. J. and Fuentes Aguti, M.},
Editor = {{Chova, LG and Torres, IC and Martinez, AL}},
Title = {{COMPETENCES AND STRATEGIES FOR EFFICIENT PROCESSES OF INTERNET
   INFORMATION SEEKING IN ACADEMIC CONTEXT}},
Booktitle = {{INTED2011: 5TH INTERNATIONAL TECHNOLOGY, EDUCATION AND DEVELOPMENT
   CONFERENCE}},
Year = {{2011}},
Pages = {{2942-2950}},
Note = {{5th International Technology, Education and Development Conference
   (INTED), Valencia, SPAIN, MAR 07-09, 2011}},
Abstract = {{Informational innovations in terms of modifications in the type and
   quantity of information available via the Internet are contributing to
   the establishment of a new social order that requires new skills to
   access, process and use information. Specifically, in academic context,
   all these innovations are aimed at the reorganization of the traditional
   models of teaching and learning, which also requires the commitment from
   teachers in the developments of strategies to access information
   efficiently and significantly.
   Under this approach, some European and national guidelines relating to
   the importance of the ability to access digital information effectively,
   making skills related to information literacy as key skills. Especially
   some generic skills have been emerged in the context of Bologna that
