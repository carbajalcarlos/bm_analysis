@inproceedings{ ISI:000317549600035,
Author = {Nemtoi, Gabriela and Bostan, Ionel},
Editor = {{Soliman, KS}},
Title = {{Digital economy and its impact over the personal data and private life
   protection}},
Booktitle = {{INNOVATION AND KNOWLEDGE MANAGEMENT: A GLOBAL COMPETITIVE ADVANTAGE,
   VOLS 1-4}},
