@inproceedings{ ISI:000412468300020,
Author = {Gambardella, Carmine and Pisacane, Nicola and Avella, Alessandra and
   Argenziano, Pasquale},
Book-Group-Author = {{IEEE}},
Title = {{Pompei Knowledge Factory Cultural Heritage and ICT for a smart city}},
Booktitle = {{PROCEEDINGS OF THE 2014 INTERNATIONAL CONFERENCE ON VIRTUAL SYSTEMS AND
   MULTIMEDIA (VSMM)}},
Series = {{Proceedings of the International Conference on Virtual Systems \&
   Multimedia (VSMM)}},
