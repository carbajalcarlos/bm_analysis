@article{ ISI:000329982000040,
Author = {Yoneda, J. and Masui, A. and Tenma, N. and Nagao, J.},
Title = {{Triaxial testing system for pressure core analysis using image
   processing technique}},
Journal = {{REVIEW OF SCIENTIFIC INSTRUMENTS}},
Year = {{2013}},
Volume = {{84}},
Number = {{11}},
Month = {{NOV}},
Abstract = {{In this study, a newly developed innovative triaxial testing system to
   investigate strength, deformation behavior, and/or permeability of gas
   hydrate bearing-sediments in deep sea is described. Transport of the
   pressure core from the storage chamber to the interior of the sealing
   sleeve of a triaxial cell without depressurization was achieved. An
   image processing technique was used to capture the motion and local
   deformation of a specimen in a transparent acrylic triaxial pressure
   cell and digital photographs were obtained at each strain level during
   the compression test. The material strength was successfully measured
   and the failure mode was evaluated under high confining and pore water
   pressures. (C) 2013 AIP Publishing LLC.}},
