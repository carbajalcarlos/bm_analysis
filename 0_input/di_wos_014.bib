Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{ENHANCING THE TEACHING AND LEARNING OF HISTORY AND GEOGRAPHY THROUGH
   GIS: THE CASE OF ROUTES AND BATTLES OF PENINSULAR WAR (1807-1814)
   DIGITAL MAP}},
Booktitle = {{ICERI2014: 7TH INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND
   INNOVATION}},
Series = {{ICERI Proceedings}},
Year = {{2014}},
