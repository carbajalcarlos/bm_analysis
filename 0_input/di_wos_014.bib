@inproceedings{ ISI:000366837206073,
Author = {Coffey, S. and Bignell, P. and Graham, L. and de la Rocha, A. and
   Chorney, D. and Muirhead, B. and Vogel, E. and Desjardins, F. and
   Prasad, M. and Stein, N.},
