@inproceedings{ ISI:000366835102005,
Author = {Schmitt, Ulrich},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{THE ROLE OF PERSONAL KNOWLEDGE MANAGEMENT SYSTEMS IN MAKING CITIZENS
   HIGHLY KNOWLEDGEABLE}},
Booktitle = {{INTED2014: 8TH INTERNATIONAL TECHNOLOGY, EDUCATION AND DEVELOPMENT
   CONFERENCE}},
