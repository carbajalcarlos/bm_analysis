@article{ ISI:000328489800010,
Author = {Liu, Jiying and Zhu, Jubo and Yan, Fengxia and Zhang, Zenghui},
Title = {{COMPRESSIVE SAMPLING AND L1 MINIMIZATION FOR SAR IMAGING WITH LOW
   SAMPLING RATE}},
Journal = {{INVERSE PROBLEMS AND IMAGING}},
Year = {{2013}},
Volume = {{7}},
Number = {{4}},
Pages = {{1295-1305}},
Month = {{NOV}},
Abstract = {{This paper presents a new Synthetic Aperture Radar (SAR) imaging system
   based on compressive sampling scheme and l1 minimization. The
   compressive sampling scheme comprises of randomization and integration
   of radar echoes which slows down the analog-to-digital converters (ADC)
   rate significantly without analiasing in image formation. Numerical
   experiments indicate that the resolution of SAR images retrieved by our
   method outperform that obtained by conventional methods. The results
   also reveal that the new SAR imaging system can still retrieve
   non-ambiguous images even when the data rate is 1/10 of the original
   one. Finally, we applied the new method on raw data of RADARSAT-1 to
   testify its practicability.}},
