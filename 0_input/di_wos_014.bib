@article{ ISI:000323651000020,
Author = {Kuhl, Matthias and Gieschke, Pascal and Rossbach, Daniel and
   Hilzensauer, Sascha A. and Panchaphongsaphak, Thanapon and Ruther,
   Patrick and Lapatki, Bernd G. and Paul, Oliver and Manoli, Yiannos},
Title = {{A Wireless Stress Mapping System for Orthodontic Brackets Using CMOS
   Integrated Sensors}},
Journal = {{IEEE JOURNAL OF SOLID-STATE CIRCUITS}},
Year = {{2013}},
Volume = {{48}},
Number = {{9}},
Pages = {{2191-2202}},
Month = {{SEP}},
Note = {{IEEE Bipolar/BiCMOS Circuits and Technology Meeting (BCTM), Portland,
   OR, SEP 30-OCT 03, 2012}},
Organization = {{IEEE; Skyworks; RFMD; IBM; Tektronix; Maxim; Analog Devices; NXP; Texas
   Instruments; Anadigics; Microsemi; Cascade Microtech}},
Abstract = {{A wireless stress mapping system for the development of ``smart
   brackets{''} is presented. The system is designed for monitoring the
   forces and moments applied to individual teeth during orthodontic
   therapy which may contribute to improve treatment effectiveness. It
   comprises a stress mapping chip fabricated in a 0.35-mu m process and a
   micro coil produced by gold electroplating in a photoresist mask.
   Twenty-four transistor-based stress sensors for measuring either
   in-plane shear stress or the difference of in-plane normal stresses are
   strategically distributed over the chip area. The sensor signals are
   processed by a variable-gain differential difference amplifier and
   digitized by a 10-bit SAR ADC, enabling a resolution down to 11 kPa at
   highest gain. A wireless interface conditions the energy received at
   13.56 MHz and transmits the data by load modulation. With dimensions of
   2 x 2.5 x 0.73 mm(3) and a power consumption of 1.75 mW, the system lays
   the foundation for the assembly of smart brackets as innovative tools in
   orthodontic therapy.}},
Publisher = {{IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC}},
Address = {{445 HOES LANE, PISCATAWAY, NJ 08855-4141 USA}},
