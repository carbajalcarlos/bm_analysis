@article{ ISI:000209378500005,
Author = {Spyridou, Lia-Paschalia and Matsiola, Maria and Veglis, Andreas and
   Kalliris, George and Dimoulas, Charalambos},
Title = {{Journalism in a state of flux: Journalists as agents of technology
   innovation and emerging news practices}},
Journal = {{INTERNATIONAL COMMUNICATION GAZETTE}},
Year = {{2013}},
Volume = {{75}},
Number = {{1, SI}},
Pages = {{76-98}},
Month = {{FEB}},
Abstract = {{The move to a networked media environment presents a range of challenges
   for journalistic roles, norms and daily practices. This article employs
   actor network theory to investigate how different actors negotiate and
   ultimately shape the manner in which the internet and related digital
   technologies are embedded in the newsroom. Findings suggest that
   professional culture -articulated in skills, ideas and practices - acts
   as a network that weakens the potential impact of technology towards
   innovation and audience-oriented models of journalism. The results point
   to the conclusion that the internet and related tools are seen as
   empowering journalists to do their (traditional) jobs better instead of
   moving on to the next stage built around a stronger commitment to
   capitalize on the growing sociotechnical potential.}},
