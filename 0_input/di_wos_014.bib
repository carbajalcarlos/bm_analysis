@inproceedings{ ISI:000367082905041,
Author = {Huizing, Klaas Jan and de Vries, Sjoerd},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{THE IMPLEMENTATION OF SOCIAL MEDIA AS A LEARNING TOOL AT NHL UNIVERSITY
   FOR APPLIED SCIENCES IN LEEUWARDEN, THE NETHERLANDS}},
