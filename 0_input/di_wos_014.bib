@inproceedings{ ISI:000366835101044,
Author = {Brikmane, Karina and Samusevica, Alida},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{THE DEVELOPMENT OF INFORMATION COMMUNICATION TECHNOLOGY COMPETENCIES TO
   ENSURE THE COMPETITIVENESS OF TEACHERS}},
