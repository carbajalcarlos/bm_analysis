@inproceedings{ ISI:000366837204043,
Author = {Lou, Y.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{INTERACTIVE SCENARIO-BASED SERIOUS GAMES FOR SCIENCE LEARNING IN MIDDLE
   AND HIGH SCHOOL CLASSROOMS}},
