@inproceedings{ ISI:000341702105145,
Author = {Lawand, N. S. and Ngamkham, W. and Nazarian, G. and French, P. J. and
   Serdijn, W. A. and Gaydadjiev, G. N. and Briaire, J. J. and Frijns, J.
   H. M.},
