@article{ ISI:000320455800018,
Author = {Chevallard, J. and Charlot, S. and Wandelt, B. and Wild, V.},
Title = {{Insights into the content and spatial distribution of dust from the
   integrated spectral properties of galaxies}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
