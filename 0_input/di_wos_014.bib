@article{ ISI:000318610700003,
Author = {Kirilenko, Andrei A. and Lo, Andrew W.},
Title = {{Moore's Law versus Murphy's Law: Algorithmic Trading and Its Discontents}},
Journal = {{JOURNAL OF ECONOMIC PERSPECTIVES}},
Year = {{2013}},
Volume = {{27}},
Number = {{2}},
Pages = {{51-72}},
Month = {{SPR}},
Abstract = {{Financial markets have undergone a remarkable transformation over the
   past two decades due to advances in technology. These advances include
   faster and cheaper computers, greater connectivity among market
   participants, and perhaps most important of all, more sophisticated
   trading algorithms. The benefits of such financial technology are
   evident: lower transactions costs, faster executions, and greater volume
   of trades. However, like any technology, trading technology has
   unintended consequences. In this paper, we review key innovations in
   trading technology starting with portfolio optimization in the 1950s and
   ending with high-frequency trading in the late 2000s, as well as
   opportunities, challenges, and economic incentives that accompanied
   these developments. We also discuss potential threats to financial
   stability created or facilitated by algorithmic trading and propose
   ``Financial Regulation 2.0,{''} a set of design principles for bringing
   the current financial regulatory framework into the Digital Age.}},
Publisher = {{AMER ECONOMIC ASSOC}},
Address = {{2014 BROADWAY, STE 305, NASHVILLE, TN 37203 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Kirilenko, AA (Reprint Author), MIT, Alfred P Sloan Sch Management, Cambridge, MA 02139 USA.
   Kirilenko, Andrei A., MIT, Alfred P Sloan Sch Management, Cambridge, MA 02139 USA.
   Kirilenko, Andrei A., US Commod Futures Trading Commiss, Washington, DC USA.
   Lo, Andrew W., MIT, Alfred P Sloan Sch Management, Lab Financial Engn, Cambridge, MA 02139 USA.
