@inproceedings{ ISI:000390749105064,
Author = {Shamn, Saqib and Cang, Shuang and Yu, Hongnian and Li, Yun},
Book-Group-Author = {{IEEE}},
Title = {{Management Approaches for Industry 4.0 A human resource management
   perspective}},
