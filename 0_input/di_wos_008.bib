@inproceedings{ ISI:000391680700050,
Author = {Evans, Nina and Bosua, Rachelle and Fourie, Louis},
Editor = {{Grant, KA and Wise, S}},
Title = {{The Challenges and Mindset Limitations Impacting Innovation in
   Manufacturing SMEs in Regional Australia}},
