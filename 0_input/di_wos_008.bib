@inproceedings{ ISI:000389720700044,
Author = {Al-Lawati, Badar H. and Fang, Xiaowen},
Editor = {{Nah, FFH and Tan, CH}},
Title = {{Diffusion of Innovations: The Case Study of Oman's e-Payment Gateway}},
