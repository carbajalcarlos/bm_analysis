@inproceedings{ ISI:000390536700095,
Author = {Shah, Vikas S.},
Editor = {{Chakrabarti, S and Saha, HN}},
Title = {{Ubiquitous Operational Governance Framework: Managing Consumer Mobile
   Commerce Active Index}},
Booktitle = {{2016 IEEE 7TH ANNUAL UBIQUITOUS COMPUTING, ELECTRONICS MOBILE
