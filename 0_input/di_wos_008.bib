@article{ ISI:000385611600006,
Author = {Laouamer, Lamri and Alshaikh, Muath and Nana, Laurent and Pascu, Anca
   Chrisitine},
Title = {{Generating optimal informed and adaptive watermark image based on
   zero-suppressed binary decision diagrams for medical images}},
Journal = {{INTERNATIONAL JOURNAL OF ELECTRONIC SECURITY AND DIGITAL FORENSICS}},
Year = {{2016}},
Volume = {{8}},
Number = {{3}},
Pages = {{262-284}},
Abstract = {{Watermarking protects legitimate copies of digital multimedia from
   unauthorised use. Digital watermarks are used to verify the
   authenticity, integrity and confidentiality of data. Watermark
   generation is one of the most important aspects of watermarking schemes,
   and should aim to produce as small a watermark to reduce the complexity
   of computational processes. Although embedding a large amount of
   watermark data increases the chances of recovering it, this also
   increases the complexity, which can become impractical for real-time
   applications. In this paper, we focus on the robustness of medical image
   watermarks and present a means to generate a small watermark. This idea
   is very innovative in the watermarking field. The proposed approach is
   based on zero-suppressed binary decision diagrams (ZBDDs). Application
   of ZBDD to medical image watermarking will help us to take into account
   the complexity, the capacity and the watermark robustness factors. The
   results obtained are very significant and encouraging.}},
