@article{ ISI:000391247000006,
Author = {Leite, Julieta},
Title = {{THE TECHNOLOGICAL IMAGINATION: BETWEEN INTERPRETATIVE APPROACH AND
   VECTOR OF INNOVATION OF CONTEMPORARY HUMAN HABITATS}},
Journal = {{SOCIETES}},
Year = {{2016}},
Volume = {{132}},
Number = {{2}},
Pages = {{65-74}},
Abstract = {{The increasing use of information and communication technologies in
   everyday life creates a new spatial configuration. This configuration is
   inseparable from both social and spatial relationships. This paper
   contributes to understanding of how such technologies redefine our
   spatial experience, articulating the collective imaginary of space from
   a multi-disciplinary perspective. We define this imaginary as how people
   perceive, experience, and give meaning to the space of architecture, and
   we discuss distinct moments in history when this imaginary was affected
   by technological developments. Finally, we introduce the concept of
   `imaginal space', defined by the fusion of the architectural and the
   digital spaces combined by its images and narratives, into physical,
   technical, and symbolic dimensions and whose relevance is to give
   cohesion to contemporary social groupings.}},
