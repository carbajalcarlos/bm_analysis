@inproceedings{ ISI:000389727300065,
Author = {Suh, Ayoung and Li, Ruohan and Liu, Lili},
Editor = {{Stephanidis, C}},
Title = {{The Use of Wearable Technologies and Body Awareness: A Body-Tool
   Relationship Perspective}},
Booktitle = {{HCI INTERNATIONAL 2016 - POSTERS' EXTENDED ABSTRACTS, PT I}},
Series = {{Communications in Computer and Information Science}},
Year = {{2016}},
Volume = {{617}},
Pages = {{388-392}},
Note = {{18th International Conference on Human-Computer Interaction (HCI
   International), Toronto, CANADA, JUL 17-22, 2016}},
Abstract = {{Wearable technologies-innovative and multi-functional media technologies
   that can be attached to our body-have received a great deal of attention
   by the digital media industry. The wearability of technology brings new
   affordances that may significantly change the way humans interact with
   technological objects. However, little is known about how such emerging
   technologies can shape our perceptions of the body and the interactions
   associated with technology use. Focusing on users' experience of
   wearable technologies, this study explores the influence of wearable
   technologies on individuals' perceptions of their body-tool relationship
   and body awareness. A series of in-depth interviews was conducted to
   investigate how users' interactions with wearable technologies affect
   their perceptions. Our findings indicate that perceptual properties
   (materials, weight, battery life, and vibration) influence users'
   body-tool relationship, whereas motor activity properties (monitoring,
   tracking, and real-time feedback) influence users' body awareness.}},
