@article{ ISI:000385618600006,
Author = {Encheva, Marina},
Title = {{TEACHING INFORMATION LITERACY COURSES IN THE CONTEXT OF LIBRARY AND
   INFORMATION SCIENCE EDUCATION IN BULGARIA: CHALLENGES AND INNOVATIVE
   APPROACHES}},
Journal = {{JOURNAL OF LIBRARY ADMINISTRATION}},
Year = {{2016}},
Volume = {{56}},
Number = {{5}},
Pages = {{595-602}},
Abstract = {{The article examines the application of innovative approaches in the
   information literacy training of library and information science
   students in Bulgaria. The change of the educational paradigm in the
   field is presented based on recent developments in one of the oldest
   Bulgarian institutions that prepares librarians and information
   specialists-the State University of Library Studies and Information
   Technologies. The active learning strategies suitable for information
   literacy skills development courses are studied by the use of three
   methods: ``learning by doing,{''} ``interaction in the learning
   group,{''} ``learning by trial and error{''} within the practical
   training of students. The prospects for transforming information
   literacy instruction through the elaboration of digital educational
   games and a game-based learning model in the frame of the international
   project tiLIT are also outlined.}},
