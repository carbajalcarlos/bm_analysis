@article{ ISI:000339045500001,
Author = {Scotti, Giuseppe and Pennisi, Salvatore and Monsurro, Pietro and
   Trifiletti, Alessandro},
Title = {{88-mu A 1-MHz Stray-Insensitive CMOS Current-Mode Interface IC for
   Differential Capacitive Sensors}},
