@article{ ISI:000214788600008,
Author = {Krumsvik, Arne H.},
Title = {{NEWSPAPER OWNERSHIP AND THE PRIORITIZATION OF DIGITAL COMPETENCES}},
Journal = {{DIGITAL JOURNALISM}},
Year = {{2015}},
Volume = {{3}},
Number = {{5}},
Pages = {{777-790}},
Abstract = {{This study of digital competence in newspapers finds a strong
   relationship between ownership and the prioritization of digital
   competences. Newspaper executives in newspapers owned by media groups
   rated most digital competences significantly higher than executives from
   other newspapers, and ownership explained the relative importance of
   digital competences necessary in process innovation. There are also
   indications of a conflict between the product innovation of online news
   features and the process innovation of efficient multi-platform news
   production. The low priority of computer-assisted journalism skills will
   have consequences for the priority of the development of advanced
   digital methods in investigative journalism.}},
