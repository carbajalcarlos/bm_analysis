@article{ ISI:000343095300032,
Author = {Saheb, Nouari and Aliyu, Ismaila Kayode and Hassan, Syed Fida and
   Al-Aqeeli, Nasser},
Title = {{Matrix Structure Evolution and Nanoreinforcement Distribution in
   Mechanically Milled and Spark Plasma Sintered Al-SiC Nanocomposites}},
