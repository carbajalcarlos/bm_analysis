@article{ ISI:000345326600002,
Author = {Ben-Zeev, D.},
Title = {{Technology-based interventions for psychiatric illnesses: improving
   care, one patient at a time}},
Journal = {{EPIDEMIOLOGY AND PSYCHIATRIC SCIENCES}},
