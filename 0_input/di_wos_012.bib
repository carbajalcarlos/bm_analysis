@article{ ISI:000337112200018,
Author = {Salaberria, Itziar and Perallos, Asier and Azpilicueta, Leire and
   Falcone, Francisco and Carballedo, Roberto and Angulo, Ignacio and
   Elejoste, Pilar and Bahillo, Alfonso and Astrain, Jose Javier and
   Villadangos, Jesus},
Title = {{Ubiquitous Connected Train Based on Train-to-Ground and Intra-Wagon
   Communications Capable of Providing on Trip Customized Digital Services
   for Passengers}},
Journal = {{SENSORS}},
Year = {{2014}},
Volume = {{14}},
Number = {{5}},
Pages = {{8003-8025}},
Month = {{MAY}},
Abstract = {{During the last years, the application of different wireless
   technologies has been explored in order to enable Internet connectivity
   from vehicles. In addition, the widespread adoption of smartphones by
   citizens represents a great opportunity to integrate such nomadic
   devices inside vehicles in order to provide new and personalized on trip
   services for passengers. In this paper, a proposal of communication
   architecture to provide the ubiquitous connectivity needed to enhance
   the smart train concept is presented and preliminarily tested. It
   combines an intra-wagon communication system based on nomadic devices
   connected through a Bluetooth Piconet Network with a highly innovative
   train-to-ground communication system. In order to validate this
   communication solution, several tests and simulations have been
   performed and their results are described in this paper.}},
