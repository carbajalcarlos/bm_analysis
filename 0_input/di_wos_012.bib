@article{ ISI:000342815500005,
Author = {Roche, Stephane},
Title = {{Geographic Information Science I: Why does a smart city need to be
   spatially enabled?}},
Journal = {{PROGRESS IN HUMAN GEOGRAPHY}},
Year = {{2014}},
Volume = {{38}},
Number = {{5}},
Pages = {{703-711}},
Month = {{OCT}},
Abstract = {{In this report I propose to examine the concept of the smart city' from
   the standpoint of spatial enablement. I analyse emerging research on
   smart cities, particularly those addressing the potential role of
   GISciences in the development and implementation of the concept of smart
   cities. I develop the idea that the intelligence of a city should be
   measured by its ability to produce favourable conditions to get urban
   operators (citizens, organizations, private companies, etc.) actively
   involved into sociospatial innovation dynamics. To obtain such a
   commitment, I believe that operators should be able to develop and
   mobilize (digital) spatial skills so that they could efficiently manage
   their spatiality. In other words, I argue that a smart city is first of
   all a spatially enabled city.}},
