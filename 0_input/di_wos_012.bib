Title = {{Multi-Loop Social Learning for Sustainable Land and Water Governance:
   Towards a Research Agenda on the Potential of Virtual Learning Platforms}},
Journal = {{NJAS-WAGENINGEN JOURNAL OF LIFE SCIENCES}},
Year = {{2014}},
Volume = {{69}},
