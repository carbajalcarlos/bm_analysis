@article{ ISI:000345544500002,
Author = {Lopez-Berzosa, David and Gawer, Annabelle},
Title = {{Innovation policy within private collectives: Evidence on 3GPP's
   regulation mechanisms to facilitate collective innovation}},
Journal = {{TECHNOVATION}},
Year = {{2014}},
Volume = {{34}},
Number = {{12, SI}},
Pages = {{734-745}},
Month = {{DEC}},
Abstract = {{This article provides insights on how to manage collective innovation in
   the digital economy, an innovation regime which is riddled with complex
   regulatory challenges and increasing litigation over intellectual
   property rights. Private collective organizations face two main
   challenges: (1) to promote collective innovation while preserving the
   private interests of the firms within the collective, and (2) to ensure
   that collective innovation does not weaken healthy competition. Through
   a case study of the Third Generation Partnership Project (3GPP), an
   exemplary private collective federation of organizations composed of
   standardization bodies, industry consortia and technology producers, we
   identify organizational solutions to these challenges. We find that a
   combination of specific IP rights instruments is key to manage these
   trade-offs. We also find that the combined policies of essential
   patenting, FRAND, and maximum royalty rate help overcome the specific
   challenges associated with collective innovation within competitive
   contexts. Finally we discuss the implications of our findings for
   managers and for policy. (C) 2014 Elsevier Ltd. All rights reserved.}},
