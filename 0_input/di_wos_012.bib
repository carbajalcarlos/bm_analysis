@article{ ISI:000336838500015,
Author = {Henfridsson, Ola and Yoo, Youngjin},
Title = {{The Liminality of Trajectory Shifts in Institutional Entrepreneurship}},
Journal = {{ORGANIZATION SCIENCE}},
Year = {{2014}},
Volume = {{25}},
Number = {{3}},
Pages = {{932-950}},
Month = {{MAY-JUN}},
Abstract = {{In this paper, we develop a process model of trajectory shifts in
   institutional entrepreneurship. We focus on the liminal periods
   experienced by institutional entrepreneurs when they, unlike the rest of
   the organization, recognize limits in the present and seek to shift a
   familiar past into an unfamiliar and uncertain future. Such periods
   involve a situation where the new possible future, not yet fully formed,
   exists side-by-side with established innovation trajectories. Trajectory
   shifts are moments of truth for institutional entrepreneurs, but little
   is known about the underlying mechanisms of how entrepreneurs
   reflectively deal with liminality to conceive and bring forth new
   innovation trajectories. Our in-depth case study research at CarCorp
   traces three such mechanisms (reflective dissension, imaginative
   projection, and eliminatory exploration) and builds the basis for
   understanding the liminality of trajectory shifts. The paper offers
   theoretical implications for the institutional entrepreneurship
