@article{ ISI:000218647300015,
Author = {Landuzzi, Carla},
Title = {{The search of a digital immortality?}},
Journal = {{IN BO-RICERCHE E PROGETTI PER IL TERRITORIO LA CITTA E L ARCHITETTURA}},
Year = {{2015}},
Volume = {{6}},
Number = {{8}},
Pages = {{159-165}},
Abstract = {{The text intends to examine how technological innovations allow the
   construction of web spaces for shared funeral rituals and their
   partecipation by those same communities that move across the network. It
   can be assumed a new resacralization process of rituals referring to an
   individual system of meanings. The web may be capable of reappropriating
   the experience of pain through new ways that allow flexible access and
   fruition without spatial or temporal limits. They are interactive
   rituals that build an elective sociality, as practiced by the inclusion
   decisions under emotionally meaningful groups. The search for a digital
