@inproceedings{ ISI:000398586305067,
Author = {de Rosa, Annamaria Silvana},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{THE EUROPEAN/INTERNATIONAL JOINT PHD IN SOCIAL REPRESENTATIONS AND
   COMMUNICATION: A PIONEER CASE OF TRIPLE ``I{''} (INTERNATIONAL,
   INTERDISCIPLINARY, INTERSECTORAL) NETWORKED JOINT DOCTORATE}},
Booktitle = {{INTED2015: 9TH INTERNATIONAL TECHNOLOGY, EDUCATION AND DEVELOPMENT
