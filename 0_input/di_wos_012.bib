@inproceedings{ ISI:000398586307076,
Author = {Morales, Ruby C. and Dieguez, Mauricio and Carrasco, Ricardo and
   Cisternas, Juan P.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{ICT MEDIATOR AS TEACHING TRANSVERSAL COMPETENCES AND DIGITAL INCLUSION}},
