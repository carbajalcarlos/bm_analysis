@article{ ISI:000337103200013,
Author = {Yueh, Hsiu-Ping and Chen, Tzy-Ling and Lin, Weijane and Sheen,
   Horn-Jiunn},
Title = {{Developing Digital Courseware for a Virtual Nano-Biotechnology
   Laboratory: A Design-based Research Approach}},
Journal = {{EDUCATIONAL TECHNOLOGY \& SOCIETY}},
Year = {{2014}},
Volume = {{17}},
Number = {{2, SI}},
Pages = {{158-168}},
Month = {{APR}},
Abstract = {{This paper first reviews applications of multimedia in engineering
   education, especially in laboratory learning. It then illustrates a
   model and accreditation criteria adopted for developing a specific set
   of nanotechnology laboratory courseware and reports the design-based
   research approach used in designing and developing the e-learning
   material. According to findings of the present study, the courseware
   developed satisfies the ``e-Learning Courseware Quality Checklist
   version 3.0{''} in most dimensions. This paper concludes by presenting
   the researchers' findings and problems encountered in this ongoing
   study, and it describes a future study plan that will include the
   student input regarding these laboratories. This study should contribute
   to the innovative use of technology in facilitating engineering
   laboratory learning and in instructional design practice and research.}},
