@article{ ISI:000244615400027,
Author = {Di Lapi, Christine M. and Ring, Steffen},
Title = {{Potential frequency bands for terrestrial mobile multimedia broadcasting
   applications}},
Journal = {{IEEE TRANSACTIONS ON BROADCASTING}},
Year = {{2007}},
Volume = {{53}},
Number = {{1, 2}},
Pages = {{412-417}},
Month = {{MAR}},
Abstract = {{While terrestrial mobile multimedia broadcasting presents an exciting
   opportunity for both innovation and convergence to provide multimedia
   services in new operating environments, it remains a challenge to locate
   frequency spectrum in which to deploy these applications. The fact is
   that the frequencies suitable for such applications in a mobile
   environment are already heavily encumbered, and it is difficult to
   locate bands that are harmonized, even on a regional basis. Nonetheless,
   there are also opportunities for new mobile multimedia broadcasting
   applications, given the transition to technologies that more efficiently
   utilize spectrum, such as in the case of the analog to digital
   television transition.}},
