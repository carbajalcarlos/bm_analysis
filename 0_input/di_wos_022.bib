@inproceedings{ ISI:000246211500095,
Author = {Recchioni, Marco and Casalino, Nunzio and Castello, Valentina and
   Roscani, Maurizio},
Editor = {{Uskov, V}},
Title = {{An innovative training system by digital terrestrial television:
   TSC-learning}},
Booktitle = {{PROCEEDINGS OF THE SIXTH IASTED INTERNATIONAL CONFERENCE ON WEB-BASED
   EDUCATION}},
Year = {{2007}},
Pages = {{537+}},
Note = {{6th IASTED International Conference on Web-Based Education, Chamonix,
   FRANCE, MAR 14-16, 2007}},
Organization = {{Int Assoc Sci \& Technol Dev; World Modelling \& Simulat Forum}},
Abstract = {{The term digital video broadcasting (DVB) is today used as a synonym for
   digital television in many countries. Digital Terrestrial Television
   (DTT) is an implementation of digital technology to provide a greater
   number of channels, a better quality of picture and sound through a
   conventional antenna instead of a satellite dish or cable connection.
   Digital television isn't just an innovative signal representation; it
   represents a new multiple-channel data broadcasting that affect not only
   the technology side but also the content itself. Within this framework,
   the T-Learning (Television-Learning) has reached a considerable
   importance in the last years and is emerging as a potentially important
   media to create opportunities for teaming at home.
