@inproceedings{ ISI:000268001100067,
Author = {Dassatti, A. and Zezza, S. and Nicola, M. and Masera, G.},
Book-Group-Author = {{ACM}},
Title = {{Beyond 3G Wireless Communication System Prototype}},
