@inproceedings{ ISI:000249585300022,
Author = {Korteland, Evelien and Bekkers, Victor},
Editor = {{Wimmer, MA and Scholl, J and Gronlund, A}},
Title = {{Diffusion of e-government innovations in the dutch public sector: The
   case of digital community policing}},
