@incollection{ ISI:000269523100042,
Author = {Lombardi, Patrizia and Cooper, Ian},
Editor = {{Cunningham, P and Cunningham, M}},
Title = {{eDomus Vs eAgora: The Italian Case and Implications for the EU 2010
   Strategy}},
Booktitle = {{EXPANDING THE KNOWLEDGE ECONOMY: ISSUES, APPLICATIONS, CASE STUDIES, PTS
   1 AND 2}},
Series = {{Information and Communication Technologies and the Knowledge Economy}},
Year = {{2007}},
Volume = {{4}},
Number = {{1-2}},
Pages = {{344-351}},
Abstract = {{Harnessing ICTs effectively is one of the main vehicles for achieving
   the EU's 2010 strategy to become the most competitive digital
   knowledge-based economy. Achieving this requires innovation and a
   process of cultural, structural and economical changes, supported by
   ICTs towards the so-called eAgora. The aims of this paper are to show
   that, instead of delivering an eAgora, we are on course for an eDomus.
   The paper is structured in four main parts. The first introduces the
   eAgora trajectory. The second discusses the eDomus use of digital
   technologies by Italian citizens as depicted by the results of recent
   surveys. The third compares this situation with the outcomes illustrated
   at a European level by the Intelcities EU IP project. Finally, the
   fourth discusses the main implications of these findings for meeting the
   Lisboa strategy objectives, using OECD framework as a benchmarking tool
   for measuring increasing levels of citizens' participation in
   decision-making.}},
