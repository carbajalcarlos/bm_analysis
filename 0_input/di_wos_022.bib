@inproceedings{ ISI:000247666500050,
Author = {Dosselmann, Richard and Yang, Xue Dong},
Title = {{A prototype no-reference video quality system}},
Booktitle = {{FOURTH CANADIAN CONFERENCE ON COMPUTER AND ROBOT VISION, PROCEEDINGS}},
Year = {{2007}},
Pages = {{411+}},
Note = {{4th Canadian Conference on Computer and Robot Vision, Montreal, CANADA,
   MAY 28-30, 2007}},
Organization = {{Canadian Image Proc Pattern Recognit Soc; Int Assoc Pattern Recognit}},
Abstract = {{This paper introduces a number of innovative no-reference algorithms to
   assess the perceived quality of real-time analog and digital television
   and video streams. A prototype system is developed to locate and measure
   the impact of three types of impairments that commonly impair television
   and video signals. Analog sequences are tested for the presence of
   random noise. In the case of digital signals, two fundamental types of
   errors are of interest. The first is the blocking artifact that is so
   pervasive among DCT-based compression schemes such as MPEG. The second
   category includes errors caused by random changes to the bit stream of a
   signal. Of the various forms that these distortions may take on, only
   those that appear as ``colored blocks `` are detected by this system.
   Ideas to address the remaining issues are discussed.}},
