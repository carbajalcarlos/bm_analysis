@article{ ISI:000248099800006,
Author = {Scollin, Patrick and Healey-Walsh, Judith and Kafel, Kathleen and Mehta,
   Apurva and Callahan, John},
Title = {{Evaluating students' attitudes to using PDAs in nursing clinicals at two
   schools}},
