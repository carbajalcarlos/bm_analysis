@article{ ISI:000250742600006,
Author = {Bonnett, John},
Title = {{Charting a new aesthetics for history: 3D, scenarios, and the future of
   the historian's craft}},
Journal = {{HISTOIRE SOCIALE-SOCIAL HISTORY}},
Year = {{2007}},
Volume = {{40}},
Number = {{79}},
Pages = {{169-207}},
Month = {{MAY}},
Abstract = {{Innovations in computing are presenting historians with access to new
   forms of expression with the potential to enhance scholars' capacities
   and to support novel methods for analysis, expression, and teaching.
   Computer-generated form can change the way we generate, appropriate, and
   disseminate content. If these benefits are to be realized, however, the
   discipline must make room for a new domain of practice-based research.
   The practices we have for knowledge generation were devised in
   association with print technology, and historians must now acquire and
   develop practices that can inform our use of digital forms of
   representation, as well as the platforms that sustain them.}},
