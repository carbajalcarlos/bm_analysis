@article{ ISI:000255192400004,
Author = {Squicciarini, A. and Bertino, E. and Ferrari, Elena and Paci, F. and
   Thuraisingham, B.},
Title = {{PP-Trust-chi: A system for privacy preserving trust negotiations}},
Journal = {{ACM TRANSACTIONS ON INFORMATION AND SYSTEM SECURITY}},
