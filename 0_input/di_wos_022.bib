Title = {{Agile, detecting and discriminating, infrared electro-optical system
   (ADDIOS) application to coded aperture imaging and non-imaging sensor
   systems}},
Booktitle = {{ADAPTIVE CODED APERTURE IMAGING AND NON-IMAGING SENSORS}},
Series = {{Proceedings of SPIE}},
