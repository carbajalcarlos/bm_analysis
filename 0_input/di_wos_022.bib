@article{ ISI:000237887500009,
Author = {Arnulfi, GL and Blanchini, F and Giannattasio, P and Micheli, D and
   Pinamonti, P},
Title = {{Extensive study on the control of centrifugal compressor surge}},
Journal = {{PROCEEDINGS OF THE INSTITUTION OF MECHANICAL ENGINEERS PART A-JOURNAL OF
   POWER AND ENERGY}},
