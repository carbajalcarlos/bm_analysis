@inproceedings{ ISI:000245436200006,
Author = {Newton, Leonard R.},
Editor = {{Tatnall, A and Okamoto, T and Visscher, A}},
Title = {{SUPPORTING TEACHERS' PROFESSIONAL DEVELOPMENT THROUGH ICT - Reflections
   on two case studies}},
Booktitle = {{Knowledge Management for Educational Innovation}},
Series = {{INTERNATIONAL FEDERATION FOR INFORMATION PROCESSING}},
Year = {{2007}},
Volume = {{230}},
Pages = {{43-51}},
Note = {{7th Conference on Information Technology in Educational Management
   (ITEM), Hamamatsu, JAPAN, JUL 23-26, 2006}},
Organization = {{IFIP WG 3 7}},
Abstract = {{This paper discusses the potential of two information and communications
   technology tools designed to support the management of school teachers'
   professional knowledge. Framed by drawing on experience of teacher
   education in England, the paper is structured around a consideration of
   what is meant by teachers' professional knowledge and issues in
   technology-mediated professional development. Two evaluative case
   studies of ICT-mediated knowledge management for teacher development
   purposes are presented. First, an evaluation of a curriculum development
