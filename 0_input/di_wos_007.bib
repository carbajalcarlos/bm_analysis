@inproceedings{ ISI:000395722500131,
Author = {Maximova, Olga},
Book-Group-Author = {{SGEM}},
Title = {{TEACHING ACADEMIC READING IN FOREIGN LANGUAGES: MEETING THE CHALLENGES
   OF KNOWLEDGE-BASED SOCIETY}},
Booktitle = {{SGEM 2016, BK 1: PSYCHOLOGY AND PSYCHIATRY, SOCIOLOGY AND HEALTHCARE,
   EDUCATION CONFERENCE PROCEEDINGS, VOL III}},
