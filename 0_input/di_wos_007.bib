@article{ ISI:000374027500003,
Author = {Szabo, Zsuzsanna Katalin and Chiriac, Lucian},
Title = {{INVESTIGATIONS CONCERNING E-GOVERNMENT ADOPTION IN TRANSITION ECONOMIES}},
Journal = {{ACTA OECONOMICA}},
