@inproceedings{ ISI:000402738402012,
Author = {Eppard, Jenny and Dolci, Ida and Reddy, Preeya and Wohler, Omar and
   Johns, Rick and Mcgladdery, Andrew and Rochdi, Aicha and Sisak, Cheryl},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{FLIP YOUR WAY TO LEARNER INDEPENDENCE USING THE ``DOUBLE FLIP TIP{''}}},
Booktitle = {{INTED2016: 10TH INTERNATIONAL TECHNOLOGY, EDUCATION AND DEVELOPMENT
   CONFERENCE}},
Series = {{INTED Proceedings}},
Year = {{2016}},
Pages = {{2008-2016}},
Note = {{10th International Technology, Education and Development Conference
   (INTED), Valencia, SPAIN, MAR 07-09, 2016}},
Abstract = {{In this fast-paced and ever changing age of digital technologies, making
   use of learning opportunities and engaging students to take
   responsibility for their own learning with technology is now at the
   forefront of learner training and independence. ``Flipped learning{''}
   is an innovative approach to teaching that is increasingly being
   implemented in many different educational institutions. In our current
   world of ubiquitous digital technologies, gone are the days where
   traditional teaching methods alone benefit the students.
   This co-presentation, ``The Double Flip Tip{''} will take a look at how
   ``flipping{''} a classroom, using digital tools such as VideoScribe, QR
   (Quick Response) codes and Bookwidgets will inspire students to easily
   access their materials and study/review language items at their own pace
   on their personal mobile devices. This will take place outside the
   normal classroom set-up, with no/little assistance from teachers.
   Subsequently, instructors will become facilitators, step aside and allow
   students to learn from their peers thus taking a more active and
   independent role in their development.
   In addition, the session intends to showcase our pilot study findings
   and illustrate ways to implement the ``flipped{''} concept into any
   classroom environment, thus focusing on the learners and their
   individual needs. Participants will leave the session with a better
   understanding of best practices that facilitate the implementation of
   this approach and will leave equipped with a toolbox of tips to help
   them ``Double Flip'' their own classes.}},
