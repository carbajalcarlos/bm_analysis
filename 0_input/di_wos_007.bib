@inproceedings{ ISI:000402063700051,
Author = {Caetano, Ines and Leitao, Antonio},
Editor = {{Herneoja, A and Osterlund, T and Markkanen, P}},
Title = {{DrAFT: an Algorithmic Framework for Facade Design}},
Booktitle = {{ECAADE 2016: COMPLEXITY \& SIMPLICITY, VOL 1}},
