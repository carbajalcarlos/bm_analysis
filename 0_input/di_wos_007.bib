@inproceedings{ ISI:000393253800084,
Author = {Hraskova, Dagmar and Seemann, Peter},
Editor = {{Kliestik, T}},
Title = {{INNOVATIVE FORMS OF ONLINE LEARNING WITHIN THE EDUCATION FIELD}},
Booktitle = {{GLOBALIZATION AND ITS SOCIO-ECONOMIC CONSEQUENCES, 16TH INTERNATIONAL
