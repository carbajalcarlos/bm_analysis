@incollection{ ISI:000403236300016,
Author = {Horne, Daniel R.},
Editor = {{Gudic, M and Parkes, C and Rosenbloom, A}},
Title = {{Financial exclusion Technology, the digital divide and poverty}},
Booktitle = {{RESPONSIBLE MANAGEMENT EDUCATION AND THE CHALLENGE OF POVERTY: A
   TEACHING PERSPECTIVE}},
Year = {{2016}},
Pages = {{208-221}},
Abstract = {{Many believe that technology will be the key to poverty eradication, and
   some high-profile individuals have gone as far as predicting that
   poverty will be eliminated in the very near future. However, some
   evidence suggests that certain groups that have limited access or
   limited ability to use new information technology face the threat of
   being left further behind by the rapid pace of technological
   innovation-hence exacerbating rather than alleviating poverty
   conditions. This digital underclass will potentially become poorer even
   as those around them enjoy increasing standards of living. This chapter
   focuses on the changes in payment technology and roadblocks that inhibit
   those who need such benefits. Suggestions are offered about how
   governmental authorities, NGOs and business can work together to
   overcome these inhibitors. The case of M-Pesa in Kenya is used to
   illustrate the successful implementation of a payments platform that is
   both effective and inclusive.}},
