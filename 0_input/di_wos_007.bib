Title = {{Measuring the Effect of 3D Printing Machinery on Technology Management}},
Booktitle = {{PORTLAND INTERNATIONAL CONFERENCE ON MANAGEMENT OF ENGINEERING AND
   TECHNOLOGY (PICMET 2016): TECHNOLOGY MANAGEMENT FOR SOCIAL INNOVATION}},
