@inproceedings{ ISI:000402738400036,
Author = {Hopkins, Susan},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{INCARCERATED STUDENTS AND DIGITAL UNIVERSITIES: OVERCOMING OBSTACLES AND
   CONSTRAINTS TO HIGHER EDUCATION PARTICIPATION IN AUSTRALIAN PRISONS}},
