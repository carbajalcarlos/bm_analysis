@inproceedings{ ISI:000406035700031,
Author = {Mangina, Eleni and O'Keeffe, Evan and Eyerman, Joe and Goodman, Lizbeth},
Editor = {{Thwaites, H and Lun, LS and Addison, A}},
Title = {{Drones for Live Streaming of Visuals for People with Limited Mobility}},
Booktitle = {{PROCEEDINGS OF THE 2016 INTERNATIONAL CONFERENCE ON VIRTUAL SYSTEMS AND
   MULTIMEDIA (VSMM)}},
Series = {{Proceedings of the International Conference on Virtual Systems \&
   Multimedia (VSMM)}},
