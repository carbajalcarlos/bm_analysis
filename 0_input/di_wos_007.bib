@inproceedings{ ISI:000402955903149,
Author = {Sergio Artal-Sevil, J. and Bernal, J. L. and Dominguez, J. A.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{FLIPPED CLASSROOM: AN INTERACTIVE METHOD TO IMPROVE THE STUDENT
   PERFORMANCE}},
