@inproceedings{ ISI:000402955909012,
Author = {Yap, Kevin Yi-Lwern and Yap, Kai Zhen and Ghani, Mazimran Yusoff Bin
   Abdol and Yap, John Yin Gwee and Athreya, Uday S.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{``PLANTS VS ZOMBIES{''} - STUDENTS' EXPERIENCES WITH AN IN-HOUSE
   PHARMACY SERIOUS GAME}},
Booktitle = {{EDULEARN16: 8TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
