@inproceedings{ ISI:000411576804033,
Author = {Li, Rere and Shi, Li and Li, Song},
Book-Group-Author = {{IEEE}},
Title = {{A Process of High Efficiency and Low Redundancy in Content Distribution
   based on Named Data Networking in VANETs}},
Booktitle = {{2016 2ND IEEE INTERNATIONAL CONFERENCE ON COMPUTER AND COMMUNICATIONS
   (ICCC)}},
Year = {{2016}},
Pages = {{2490-2494}},
Note = {{2nd IEEE International Conference on Computer and Communications (ICCC),
   Chengdu, PEOPLES R CHINA, OCT 14-17, 2016}},
Organization = {{IEEE}},
Abstract = {{With the rapid increasing in applications such as digital media, social
   network, and e-commerce applications, the current dominant function of
   Internet has changed from end-to-end message delivery to large-scale
   content distribution. It has always been an important challenge in
   Vehicular Ad Hoc Networks (VANETs) to apply the content distribution
   mechanism. In paper {[}1], a simple inter-vehicle one-hop large-size
   content distribution mechanism was proposed which applies the emerging
   Named Data Networking (NDN) concept to vehicle-to-vehicle (V2V)
   communications. This method improves the performance of distribution
   efficiency to a certain extent, but it also generates a large amount of
   redundant Data while distributing content. This paper proposes two
   innovative mechanisms which include adaptive switching and piecewise
   random walk method by redesigning the process of content distribution.
   Finally simulation results from ndnSIM network simulator show that the
   redesigned mechanism not only improves the transportation efficiency,
   but also reduces the redundancy of Data packets.}},
