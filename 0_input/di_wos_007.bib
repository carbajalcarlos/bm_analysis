@inproceedings{ ISI:000392696700016,
Author = {Signori, P. and Flint, D. J.},
Editor = {{RussoSpena, T and Mele, C}},
Title = {{DIGITAL MARKETING INNOVATIONS AND THEIR ROLE IN SERVICE ECOSYSTEMS, THE
   EXCHANGE OF VALUE AND SOCIAL IMPACT}},
Booktitle = {{WHAT'S AHEAD IN SERVICE RESEARCH?: NEW PERSPECTIVES FOR BUSINESS AND
   SOCIETY}},
Year = {{2016}},
Pages = {{259-273}},
Note = {{26th Annual Conference of the
   European-Association-for-Research-on-Services (RESAR), Naples, ITALY,
   SEP 08-10, 2016}},
Organization = {{European Assoc Res Serv; Univ Napoli Federico II}},
Abstract = {{This paper presents a discussion of results from an ongoing project,
   specifically providing an in-depth case study of a digital marketing
   innovator playing an important role for online brand engagement within
   its service ecosystem and how they are helping to facilitate the
   exchange of value and have a positive social impact. In particular,
   examples of focal company's digital marketing innovations are examined
   to understand: what roles various actors are playing, how the ecosystem
