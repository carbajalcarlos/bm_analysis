@inproceedings{ ISI:000388114600083,
Author = {Hajmsek, Irena and Busche, Thomas},
Book-Group-Author = {{IEEE}},
Title = {{TANDEM-X: SCIENCE ACTIVITIES}},
Booktitle = {{2016 IEEE INTERNATIONAL GEOSCIENCE AND REMOTE SENSING SYMPOSIUM (IGARSS)}},
