Author = {Uhlirova, Martina},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{BARRIERS TO THE SUCCESSFUL DIGITAL TECHNOLOGIES IMPLEMENTATION IN
   PRIMARY MATHEMATICS EDUCATION}},
