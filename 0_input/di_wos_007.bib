@inproceedings{ ISI:000402955901092,
Author = {Jeladze, Eka and Pata, Kai},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{DIGITALLY ENHANCED SCHOOLS AND SERVICE-BASED LEARNING ECOSYSTEM}},
Booktitle = {{EDULEARN16: 8TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
Series = {{EDULEARN Proceedings}},
Year = {{2016}},
Pages = {{1569-1578}},
Note = {{8th International Conference on Education and New Learning Technologies
   (EDULEARN), Barcelona, SPAIN, JUL 04-06, 2016}},
Abstract = {{This study explored the schools as learning ecosystems of services to
   understand how digitally enhanced schools differ and what ICT-boosted
   services in the whole service ecosystem distinguish innovative and less
   innovative schools. We developed the grid to differentiate services in
   school learning ecosystems into the levels of internal, external and
   trade-off services within 3 big domains: digital infrastructure,
   learning facilitation, and change management. The study used mixed
   methods research design. Qualitative analysis of case studies (n= 15
   schools) in Georgia was used to identify services in learning
   ecosystems. The data in each school was then quantitatively mapped to
   the grid using the binary coding. K-means clustering revealed 2 groups
   of the schools. The services differentiating the more and less
   innovative schools were identified with Mann-Whitney test. The two
   learning ecosystem types differed based on the internal and trade-off
   services. The biggest difference in services was found in the change
   management domain: ICT strategic planning, incentive schemes and teacher
   support system differed significantly in more and less innovative
   learning ecosystems.}},
