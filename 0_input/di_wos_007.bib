@inproceedings{ ISI:000412571000014,
Author = {Liberati, Nicola},
Editor = {{Gadducci, F and Tavosanis, M}},
Title = {{From Information to Perception The New Design of the Emerging Computer
   Technologies and Its Effects}},
Booktitle = {{HISTORY AND PHILOSOPHY OF COMPUTING}},
