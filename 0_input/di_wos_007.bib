@inproceedings{ ISI:000401525100026,
Author = {Marliani, Reni Sri and Bandung, Yoanes},
Book-Group-Author = {{IEEE}},
Title = {{Measuring E-Learning Readiness in Metrology Training Center Ministry of
   Trade of Indonesia}},
Booktitle = {{PROCEEDINGS OF 2016 INTERNATIONAL CONFERENCE ON INFORMATION TECHNOLOGY
   SYSTEMS AND INNOVATION (ICITSI)}},
Year = {{2016}},
Note = {{3rd International Conference on Information Technology Systems and
   Innovation (ICITSI), Inst Teknologi Bandung, Sch Elect Engn \& Informat,
   Bandung, INDONESIA, OCT 24-27, 2016}},
Organization = {{IEEE Joint Chapter Educ Soc Elect Devices Soc Power Elect Soc Signal
   Proc Soc; IEEE Joint Chapter Control Syst Soc Robot \& Automat Soc}},
Abstract = {{Many government institutions in Indonesia failed in adopting E-learning
   due to lack of assessment and measuring their E-learning readiness. The
   cause of failure was not come from technology sector because Indonesia
   is the sixth largest Internet user in the world, but due to unreadiness
   in human resources and learning systems that were implemented. It is
   substantial to use an appropriate E-learning readiness measurement model
   so that we can ascertain our institution's capability in implementing
   digital learning. The research used Aydin \& Tasci model as a tool to
   conduct an E-learning readiness survey on 115 respondents (Teachers and
   Students) in 17 province all over Indonesia. We compare the score of
   this survey with Aydin \& Tasci Level of E-learning Readiness. The
   result of this research exhibit 4.09 as mean score of E-learning
   readiness level, it signify readiness of Development Center of
   Metrological Resources, Ministry of Trade of Indonesia to implement
   digital learning even though still needs some improvements. Improvement
   should primarily be done on the innovation factors which had the lowest
   score. Media and learning materials enrichment might become our chance
   to improve E-learning readiness score. That enrichment requires a futher
   research in designing a learning system that can accomodate learning
   media and material tranformation, either it is a full learning system or
   blended learning.}},
