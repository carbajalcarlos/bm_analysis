@inproceedings{ ISI:000406220800079,
Author = {Malec, Christopher and Semwal, Sudhanshu Kumar},
Editor = {{Smari, WW}},
Title = {{Enriching Play Experience across Multiple Platforms through
   Collaborative Real World Actions}},
