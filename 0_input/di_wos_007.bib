@inproceedings{ ISI:000402955904020,
Author = {Sleiman, Tina and Ajeenah, Lama},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{ARABIC TYPOGRAPHY IN THE CURRICULUM: PERSPECTIVES FROM THE MIDDLE EAST}},
Booktitle = {{EDULEARN16: 8TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
