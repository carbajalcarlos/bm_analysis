@inproceedings{ ISI:000406057400049,
Author = {Yu, Xianghao and Zhang, Jun and Letaief, Khaled B.},
Editor = {{Matthews, MB}},
Title = {{Alternating Minimization for Hybrid Precoding in Multiuser OFDM mmWave
   Systems}},
Booktitle = {{2016 50TH ASILOMAR CONFERENCE ON SIGNALS, SYSTEMS AND COMPUTERS}},
