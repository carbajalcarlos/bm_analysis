@inproceedings{ ISI:000398053800005,
Author = {Landherr, Martin and Schneider, Ulrich and Bauernhansl, Thomas},
Editor = {{Westkamper, E and Bauernhansl, T}},
Title = {{The Application Center Industrie 4.0-Industry-driven manufacturing,
   research and development}},
Booktitle = {{FACTORIES OF THE FUTURE IN THE DIGITAL ENVIRONMENT}},
Series = {{Procedia CIRP}},
