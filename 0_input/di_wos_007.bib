@article{ ISI:000393430800001,
Author = {Cuesta Cambra, Ubaldo and Manas Viniegra, Luis},
Title = {{Integration of immersive virtual reality in Communication Degrees}},
Journal = {{REVISTA ICONO 14-REVISTA CIENTIFICA DE COMUNICACION Y TECNOLOGIAS}},
Year = {{2016}},
Volume = {{14}},
Number = {{2}},
Abstract = {{The European Higher Education Area promotes the integration of new
   technologies in didactic innovation and it aims to improve skills. It
   has been requested by students at the Complutense University of Madrid,
   who have a digital native profile or millennial. This article is a study
   about implementation of immersive virtual reality in the practical part
   of the subjects related to business communication. Specifically, it
