@inproceedings{ ISI:000402955907055,
Author = {Fondevila-Gascon, Joan-Francesc and Casas, Joaquim and Mir-Bernal, Pedro
   and Santana-Lopez, Eva and Rom-Rodriguez, Josep and Feliu-Roe, Lluis},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{EVALUATION OF VOICE THROUGH DIGITAL MEDIA: IMPACT ON EDUCATION AND
   PERFORMANCES}},
Booktitle = {{EDULEARN16: 8TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
