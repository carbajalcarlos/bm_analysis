Title = {{A Minimax Framework for Gender Classification Based on Small-Sized
   Datasets}},
Booktitle = {{ADVANCED CONCEPTS FOR INTELLIGENT VISION SYSTEMS, ACIVS 2015}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2015}},
Volume = {{9386}},
