@article{ ISI:000367593900005,
Author = {Vicent, Naiara and Rivero Gracia, Ma Pilar and Feliu Torruella, Maria},
Title = {{Archeology and digital technologies in Heritage Education}},
Journal = {{EDUCATIO SIGLO XXI}},
Year = {{2015}},
Volume = {{33}},
Number = {{1}},
Pages = {{83-102}},
Abstract = {{This paper analyses the current trends regarding the use of digital
   technologies in archeology and heritage education. The study stems from
   a theoretical analysis of the educational potential of archeology which
   is channeled later into the didactic use of digital models
   reconstruction and interpretation of archaeological objects and spaces.
   This research establishes three basic lines of educational innovation:
   immersive virtual reality models, social interaction web 2.0
   environments, and mobile learning. It also highlights the evaluation of
   heritage projects and materials based on digital technologies as an
   emerging interest in this field of research. The article is the result
   of original research conducted by the authors and supported by
   competitive research projects, but it also refers to other projects and
   experiences.}},
