@article{ ISI:000378412900003,
Author = {Zhou Weihuan and Arner, Douglas W. and Buckley, Ross P.},
Title = {{REGULATION OF DIGITAL FINANCIAL SERVICES IN CHINA: LAST MOVER ADVANTAGE?}},
