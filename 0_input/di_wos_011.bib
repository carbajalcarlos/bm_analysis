@inproceedings{ ISI:000377304002042,
Author = {Medina Vidal, Fernando and Hernndez Gomez, Elena},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{EMOTIONAL SKILLS BY USING ICT}},
Booktitle = {{ICERI2015: 8TH INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND
