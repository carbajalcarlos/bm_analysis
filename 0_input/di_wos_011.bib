@inproceedings{ ISI:000359443600114,
Author = {Hu, Yanfang and Li, Jingbing and Huang, Mengxing},
Editor = {{Chang, L and Guiran, C and Zhen, L}},
Title = {{A Robust Watermarking of Medical Image Based on 3D-DFT and Arnold
   Scrambling In Compressed Domain}},
Booktitle = {{PROCEEDINGS OF THE 2015 INTERNATIONAL CONFERENCE ON MECHATRONICS,
   ELECTRONIC, INDUSTRIAL AND CONTROL ENGINEERING}},
Series = {{AER-Advances in Engineering Research}},
