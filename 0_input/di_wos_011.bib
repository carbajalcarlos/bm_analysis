@inproceedings{ ISI:000380512200064,
Author = {Eksi, Gonca and Yakisik, Burcak Yilmaz},
Editor = {{Mirici, IH and Erten, IH and Oz, H}},
Title = {{An investigation of prospective English language teachers' multimodal
   literacy}},
