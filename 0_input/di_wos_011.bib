@inproceedings{ ISI:000376685707048,
Author = {Dijkstra, P. and Pirlot, J. and Velichkov, D. and Parseliunas, E. and
   Wijngaarde, M. and Donert, K. and Frigne, D. and Westerbeek, H. and
   Levoleger, K. and Popovas, D. and Obuchovski, R. and Slikas, D. and
   Buga, A.},
Editor = {{GomezChova, L and LopezMartinez, A and CandelTorres, I}},
Title = {{PROMOTING OF THE GEOSPATIAL SKILLS: FIRST RESULTS OF THE GEOSKILLS PLUS
   PROJECT OF THE LEONARDO DA VINCI PROGRAMME}},
Booktitle = {{EDULEARN15: 7TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
