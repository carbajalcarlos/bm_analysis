@inproceedings{ ISI:000366264104033,
Author = {Svahn, Fredrik and Lindgren, Rikard and Mathiassen, Lars},
Editor = {{Bui, TX and Sprague, RH}},
Title = {{Applying Options Thinking to Shape Generativity in Digital Innovation:
   An Action Research into Connected Cars}},
Booktitle = {{2015 48TH HAWAII INTERNATIONAL CONFERENCE ON SYSTEM SCIENCES (HICSS)}},
Series = {{Proceedings of the Annual Hawaii International Conference on System
   Sciences}},
Year = {{2015}},
Pages = {{4141-4150}},
