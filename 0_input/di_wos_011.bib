Editor = {{Bui, TX and Sprague, RH}},
Title = {{Offshoring Digital Work, But Not Physical Output: Differential Access to
   Task Objects and Coordination in Globally Distributed Automotive
   Engineering and Graphic Design Work}},
Booktitle = {{2015 48TH HAWAII INTERNATIONAL CONFERENCE ON SYSTEM SCIENCES (HICSS)}},
Series = {{Proceedings of the Annual Hawaii International Conference on System
   Sciences}},
Year = {{2015}},
