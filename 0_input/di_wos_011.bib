@inproceedings{ ISI:000377304005078,
Author = {Luojus, Satu and Kauppinen, Sami and Lahti, Janne},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{INTEGRATING TEACHING AND R\&D IN HIGHER EDUCATION - THE WELIVE PROJECT}},
