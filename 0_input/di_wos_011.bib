@inproceedings{ ISI:000376685707092,
Author = {Muhtaseb, Rami},
Editor = {{GomezChova, L and LopezMartinez, A and CandelTorres, I}},
Title = {{EXPLORING THE USE OF DIGITAL TECHNOLOGIES IN A SAMPLE OF SCIENTIFIC AND
   EDUCATIONAL INSTITUTIONS}},
