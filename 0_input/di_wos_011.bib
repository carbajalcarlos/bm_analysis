@inproceedings{ ISI:000376685704071,
Author = {Serafin, Eduardo and Wetton, Mark},
Editor = {{GomezChova, L and LopezMartinez, A and CandelTorres, I}},
Title = {{ADAPTING AROUND TWO DIFFERING PEDAGOGIES - A COMPARATIVE CASE STUDY}},
Booktitle = {{EDULEARN15: 7TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
