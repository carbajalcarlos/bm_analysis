@inproceedings{ ISI:000371976400046,
Author = {Misfeldt, Morten and Gjedde, Lisa},
Editor = {{Munkvold, R and Kolas, L}},
Title = {{What is a Game for Geometry Teaching: Creative, Embodied and Immersive
   Aspects}},
Booktitle = {{PROCEEDINGS OF THE 9TH EUROPEAN CONFERENCE ON GAMES BASED LEARNING
   (ECGBL 2015)}},
