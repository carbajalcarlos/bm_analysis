Editor = {{Yao, ZL and Chen, Y}},
Title = {{Research on the Development of Intangible Cultural Heritage from the
   Network Perspective}},
Booktitle = {{PROCEEDINGS OF THE 2015 INTERNATIONAL CONFERENCE ON ECONOMICS, SOCIAL
   SCIENCE, ARTS, EDUCATION AND MANAGEMENT ENGINEERING}},
