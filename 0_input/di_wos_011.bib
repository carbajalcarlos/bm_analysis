@article{ ISI:000357286800012,
Author = {Tuccimei, P. and Mollo, S. and Soligo, M. and Scarlato, P. and
   Castelluccio, M.},
Title = {{Real-time setup to measure radon emission during rock deformation:
   implications for geochemical surveillance}},
