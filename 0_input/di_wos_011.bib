Title = {{Application of Blast Furnace Falling Materials Trajectories Measurement
   Using Laser Scan Technology}},
Booktitle = {{2015 IEEE INTERNATIONAL CONFERENCE ON CONSUMER ELECTRONICS - TAIWAN
   (ICCE-TW)}},
Series = {{International Conference on Consumer Electronics}},
Year = {{2015}},
Pages = {{322-323}},
