@article{ ISI:000363307800005,
Author = {Chou, Wen Huei and Chen, Shih-Nung and Ni, Jia Yang and Lin, Jia-Sian
   and Guo, Sheng Ming},
Title = {{Media mediocracy: live archiving as an intervention in preserving
   cultural heritage}},
