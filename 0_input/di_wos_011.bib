@inproceedings{ ISI:000357948700023,
Author = {Re, C. and Simioni, E. and Cremonese, G. and Roncella, R. and Forlani,
   G. and Da Deppo, Vania and Naletto, G. and Salemi, G.},
Editor = {{Remondino, F and Shortis, MR}},
Title = {{DTM generation from STC-SIMBIO-SYS images}},
Booktitle = {{VIDEOMETRICS, RANGE IMAGING, AND APPLICATIONS XIII}},
Series = {{Proceedings of SPIE}},
