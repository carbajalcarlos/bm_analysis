@inproceedings{ ISI:000371976400024,
Author = {Freina, Laura and Canessa, Andrea},
Editor = {{Munkvold, R and Kolas, L}},
Title = {{Immersive vs Desktop Virtual Reality in Game Based Learning}},
Booktitle = {{PROCEEDINGS OF THE 9TH EUROPEAN CONFERENCE ON GAMES BASED LEARNING
   (ECGBL 2015)}},
Series = {{Proceedings of the European Conference on Games-Based Learning}},
