Editor = {{Plinski, EF}},
Title = {{Digital image color printing on plastics using a slab, RF excited CO2
   laser}},
Booktitle = {{XIV INTERNATIONAL SYMPOSIUM ON GAS FLOW, CHEMICAL LASERS, AND HIGH-POWER
   LASERS}},
