@article{ ISI:000179732400003,
Author = {Hanmandlu, M and Mohan, KRM and Chakraborty, S and Goyal, S and
   Choudhury, DR},
Title = {{Unconstrained handwritten character recognition based on fuzzy logic}},
Journal = {{PATTERN RECOGNITION}},
Year = {{2003}},
Volume = {{36}},
