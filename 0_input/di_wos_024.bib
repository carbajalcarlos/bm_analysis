@inproceedings{ ISI:000182087700007,
Author = {Crawford, K},
Editor = {{Marshall, G and Katz, Y}},
Title = {{Cooperative networks enable shared knowledge - Rapid dissemination of
   innovative ideas and digital culture}},
Booktitle = {{LEARNING IN SCHOOL, HOME AND COMMUNITY: ICT FOR EARLY AND ELEMENTARY
   EDUCATION}},
Series = {{INTERNATIONAL FEDERATION FOR INFORMATION PROCESSING}},
Year = {{2003}},
Volume = {{113}},
