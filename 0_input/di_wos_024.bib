@article{ ISI:000224081900008,
Author = {Chochliouros, IP and Spiliopoulou-Chochliourou, AS and Lalopoulos, GK},
Title = {{Radio local area networks (R-LANs) - An enabler of the European
   broadband perspective}},
Journal = {{JOURNAL OF THE COMMUNICATIONS NETWORK}},
Year = {{2004}},
Volume = {{3}},
Number = {{1}},
Pages = {{59-65}},
Month = {{JAN-MAR}},
Abstract = {{Radio local area networks (R-LANs, WLANs or Wi-Fi) provide a
   fast-developing and promising means for effective broadband solutions,
   especially within the context of the current European initiatives for
   the promotion of innovative electronic communications infrastructures
   and services in a knowledge-based economy. These networks, using
   inexpensive and easy-to-use customer equipment and often employing
   `free' unlicensed radio spectrum, can open the door to wireless
   broadband Internet access for the mass market (business and
   residential), and will be complementary to other existing means. The
   particular role of R-LANs as an enabler of the broadband options in the
   European Union is investigated. Furthermore, an overview of the main
   technical features of the underlying technologies is given, along with
   an evaluation of some major issues related to the effective use of the
   radio spectrum, especially for the successful penetration of new
   services. In addition various perspectives of market development are
   investigated, which could be essential for the future of the European
   (and of the global) telecommunications industry sector, in particular to
   draw the full benefits of digital technologies and the Internet in terms
   of sustainable growth, increased productivity and competitiveness,
   creation of business and social progress.}},
