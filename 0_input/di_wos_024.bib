@article{ ISI:000182887300011,
Author = {Ranky, PG},
Title = {{A 3D web-enabled, case-based learning architecture and knowledge
   documentation method for engineering, information technology,
   management, and medical science/biomedical engineering}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTER INTEGRATED MANUFACTURING}},
Year = {{2003}},
Volume = {{16}},
Number = {{4-5}},
