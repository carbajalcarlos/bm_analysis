@article{ ISI:000220899700004,
Author = {Dutton, WH and Gillett, SE and McKnight, LW and Peltu, M},
Title = {{Bridging broadband Internet divides: reconfiguring access to enhance
   communicative power}},
Journal = {{JOURNAL OF INFORMATION TECHNOLOGY}},
Year = {{2004}},
Volume = {{19}},
Number = {{1}},
Pages = {{28-38}},
Month = {{MAR}},
Abstract = {{Government and industry initiatives to stimulate the diffusion of
   high-performance broadband telecommunications links have given a fresh
   impetus to debates over the social and economic implications of the
   growing use of the Internet and other information and communication
   technologies (ICTs). This paper analyses how outcomes tied to ICT
   innovation are shaped by choices about whether and how to use, or not
   use, the technology to reconfigure access to people, services,
   information and technologies in ways that significantly change the
   communicative power of individuals, communities, organisations, nations
   and regions. It explains why these outcomes are not predetermined by the
   technology, but unfold over time through the complex interplay among
   many actors, in many arenas. A framework is presented to assist in
   addressing the issue of digital divides and other areas of research,
   policy and practice affected by the design and use of broadband Internet
   and related ICTs.}},
