@inproceedings{ ISI:000239453500035,
Author = {Berger, Ulrich and Lepratti, Raffaello and Otte, Henning},
Editor = {{Horvath, I and Xirouchakis, P}},
Title = {{Application of Digital Human Modelling concepts for automotive
   production}},
Booktitle = {{TOOLS AND METHODS OF COMPETITIVE ENGINEERING Vols 1 and 2}},
Year = {{2004}},
Pages = {{365-373}},
Note = {{5th International Symposium on Tools and Methods of Competitive
   Engineering, Lausanne, SWITZERLAND, APR 13-17, 2004}},
Abstract = {{In this paper concepts and results in Digital Human Modelling within the
   automotive enterprise are described. It will be shown, how targets like
   reduction of health based lack, selection and best-fit practises for
   tools and supporting elements, i.e. jigs or fixtures, as well as
   logistical devices integration and adaptation on ergonomic view can be
   easier achieved within innovative digital solutions. Furthermore, the
   integration of such technologies leads also to relevant improvements in
   optimising and balancing production processes and in increasing
   productivity and product-assembly. Within the methods described in this
   paper an opportunity for rectifying enterprise-intern ergonomic
   regulations in planning assembly tasks will be offered. Exemplary
   shop-floor personal activities within the automotive production have
   been analysed and evaluated according to international standards, like
   RULA, in order to recognize hazardous statically and dynamically
   postures. The obtained results were compared with the specific
   automotive enterprise requirements, in order to underlie
   con-/divergences. The first 3D simulation tests have shown high
   performance and adaptability motivating, thus, to investigate further
   exemplar assembly activities in order to eventually transfer the
   achieved results to the real assembly plants.}},
