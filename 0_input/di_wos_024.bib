@article{ ISI:000225851000002,
Author = {Chochliouros, IP and Spiliopoulou-Chochliourou, A and Lalopoulos, GK},
Title = {{Dynamic value constellation for the creation of innovative services via
   the exploitation of interactive digital television infrastructure}},
Journal = {{JOURNAL OF THE COMMUNICATIONS NETWORK}},
Year = {{2004}},
Volume = {{3}},
Number = {{3}},
Pages = {{6-10}},
Month = {{JUL-SEP}},
Note = {{43rd European Telecommunications Congress, Ghent, BELGIUM, SEP   08,
   2004}},
Abstract = {{This paper provides essential information related to the various results
   and the multiple perspectives of the GEMINI IST Project. It discusses
   the development, the deployment and the use of an integrated mediation
   platform, within the context of digital television (DTV), able to
   support modern applications and innovative services, towards promoting
   an effective convergence effect between telecommunications, broadcasting
   and information technology (IT). The system solution transforms the
   traditional value chain of the media sector and introduces a new
   mediator role that will coordinate the whole market by reducing the
   current `gap' between demand and supply forces. It promotes a new
   framework to sufficiently support the dynamic value constellation form
   or organisation. We also discuss the system architecture as well as the
   functional components and their particular role and functionalities. In
   addition, we investigate the arising opportunities for the provision of
   four different interactive multimedia services (that is,
   chatting/messaging, voting, alerting and content recommendations)
   developed according to the global Multimedia Home Platform (MHP)
   standard in parallel with the major original options promoted by the
   system itself. Furthermore, we evaluate possible business benefits for
   various categories of market players involved, and conclude with
   estimations for further future development.}},
