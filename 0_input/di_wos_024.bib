@article{ ISI:000185419800015,
Author = {Di Nucci, C and Fort, A and Rocchi, S and Tondi, L and Vignoli, V and Di
   Francesco, F and Santos, MBS},
Title = {{A measurement system for odor classification based on the dynamic
   response of-OCM sensors}},
