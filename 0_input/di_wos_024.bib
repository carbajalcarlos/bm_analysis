@inproceedings{ ISI:000189471800005,
Author = {Alves, N and Bartolo, P},
Editor = {{Anumba, CJ}},
Title = {{Human vision mimicry for design}},
Booktitle = {{INNOVATIVE DEVELOPMENTS IN ARCHITECTURE, ENGINEERING AND CONTRUCTION}},
