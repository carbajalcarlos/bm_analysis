@article{ ISI:000174983600005,
Author = {Toth, E},
Title = {{Innovative solutions in automatic classification: A brief summary}},
Journal = {{LIBRI}},
Year = {{2002}},
Volume = {{52}},
Number = {{1}},
Pages = {{48-53}},
Month = {{MAR}},
Abstract = {{There is a growing need for practical solutions to provide flexible
   access to digital documents in a structured form on the Web. The
   existing library classification schemes serve as good bases for
   achieving this goal. This paper presents a brief review of the various
   methods applied in automatic classification. It focuses on the main
