@article{ ISI:000177669700009,
Author = {Deshpande, HV and Cheng, BH and Woo, JCS},
Title = {{Channel engineering for analog device design in deep submicron CMOS
   technology for system on chip applications}},
Journal = {{IEEE TRANSACTIONS ON ELECTRON DEVICES}},
Year = {{2002}},
Volume = {{49}},
Number = {{9}},
Pages = {{1558-1565}},
Month = {{SEP}},
Abstract = {{Scaling of analog CMOS in deep submicron regime is challenging,
   particularly for mixed mode system on chip applications due to the
   tradeoff in design requirements for analog and digital applications. The
   conventional approach employing aggressive gate oxide and S/D junction
   scaling to suppress the two-dimensional (2-D) electrostatic coupling and
   related short channel effects that degrade the device behavior in deep
   submicron regime, though, improves the digital performance. However,
   this approach is not sufficient to get reasonable analog performance.
   This paper presents comprehensive study on the analog performance of
   scaled MOSFETs and explores alternative ways for improving analog
   performance of these devices. It is shown that easily integrable
   innovative channel engineering scheme in the form of single pocket
   structures can be used in the standard logic CMOS process to
   significantly improve the device analog performance of the deep
   submicron devices.}},
