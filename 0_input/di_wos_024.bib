@inproceedings{ ISI:000227983000035,
Author = {Knight, MT and Rogers, C},
Editor = {{Chu, HW and Savoie, M and Toraichi, K and Kwan, P}},
Title = {{Gather round the ROBOTable: Imoroving distance learning through
   innovative interaction technology}},
Booktitle = {{International Conference on Computing, Communications and Control
   Technologies, Vol 3, Proceedings}},
Year = {{2004}},
Pages = {{185-190}},
Note = {{International Conference on Computing, Communications and Control
   Technologies (CCCT 2004), Austin, TX, AUG 14-17, 2004}},
Organization = {{Univ Texas Austin; Int Inst Informat \& System; IEEE Comp Soc, Venezuela
   Chapter; Inter Amer Org Higher Educ}},
Abstract = {{Distance learning has been revolutionized by the incorporation of
   internet-based communication. Unfortunately, most online learning
   environments merely enhance traditional formats, resulting in multimedia
   books viewed through computer monitors with the aid of a keyboard and a
   mouse. Engineers at Tufts University and Lincoln University have
   collaborated to create an environment where remote groups of learners
   can interact through a technology called the ROBOTable. Inspired by the
   research and development of the HI SPACE table, the. ROBOTable consists
   of a piece of frosted glass with a computer screen projected on it, and
   a computer monitor behind it. Users interact by using the digital pen to
   write on the table surface, creating a ``shared whiteboard{''}. Users
   can also use the tab le as a platform for competition or collaboration.
   When a user places an object, such as a LEGO robot, on the table
   surface, a virtual image is created in the other location. The ROBOTable
   becomes a portal that can support group collaboration spanning remote
   locations in a ``hands-on{''} sense. By providing more effective
   communication, we theorize that the table will facilitate a more
   seamless interface between the real world and the virtual world, and a
   more powerful distance learning experience for participants.}},
