@inproceedings{ ISI:000224224100002,
Author = {Sherwin, C},
Editor = {{Bhamra, T and Hon, B}},
Title = {{Critical friendships - `non-Government organizations' (NGOs) as a
   stimulus for sustainable innovation and change}},
Booktitle = {{DESIGN AND MANUFACTURE FOR SUSTAINABLE DEVELOPMENT 2004}},
