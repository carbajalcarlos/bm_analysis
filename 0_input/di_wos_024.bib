@article{ ISI:000222046600004,
Author = {Krysan, M and Couper, MR},
Title = {{Race in the live and the virtual interview: Racial deference, social
   desirability, and activation effects in attitude surveys}},
Journal = {{SOCIAL PSYCHOLOGY QUARTERLY}},
