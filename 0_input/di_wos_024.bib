@inproceedings{ ISI:000229602302127,
Author = {Carlsen, R},
Editor = {{Cantoni, L and McLoughlin, C}},
Title = {{The Online Digital Library: A constructivist tool today's ICT classrooms}},
Booktitle = {{ED-MEDIA 2004: World Conference on Educational Multimedia, Hypermedia \&
   Telecommunications, Vols. 1-7}},
Year = {{2004}},
Pages = {{2341-2344}},
Note = {{World Conference on Educational Multimedia, Hypermedia and
   Telecommunications, Lugano, SWITZERLAND, JUN 21-26, 2004}},
Abstract = {{Some educators enhance their classes by adding Internet materials,
   traditional library resources, or using copies of a new article. While
   such changes may work well, there just may be an even better way to
   enhance one's classes. Recently published proceedings and journal
   articles are available from AACE's Digital Library (AACE'DL) that
   affords students with options more consistent with Internet time.
   McMillan Culp, Honey, and Mandinach (2003) identify the importance of
   educators finding a personal technology policy that is predicated on
   one's personal experimentations, investigations, and experiences. The
   AACE'DL helps do that; it becomes an idea machine for innovation. This
   roundtable will demonstrate how the AACE'DL helps educators and students
   find their professional voices. Educators must become members of an
   educationally savvy computer and communication technology (CCT)
   educational culture. Teachers and students will only move forward with
   CCT when they understand how adequate equipment, re-skilling, and one's
   personal understanding all connect together. Attendees will be provided
   with recent examples of how the AACE'DL was used in graduate educational
   technology classes.}},
