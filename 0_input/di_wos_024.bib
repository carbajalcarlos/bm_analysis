@article{ ISI:000183110000003,
Author = {Price, S and Rogers, Y and Scaife, M and Stanton, D and Neale, H},
Title = {{Using `tangibles' to promote novel forms of playful learning}},
Journal = {{INTERACTING WITH COMPUTERS}},
Year = {{2003}},
Volume = {{15}},
Number = {{2}},
Pages = {{169-185}},
Month = {{APR}},
Abstract = {{Tangibles, in the form of physical artefacts that are electronically
   augmented and enhanced to trigger various digital events to happen, have
   the potential for providing innovative ways for children to play and
   learn, through novel forms of interacting and discovering. They offer,
   in addition, the scope for bringing playfulness back into learning. To
   this end, we designed an adventure game, where pairs of children have to
   discover as much as they can about a virtual imaginary creature called
   the Snark, through collaboratively interacting with a suite of
   tangibles. Underlying the design of the tangibles is a variety of
   transforms, which the children have to understand and reflect upon in
   order to make the Snark come alive and show itself in a variety of
   morphological and synaesthesic forms. The paper also reports on the
   findings of a study of the Snark game and discusses what it means to be
   engrossed in playful learning. (C) 2003 Elsevier Science B.V. All rights
   reserved.}},
