@inproceedings{ ISI:000404892000051,
Author = {Spil, Ton and Pris, Miran and Kijl, Bjorn},
Editor = {{Ndaba, Z and Mokoteli, T}},
Title = {{Exploring the BIG Five of e-Leadership by Developing Digital Strategies
   With Mobile, Cloud, Big Data, Social Media, and the Internet of Things}},
Booktitle = {{PROCEEDINGS OF THE 5TH INTERNATIONAL CONFERENCE ON MANAGEMENT,
   LEADERSHIP AND GOVERNANCE (ICMLG 2017)}},
Series = {{Proceedings of the International Conference on Management Leadership and
