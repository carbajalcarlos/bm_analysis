@article{ ISI:000400762600005,
Author = {Ghouchani, Hamid Tavakoli and Niknami, Shamsedin and Aminshokravi,
   Farkhondeh and Hojjat, Seyd Kaveh},
Title = {{Effectiveness of the video-based education in the retention of addiction
   treatment}},
