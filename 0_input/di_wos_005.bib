@article{ ISI:000392445000007,
Author = {Linares-Palomar, Rafael and Baraybar-Fernandez, Antonio},
Title = {{THE EMPOWERMENT OF THE FILM SPECTATOR: THEATRICAL ON DEMAND IN SPAIN}},
