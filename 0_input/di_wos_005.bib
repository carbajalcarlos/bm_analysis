@article{ ISI:000395240500005,
Author = {Wright, Mark and Stubbs, Mike},
Title = {{Recalibrating the white cube as a hub for social action}},
Journal = {{DIGITAL CREATIVITY}},
Year = {{2016}},
Volume = {{27}},
Number = {{4}},
Pages = {{304-313}},
Month = {{DEC}},
Abstract = {{A new form of practice is developing in which cultural organisations are
   transformed from white cubes to hubs for social action and learning.
   Focus extends from the showing of art to its creation, and the
   empowerment and agency of communities through enactive learning. In this
   model the arts organisation acts as a catalyst for collaborative action
   and enquiry involving academia and a wider ecosystem of communities and
   stakeholders including the public, artists, digital creative industry,
   maker spaces and local government. The new model entails embedding of
   research, innovation and arts practice within the arts organisation
   itself. We illustrate the approach with examples of projects spanning
   mental health, physical disability, young people, veterans, children and
   parents, which have had a real impact on health and well-being of our
   communities.}},
