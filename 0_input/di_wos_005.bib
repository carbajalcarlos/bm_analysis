@inproceedings{ ISI:000390893100001,
Author = {Kauffman, Yashu and Kauffman, Douglas},
Editor = {{Kantola, JI and Barath, T and Nazir, S and Andre, T}},
Title = {{The Impact of Competency-Based Learning and Digital Self-assessment on
   Facilitating Students' Cognitive and Interpersonal Skills}},
Booktitle = {{ADVANCES IN HUMAN FACTORS, BUSINESS MANAGEMENT, TRAINING AND EDUCATION}},
Series = {{Advances in Intelligent Systems and Computing}},
Year = {{2017}},
Volume = {{498}},
Pages = {{3-13}},
Note = {{International Conference on Human Factors, Business Management and
   Society, FL, JUL 27-31, 2016}},
Abstract = {{This paper aims to utilize a mixed-methods assessment for an innovative
   interdisciplinary course, Application Period, in a world-class Russian
   University. In order to examine how the cognitive (competency-based
   learning) and motivational (self-efficacy for interpersonal skills)
   concepts impact students' achievement in engineering education, an
   exploratory sequential design was conducted by firstly collecting
   qualitative data to signify the students' interactive learning process
   during the project-based collaboration and team communication.
   Subsequently, two instruments measuring the students' learning outcomes
   were built based on the previous qualitative data and preliminary
   learning objectives. The suggestions and implications are provided to
