@inproceedings{ ISI:000413668602065,
Author = {Sardinha, L. and Almeida, A. M. P. and Barbas, M. P.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{DIGITAL FUTURE CLASSROOM: THE PHYSICAL SPACE AND THE INCLUSION OF THE
   NEET/REFUGEE POPULATION - CONCEPTUAL AND THEORETICAL FRAMEWORKS AND
   METHODOLOGY}},
Booktitle = {{INTED2017: 11TH INTERNATIONAL TECHNOLOGY, EDUCATION AND DEVELOPMENT
   CONFERENCE}},
Series = {{INTED Proceedings}},
