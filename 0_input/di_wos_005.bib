@inproceedings{ ISI:000411458900186,
Author = {Cao, Yiping and Chen, Wenjing and Zhang, Qican and Liu, Yuankun and Li,
   Dahai and Zhou, Xinzhi and Wei, Jun},
Editor = {{Liu, X and Zhang, XC}},
Title = {{Exploration on the Matching between Optical Comprehensive Design
   Experiment and Washington Accord}},
Booktitle = {{14TH CONFERENCE ON EDUCATION AND TRAINING IN OPTICS AND PHOTONICS (ETOP
   2017)}},
Series = {{Proceedings of SPIE}},
Year = {{2017}},
Volume = {{10452}},
