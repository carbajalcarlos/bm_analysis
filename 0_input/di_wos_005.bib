@inproceedings{ ISI:000410252702024,
Author = {Jipa, Gabriel},
Editor = {{Soliman, KS}},
Title = {{Employee Satisfaction - IT User Experience Evaluation in Banking: A Case
   Study}},
Booktitle = {{SUSTAINABLE ECONOMIC GROWTH, EDUCATION EXCELLENCE, AND INNOVATION
