@inproceedings{ ISI:000411195100024,
Author = {Lima, George and Bate, Iain},
Editor = {{Parmer, G}},
Title = {{Valid Application of EVT in Timing Analysis by Randomising Execution
   Time Measurements}},
Booktitle = {{PROCEEDINGS OF THE 23RD IEEE REAL-TIME AND EMBEDDED TECHNOLOGY AND
   APPLICATIONS SYMPOSIUM (RTAS 2017)}},
