@article{ ISI:000405992100005,
Author = {Zhang, Jing},
Title = {{Analysis of the Innovative Mode of College Foreign Language Teaching
   from the Perspective of Digital Literacy}},
Journal = {{AGRO FOOD INDUSTRY HI-TECH}},
Year = {{2017}},
Volume = {{28}},
Number = {{1}},
Pages = {{14-17}},
Month = {{JAN-FEB}},
Abstract = {{With the rapid development of digital information technology, the key
   ability of digital literacy has become a consensus of the new era.
   Combining digital literacy with college teaching practice and innovating
   a new mode of foreign language teaching are good for cultivating more
   comprehensive high-quality talents who will be suitable for the
   information age. At present, the domestic research on the reforms of
   college foreign language teaching has already taken shape while the
   research on college foreign language teaching from the perspective of
   digital literacy is rarely. Hence, based on the connotation and
   composition framework of digital literacy, this paper analyzes the
   necessity of applying digital literacy in foreign language teaching
   reform. Finally, from the perspective of digital literacy, this paper
   puts forward the innovative mode of college foreign language teaching
   reform, with a view to providing reference for the training of Chinese
   compound foreign language talents.}},
