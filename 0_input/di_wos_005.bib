@inproceedings{ ISI:000403403400047,
Author = {Saquib, Zia and Kumar, Manish and Kamal, Kapil Kant and Varyani, Bharat},
Book-Group-Author = {{IEEE}},
Title = {{Secure Solution: One Time Mobile Originated PKI}},
Booktitle = {{2017 11TH ANNUAL IEEE INTERNATIONAL SYSTEMS CONFERENCE (SYSCON)}},
Series = {{Annual IEEE Systems Conference}},
Year = {{2017}},
Pages = {{340-345}},
Note = {{11th Annual IEEE International Systems Conference (SysCon), Montreal,
   CANADA, APR 24-27, 2017}},
Organization = {{IEEE; IEEE Syst Council}},
Abstract = {{With the advancement of eGovernance and development of mobile governance
   services, plenty of services are being made available online. In many of
   services, using simple username password kind of authentication,
   citizens need to prove their identity online. In order to prove their
   identity for such services in the online environment, a user needs to
   prove in ownership of certain information or token that links their
   natural identity to an online account for the access to such services.
   This is called e-authentication; it is usually followed by
   identification with a valid ID card/token etc. to the authority issuing
   the credential. Online authentication implies user's control of valid
   credentials (e.g. username-password, OTP, digital keys, BIOMETRICS
   etc.), which are issued either by the Trusted Authorities. Here we are
   introducing an innovative way of secure authentication, One Time Mobile
   Originated PKI (Public Key Infrastructure), which is basically a way to
   ensure that an electronic data is from genuine user. In this paper we
   will explain how One Time Mobile Originated PKI can be used to securely
   authenticate any online user.}},
