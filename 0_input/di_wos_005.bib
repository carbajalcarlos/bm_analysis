@article{ ISI:000393748300025,
Author = {Brunswicker, Sabine and Matei, Sorin Adam and Zentner, Michael and
   Zentner, Lynn and Klimeck, Gerhard},
Title = {{Creating impact in the digital space: digital practice dependency in
   communities of digital scientific innovations}},
Journal = {{SCIENTOMETRICS}},
