@inproceedings{ ISI:000413668602103,
Author = {Randelovic, Dragana and El-Sokkary, Wael},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{STUDENTS DESIGN AND PROGRAM THEIR OWN VIDEO GAME TO LEARN ENGINEERING
   DESIGN PROCESS}},
