@article{ ISI:000410915400009,
Author = {Farrington, Ted and Alizadeh, Azar},
Title = {{On the Impact of Digitalization on R\&D}},
Journal = {{RESEARCH-TECHNOLOGY MANAGEMENT}},
Year = {{2017}},
Volume = {{60}},
Number = {{5}},
Pages = {{24-30}},
Abstract = {{OVERVIEW:Digitalization and Its Implications for R\&D Management is an
   IRI Research platform established to consider the likely impacts of
   three distinct sets of digitalization technologiesvirtual
   experimentation and simulation, virtual collaboration, and big dataon
   R\&D in the next five years. In October 2016, as a preliminary step in
   defining its work, platform participants conducted a workshop to gain
   broad input about the potential impacts of digitalization in the three
   focus areas. As a result of small-group brainstorming about how each
   area of digitalization could inform, enable, or disrupt a specific
   aspect of research and technology management, the research teams
   identified a number of possible effects of digitalization, including
   increased consumer and customer intimacy, greater speed to market,
   broader use of open innovation, leveling of the information playing
   field across companies, and the growth of true cradle-to-grave product
   lifecycle simulation.}},
