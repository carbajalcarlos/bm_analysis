@article{ ISI:000404607100007,
Author = {Kurbad, A.},
Title = {{A new milling machine for computer-aided, in-office restorations
   Technology, integration, and workflow}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTERIZED DENTISTRY}},
Year = {{2017}},
Volume = {{20}},
Number = {{2}},
Pages = {{201-213}},
Abstract = {{Chairside computer-aided design/computer-aided manufacturing (CAD/CAM)
   technology requires an effective technical basis to obtain dental
   restorations with optimal marginal accuracy, esthetics, and longevity in
   as short a timeframe as possible. This article describes a compact,
   5-axis milling machine based on an innovative milling technology (5XT
   five-axis turn-milling technique), which is capable of achieving
   high-precision milling results within a very short processing time.
   Furthermore, the device's compact dimensioning and state-of-the-art mode
   of operation facilitate its use in the dental office. This model is also
   an option to be considered for use in smaller dental laboratories,
   especially as the open input format enables it to be quickly and simply
   integrated into digital processing systems already in use. The
   possibility of using ceramic and polymer materials with varying
   properties enables the manufacture of restorations covering all
   conceivable indications in the field of fixed dental prosthetics.}},
