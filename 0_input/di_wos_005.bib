@article{ ISI:000399616200002,
Author = {Jetha, Karim and Berente, Nicholas and King, John Leslie},
Title = {{Digital and analog logics: An analysis of the discourse on property
   rights and information goods}},
Journal = {{INFORMATION SOCIETY}},
Year = {{2017}},
Volume = {{33}},
Number = {{3}},
Pages = {{119-132}},
Abstract = {{We examine arguments both for strengthening property rights and
   expanding the public domain for digital information goods. Specifically,
   we conduct a Toulminian analysis of arguments made at a
   cross-disciplinary symposium at Duke Law School on information goods and
   the public domain. We find that there are two logics underlying
   arguments on issues related to information goods: what we describe as
   digital and analog logics. We also identify five argument
   categoriesprofit motive, economic efficiency, innovation, ethics and
   fundamental rights, and temporalityand two different time frames used
   for judging impactsproximal and distal. We conclude with a number of
   theory-generating propositions and a provocation for our conceptions of
   capitalism in the digital age.}},
