@article{ ISI:000391134400003,
Author = {Khan, Ikram Ullah and Hameed, Zahid and Khan, Safeer Ullah},
Title = {{Understanding Online Banking Adoption in a Developing Country: UTAUT2
   with Cultural Moderators}},
