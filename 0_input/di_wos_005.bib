@inproceedings{ ISI:000408271500015,
Author = {Vuppalapati, Jaya Shankar and Kedari, Santosh and Ilapakurthy, Ananth
   and Ilapakurti, Anitha and Kedari, Sharat and Vuppalapati, Chandrasekar},
Book-Group-Author = {{IEEE}},
Title = {{Smart Dairies - Enablement of Smart City At Gross Root Level}},
