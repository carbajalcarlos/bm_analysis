@inproceedings{ ISI:000381319700035,
Author = {Fornasari, Alberto},
Editor = {{Nagy, D and Borcan, V and Calvano, G and Levonian, RM and Pilchin, M}},
Title = {{ICT AND INNOVATIVE TEACHING. HOW TO BUILD SKILLS FOR GENERATING WEB 2.0
   WITH FLIPPED LEARNING. A CASE STUDY ITIS MAJORANA OF BRINDISI}},
Booktitle = {{REDEFINING COMMUNITY IN INTERCULTURAL CONTEXT, RCIC'15}},
