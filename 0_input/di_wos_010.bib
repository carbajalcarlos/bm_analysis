@inproceedings{ ISI:000380512300006,
Author = {Wu, Dazhong and Terpenny, Janis and Gentzsch, Wolfgang},
Editor = {{Shih, AJ and Wang, LH}},
Title = {{Cloud-Based Design, Engineering Analysis, and Manufacturing: A
   Cost-Benefit Analysis}},
