Title = {{A 3.7 M-Pixel 1300-fps CMOS Image Sensor With 5.0 G-Pixel/s High-Speed
   Readout Circuit}},
Journal = {{IEEE JOURNAL OF SOLID-STATE CIRCUITS}},
Year = {{2015}},
Volume = {{50}},
