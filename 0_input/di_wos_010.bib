@inproceedings{ ISI:000384470100026,
Author = {Zacharova, Zlatica Jursova and Sokolova, Lenka and Lemesova, Miroslava},
Editor = {{Roceanu, I and Ciolan, L and Holotescu, C and Radu, C and Stanescu, I and Logofatu, B}},
Title = {{INTERACTIVE TEACHING OF PSYCHOLOGY: USING DESIGN-BASED RESEARCH IN
   DEVELOPING AN INTERACTIVE PSYCHOLOGY COURSE}},
Booktitle = {{RETHINKING EDUCATION BY LEVERAGING THE ELEARNING PILLAR OF THE DIGITAL
   AGENDA FOR EUROPE!, VOL. II}},
