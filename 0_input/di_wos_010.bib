@inproceedings{ ISI:000380437800060,
Author = {Smuts, K.},
Editor = {{Yen, YN and Weng, KH and Cheng, HM}},
Title = {{SAHRIS: USING THE SOUTH AFRICAN HERITAGE REGISTER TO REPORT, TRACK AND
   MONITOR HERITAGE CRIME}},
Booktitle = {{25TH INTERNATIONAL CIPA SYMPOSIUM 2015}},
