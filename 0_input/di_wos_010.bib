@article{ ISI:000353572400004,
Author = {Xiao, Sonia and Liu, Cathy Xiaoyue and Wang, Yinhai},
Title = {{Data-Driven Geospatial-Enabled Transportation Platform for Freeway
   Performance Analysis}},
Journal = {{IEEE INTELLIGENT TRANSPORTATION SYSTEMS MAGAZINE}},
