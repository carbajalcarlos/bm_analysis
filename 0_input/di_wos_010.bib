@inproceedings{ ISI:000380490000096,
Author = {Mierop, R. and Oudmaijer, S. and Tol, T. and Spitsbaard, K. and Madas,
   M. and Zografos, K.},
Editor = {{Mahdavi, A and Martens, B and Scherer, R}},
Title = {{Breaking down the barriers to ICT usage in the European construction
   sector}},
Booktitle = {{EWORK AND EBUSINESS IN ARCHITECTURE, ENGINEERING AND CONSTRUCTION 2014}},
Year = {{2015}},
Pages = {{711-718}},
Note = {{PROCEEDINGS OF THE 10TH EUROPEAN CONFERENCE ON PRODUCT AND PROCESS
   MODELLING (ECPPM 2014), Department of Building Physics and Building
   Ecology of the Vienna Universi, Vienna, AUSTRIA, SEP 17-19, 2014}},
Abstract = {{ICT is recognised as a key driver for innovation, competitiveness and
   growth. Still, ICT uptake in the construction sector is relatively low,
   especially for SMEs, as compared to other economic sectors. Limited
   financial resources, lack of ICT customisation to sector needs, and
   limited ICT awareness currently prevents construction companies from
   using ICT. This paper aims to break down these barriers by developing
   and validating an integrated approach that aims to raise awareness of
   ICT benefits, while simultaneously demonstrating a user-friendly,
   inexpensive ICT solution/tool for digital collaboration. The
   demonstration of the digital collaboration tool in real-world project
   cases provides a successful proof-of-concept and shows that it is
