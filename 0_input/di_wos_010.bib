@article{ ISI:000354726600008,
Author = {Khalifah, Noor Aini and Salleh, Salmah Mohd and Adam, Radziah},
Title = {{FDI productivity spillovers and the technology gap in Malaysia's
   electrical and electronic industries}},
Journal = {{ASIAN-PACIFIC ECONOMIC LITERATURE}},
Year = {{2015}},
Volume = {{29}},
Number = {{1}},
Pages = {{142-160}},
Month = {{MAY}},
Abstract = {{This study measures the impact of indirect technology transfer through
   spillovers from foreign direct investment (FDI) on Malaysia's
   development of a high-tech' sector, namely the electrical and electronic
   (E\&E) industries. We estimate the total factor productivity (TFP) of
   establishments as a function of foreign presence within and across
   industries. Both the fixed assets and wages share of foreign
   establishments in a five-digit ISIC (International Standard Industrial
   Classification) industry are used as measures of foreign presence. The
   estimations provide evidence of significantly negative (or
   insignificant) FDI vertical spillover effects and insignificant
   horizontal spillover effects on the TFP of domestic establishments. The
   positive (negative) coefficient of the interaction term between the
   forward (horizontal) spillover variable and the technology gap supports
   the catching-up' (technology accumulation) hypothesis. The negative
   impact, even absence, of FDI spillover effects on TFP and the mixed
   evidence on the effects of interactions between FDI spillovers and the
   technology gap suggest that fine-tuning of fiscal incentive schemes for
   FDI to arrive at positive net benefits may prove to be a daunting task
   in the Malaysian E\&E industries.}},
