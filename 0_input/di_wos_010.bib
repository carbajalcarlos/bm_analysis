Title = {{Cultural Heritage 2.0. Toward innovative tools for the communication of
   cultural and historical asset}},
Booktitle = {{Heritage and Technology: Mind Knowledge Experience}},
Series = {{Fabbrica della Conoscenza}},
Year = {{2015}},
