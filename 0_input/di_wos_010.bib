@incollection{ ISI:000386019300014,
Author = {Laferriere, Therese and Allaire, Stephane and Breuleux, Alain and Hamel,
   Christine and Law, Nancy and Montane, Mireia and Hernandez, Oscar and
   Turcotte, Sandrine and Scardamalia, Marlene},
