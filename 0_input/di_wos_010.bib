@article{ ISI:000353088300033,
Author = {Barisic, Ivana and Doksanovic, Tihomir and Draganic, Hrvoje},
Title = {{Characterization of hydraulically bound base materials through digital
   image correlation}},
Journal = {{CONSTRUCTION AND BUILDING MATERIALS}},
Year = {{2015}},
Volume = {{83}},
Pages = {{299-307}},
Month = {{MAY 15}},
Abstract = {{Accurate design of pavement behavior requires elastic properties of all
   the constituting layers. Measuring of elastic modulus on crude surfaces,
   by contact based mechanical devices, often results in a high dispersion
   of determined values. Therefore, application of multiple measurement
   methods on six different mixtures is presented, among which an
   innovative approach using spatial digital image correlation. Obtained
   results indicate that there is a strong correlation between the
   compressive strength and the dynamic and static modulus. Thus, by using
   presented equations it is possible to predict elastic properties of
   cement bound aggregate by measuring just one of the relevant parameters.
   (C) 2015 Elsevier Ltd. All rights reserved.}},
