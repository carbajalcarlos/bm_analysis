@article{ ISI:000354998000007,
Author = {Coe, Anna-Britt and Vandegrift, Darcie},
Title = {{Youth Politics and Culture in Contemporary Latin America: A Review}},
Journal = {{LATIN AMERICAN POLITICS AND SOCIETY}},
Year = {{2015}},
Volume = {{57}},
Number = {{2}},
Pages = {{132-153}},
Month = {{SUM}},
Abstract = {{Youth politics in contemporary Latin America diverge from those of
   previous generations. Increasingly decoupled from parties, unions, and
   the state, young people glide seamlessly across previously assumed
   boundaries: culture and politics, individual and organization,
   subjectivity and collectivity, virtual and real. This article presents
   findings from a systematic review of research on youth politics and
   demonstrates the new direction through three main categories:
   repression, incorporation, and exclusion, relationships between state
   institutions and youth identities; generational, cultural, and digital
   lenses, the innovative trends for theorizing current patterns of youth
   politics; and unsettling politics, the fusion and diffusion of youth
   political dexterity. The article concludes by highlighting current
   strengths and proposing future steps to build on this new direction.}},
