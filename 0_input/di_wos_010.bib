@inproceedings{ ISI:000383740300097,
Author = {Salminen, Vesa and Kantola, Jussi I. and Vanharanta, Hannu},
Editor = {{Ahram, T and Karwowski, W and Schmorrow, D}},
Title = {{Competence portfolio assessment of research and development center for
   regional development}},
