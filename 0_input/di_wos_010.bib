@inproceedings{ ISI:000380517100012,
Author = {Maharjan, Mahesh and Shakya, Subama},
Book-Group-Author = {{IEEE}},
Title = {{System Architecture for Social Security Cash Transfer Through Branchless
   Banking in Nepal}},
Booktitle = {{2015 International Conference on Green Computing and Internet of Things
   (ICGCIoT)}},
Year = {{2015}},
Pages = {{60-64}},
Note = {{International Conference on Green Computing and Internet of Things
   (ICGCIoT), GALGOTIAS Educ Inst, Greater Noida, INDIA, OCT 08-10, 2015}},
Organization = {{IEEE Advancing Technol Human; IEEE UP Sect India; IEEE Comp Soc; IEEE
   Syst Man \& Cybernet Soc; Malaysia Chapter; Inst Neural Network Soc}},
Abstract = {{Social security is a basic right of a citizen. Every citizen but mostly
   poor, old, weak and helpless citizen of a nation is in need of social
   security. In order to maintain minimum living standard, government
   provides social security to its citizens. In Nepal, the number of social
   security allowance beneficiaries has been increasing each year. Mostly
   in rural areas, direct cash distribution to beneficiaries is a common
   practice by local government through manual process. In urban areas
   where there is access of banks, social cash transfer has been carried
   out through banking channel through manual process. Because of manual
   process, some are benefitted twice and needy citizens are missing from
   the programme. The manual process is really time consuming, irritating,
   problems at time of payment and extracting report is difficult.
   Therefore, digitization of beneficiaries list and process of social
   security cash transfer needs to be automatic. The main motive of
   financial institutions is to increase maximum profit that's why they
   have been providing banking facilities at core market area and hesitate
   to reach to rural areas. Branchless banking is a new innovative idea to
   reach to poor and unbanked people of rural areas. The research aims to
   design system architecture for social security cash transfer through
   branchless banking.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
