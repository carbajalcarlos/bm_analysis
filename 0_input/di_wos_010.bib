@inproceedings{ ISI:000380493600034,
Author = {Triantis, D. and Stavrakas, I. and Pasiou, E. D. and Hloupis, G. and
   Kourkoulis, S. K.},
Editor = {{Iacoviello, F and Ferro, GA and Susmel, L}},
