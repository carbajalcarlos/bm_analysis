@inproceedings{ ISI:000380582500003,
Author = {Sharma, Gajendra},
Editor = {{Wan, M}},
Title = {{Big Data for New Industrialization and Urbanization Development A Case
   Study in Chinese Cities}},
Booktitle = {{NEW INDUSTRIALIZATION AND URBANIZATION DEVELOPMENT ANNUAL CONFERENCE:
   THE INTERNATIONAL FORUM ON NEW INDUSTRIALIZATION DEVELOPMENT IN BIG-DATA
   ERA}},
Year = {{2015}},
