Title = {{Towards a Human Centric Intelligent Society}},
Booktitle = {{2015 9TH INTERNATIONAL CONFERENCE ON INNOVATIVE MOBILE AND INTERNET
   SERVICES IN UBIQUITOUS COMPUTING IMIS 2015}},
Year = {{2015}},
