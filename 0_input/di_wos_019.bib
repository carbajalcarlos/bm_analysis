@inproceedings{ ISI:000313664900040,
Author = {Cardamone, Jeanette M.},
Book-Group-Author = {{Natl Engn Res Ctr Dye \& Finish Textiles
   Minist Educ, Key Lab Sc \& Technol Eco-Textile
   Donghua Univ, Coll Chem, Chem Engn \& Biotechnol}},
Title = {{Wool Shrinkproofing by Chemical and Enzymatic Methods}},
Booktitle = {{PROCEEDINGS OF THE 12TH INTERNATIONAL WOOL RESEARCH CONFERENCE, VOLS I
   AND II}},
Year = {{2010}},
Pages = {{166-180}},
Note = {{12th International Wool Research Conference (IWRC 2010), Shanghai,
   PEOPLES R CHINA, OCT 19-22, 2010}},
Organization = {{Jiangsu Sunshine Grp; Shandong Ruyi Sci \& Technol; Hong Kong Esquel
   Grp; Australian Wool Innovat}},
Abstract = {{The uniqueness of wool, its functionality, morphology, physical/
   mechanical behavior, and aesthetics, continue to be topics for
   innovative research to improve properties and create novel products. The
   perennial problems of yellowness, discomfort in wear, and shrinkage have
   been addressed over time as isolated and combined topics of
   investigations altering morphology and chemistry with processes designed
   to prevent or limit negative impact on wool's appearance and
   performance. Chemical bleaching with alkaline peroxide and
   shrinkproofing with chlorination have limited economy and ecological
   acceptance. The ARS Process for bleaching, biopolishing, and
   shrink-proofing wool requires near-room-temperature conditions with
   short exposure times to reach high levels of whiteness, softness, and
   dimensional stability, and the design has implications for
   sustainability with low energy and low impact on the environment.
   Scanning electron micrographs of fibers from ARS-treated fabrics showed
   erosion of wool's surface scales without fiber damage to limit felting
   shrinkage to less than 2\%, the indication of a mercerizing effect on
   cotton limiting shrinkage to 4\%, and complete shrinkage control of
   viscose challis. After processing, the mechanical properties of the
   fabrics and the structural integrity of the fibers were preserved. High
