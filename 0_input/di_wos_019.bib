@inproceedings{ ISI:000318805500024,
Author = {Quijano Lopez, Rocio and Ocana Moral, Ma Teresa},
Editor = {{Chova, LG and Belenguer, DM and Torres, IC}},
Title = {{``BLENDED LEARNING{''} AND EDUCATIONAL INNOVATION FOR THE ADAPTATION TO
   THE EUROPEAN SPACE OF HIGHER EDUCATION OF CONTENTS RELATED TO TEACHING
   SCIENCES OF THE NATURE IN THE UNIVERSITY OF JAEN}},
Booktitle = {{4TH INTERNATIONAL TECHNOLOGY, EDUCATION AND DEVELOPMENT CONFERENCE
   (INTED 2010)}},
Year = {{2010}},
Pages = {{170-180}},
Note = {{4th International Technology, Education and Development Conference
   (INTED), Valencia, SPAIN, MAR 08-10, 2010}},
Abstract = {{In this work, we describe an experience carried out for the subject
   ``Conocimiento del Medio Natural y su Didactica{''}, that is given in
   the University of Jaen, with the aim to join some aspects referred to
   the formative demands associated with the renovation of the educational
   methodologies that European Space of Higher Education (E. S. H. E.)
   proposes, and the contributions that the use of the new technologies
   related to the information and communication (I. C. T.) could have, so
   that it promoted and make easier an effective learning of the contents
   related to the sciences of the nature in students of Preservice Teacher
   Degree.
   We show the use of some tools related with I. C. T. -as computer
   networks or digitized platforms - that university teachers have for the
   development of their teaching work, in agreement with the change of
   paradigm that supposes the implementation of the aims proposed in the
   Bologna Declaration, so that there takes place a significant learning of
   the contents worked by the students in a autonomous way - but directed
   by the teacher-by means of the integration, in the cognitive structures
   of this students, of the new knowledge acquired during the process of
   learning and teaching.}},
Publisher = {{IATED-INT ASSOC TECHNOLOGY EDUCATION \& DEVELOPMENT}},
Address = {{LAURI VOLPI 6, VALENICA, BURJASSOT 46100, SPAIN}},
