Title = {{Fast Reference Phase Extraction and Wave-front Reconstruction for
   Generalized Phase-shifting Interferometry}},
Booktitle = {{INTERNATIONAL CONFERENCE ON ADVANCED PHASE MEASUREMENT METHODS IN OPTICS
   AN IMAGING}},
Series = {{AIP Conference Proceedings}},
