@incollection{ ISI:000278501200009,
Author = {Starkey, Louise},
Editor = {{Henshall, AD and Fontanez, BC}},
Title = {{THE COMPLEXITY OF EDUCATIONAL CHANGE AT THE START OF THE DIGITAL AGE}},
Booktitle = {{EDUCATIONAL CHANGE}},
Series = {{Education in a Competitive and Globalizing World}},
Year = {{2010}},
Pages = {{189-204}},
Abstract = {{Digital technologies have become ubiquitous in most societies, changing
   the way that communication, collaboration, access to information, and
   knowledge development can occur. The introduction of digital
   technologies into society has been a catalyst for change within
   schooling. Innovative teachers have integrated new technologies to their
   teaching practice developing ideas found to work and setting aside
   experiences that do not, theories are developed debated and altered
   through educator networks. New knowledge about pedagogy is emerging
   which is likely to cause significant change as systems, mental models
   and processes adapt to new ways of thinking about teaching and learning
   in a digitally enhanced society. The interaction between education
   policies, teaching practice, research, and educational theory is
   influencing teaching praxis. This chapter examines the change process
   across complex schooling structures during the start of a digital age.}},
