@inproceedings{ ISI:000285047500056,
Author = {Refai, Hakki H. and Salahieh, Basel and Sluss, Jr., James J.},
Editor = {{Wong, KH and Miga, MI}},
Title = {{3D Visualization of Medical Imaging Using Static Volumetric Display -
   CSpace (R)}},
Booktitle = {{MEDICAL IMAGING 2010: VISUALIZATION, IMAGE-GUIDED PROCEDURES, AND
   MODELING}},
Series = {{Proceedings of SPIE}},
Year = {{2010}},
Volume = {{7625}},
