@article{ ISI:000280194800012,
Author = {Paris, S. and Hopfenmuller, W. and Meyer-Lueckel, H.},
Title = {{Resin Infiltration of Caries Lesions: an Efficacy Randomized Trial}},
Journal = {{JOURNAL OF DENTAL RESEARCH}},
