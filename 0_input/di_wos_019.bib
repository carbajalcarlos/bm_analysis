@inproceedings{ ISI:000302331000001,
Author = {Mambretti, Joe},
Editor = {{Doulamis, A and Mambretti, J and Tomkos, I and Varvarigou, T}},
Title = {{Designing 21st Century Communications: Architecture, Services,
   Technology, and Facilities}},
