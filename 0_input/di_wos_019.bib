@article{ ISI:000274196100023,
Author = {Valley, Pouria and Mathine, David L. and Dodge, Mohammad Reza and
   Schwiegerling, Jim and Peyman, Gholam and Peyghambarian, N.},
Title = {{Tunable-focus flat liquid-crystal diffractive lens}},
Journal = {{OPTICS LETTERS}},
Year = {{2010}},
Volume = {{35}},
Number = {{3}},
Pages = {{336-338}},
Month = {{FEB 1}},
Abstract = {{We demonstrate an innovative variable-focus flat liquid-crystal
   diffractive lens ( LCDL) with 95\% diffraction efficiency and
   millisecond switching times using a +/- 2.4 V ac input. This lens is
   based on the electrical modulation of a 3 mu m layer of nematic
   liquid-crystal sandwiched between a Fresnel zone electrode structure and
   a reference substrate. Each zone is divided into 12 subzones to digitize
   the phase profiles and define the phase wrapping points. The focusing
   power can rapidly be switched by electrically changing the number of
   subzones and re-establishing the wrapping points. Potential applications
   include zooms with no moving parts and autofocus lenses for compact
   imaging devices. (C) 2010 Optical Society of America}},
