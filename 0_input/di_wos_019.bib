@inproceedings{ ISI:000318781702019,
Author = {Stoyanov, Tinko},
Editor = {{Chova, LG and Belenguer, DM and Torres, IC}},
Title = {{DESIGNING MORE EFFICIENT E-LEARNING IN THE WORKPLACE}},
