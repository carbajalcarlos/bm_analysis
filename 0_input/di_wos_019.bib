@inproceedings{ ISI:000286407000007,
Author = {Ebne-Alian, Mohammad and Kharma, Nawwaf},
Editor = {{Tempesti, G and Tyrrell, AM and Miller, JF}},
Title = {{A New Method to Find Developmental Descriptions for Digital Circuits}},
Booktitle = {{EVOLVABLE SYSTEMS: FROM BIOLOGY TO HARDWARE}},
