@inproceedings{ ISI:000280109500018,
Author = {Ogan, Amy and Aleven, Vincent and Kim, Julia and Jones, Christopher},
Editor = {{Aleven, V and Kay, J and Mostow, J}},
Title = {{Intercultural Negotiation with Virtual Humans: The Effect of Social
   Goals on Gameplay and Learning}},
