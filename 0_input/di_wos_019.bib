Title = {{Electronics and telecommunications in Poland, issues and perspectives
   Part III: Innovativeness, Applications, Economy, Development Scenarios,
   Politics}},
Booktitle = {{PHOTONICS APPLICATIONS IN ASTRONOMY, COMMUNICATIONS, INDUSTRY, AND
   HIGH-ENERGY PHYSICS EXPERIMENTS 2010}},
