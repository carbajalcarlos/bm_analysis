@article{ ISI:000267235000008,
Author = {Ng, M. C. F. and Zhou, J.},
Title = {{A Study on Figured Double-face Jacquard Fabric with Full-color Effect}},
Journal = {{TEXTILE RESEARCH JOURNAL}},
Year = {{2009}},
Volume = {{79}},
Number = {{10}},
Pages = {{930-936}},
Month = {{JUL}},
Abstract = {{The application of digital design technology has provided the chance for
   innovating the structure design of traditional jacquard fabric. In this
   paper, a study on innovative design was carried out aimed at creating
   double-face fabric with novel full-color effects on both face and back
   sides. The basic design theory was that with the application of
   full-color compound fabric structure and regular stitch weaves, under
   layered-combination design mode, the figured double-face jacquard fabric
   could be designed through the combination of face and backing fabric
   structures that were designed respectively in advance. By such way, the
   resultant fabric structure was capable of expressing independent
   full-color figuring effects on both sides of fabric, in which the
   backing structure was covered by face structure. Such fabric fully meets
   the requirement of balanced interlacement and poses no problems in
   industrial production due to the application of the regular face,
   backing and stitch weaves.}},
