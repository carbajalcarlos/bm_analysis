@inproceedings{ ISI:000268721800019,
Author = {Krieger, G. and Hajnsek, I. and Papathanassiou, K. and Eineder, M. and
   Younis, M. and De Zan, F. and Prats, P. and Huber, S. and Werner, M. and
   Fiedler, H. and Freeman, A. and Rosen, P. and Hensley, S. and Johnson,
   W. and Veilleux, L. and Grafmueller, B. and Werninghaus, R. and Bamler,
   R. and Moreira, A.},
