@inproceedings{ ISI:000289878100017,
Author = {Vogt, Andreas and Eich, Markus and Samperio, Renato and Kirchner, Frank},
Book-Group-Author = {{IEEE}},
Title = {{A Practical Interaction Between Robots and RFID-based Digital Product
   Memories in a Logistic Scenario}},
