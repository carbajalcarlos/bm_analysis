@article{ ISI:000266296900002,
Author = {Lee, Hsing-Juin and Jong, Yow-Jeng and Chang, Li-Min and Wu, Wen-Lin},
Title = {{Propulsion Strategy Analysis of High-Speed Swordfish}},
