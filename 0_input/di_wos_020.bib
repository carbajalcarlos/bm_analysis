@inproceedings{ ISI:000393008200016,
Author = {Wannous, Hazem and Lucas, Yves and Treuillet, Sylvie},
Editor = {{Richard, P and Braz, J}},
Title = {{COMBINED MACHINE LEARNING WITH MULTI-VIEW MODELING FOR ROBUST WOUND
   TISSUE ASSESSMENT}},
Booktitle = {{VISAPP 2010: PROCEEDINGS OF THE INTERNATIONAL CONFERENCE ON COMPUTER
   VISION THEORY AND APPLICATIONS, VOL 1}},
Year = {{2010}},
Pages = {{98-104}},
