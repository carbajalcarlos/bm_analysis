@inproceedings{ ISI:000358303400019,
Author = {de Amicis, Raffaele and Conti, Giuseppe and Prandi, Federico},
Editor = {{Brovelli, MA and Dragicevic, S and Li, S and Veenendaal, B}},
Title = {{AN INTEGRATED FRAMEWORK FOR SPATIO-TEMPORAL DATA MANAGEMENT: THE PROJECT
   BRISEIDE - BRIDGING SERVICES INFORMATION AND DATA FOR EUROPE}},
Booktitle = {{WEBMGS 2010: 1ST INTERNATIONAL WORKSHOP ON PERVASIVE WEB MAPPING,
   GEOPROCESSING AND SERVICES}},
Series = {{International Archives of the Photogrammetry Remote Sensing and Spatial
   Information Sciences}},
