@inproceedings{ ISI:000289887900015,
Author = {Li Jin and Liang Hong and Yang Guangda and Feng Yaoyu and Meichao, Lv},
Editor = {{Tan, H and Luo, Q}},
Title = {{A Semantic Image Retrieval Approach Between Visual Features and Medical
   Concepts}},
