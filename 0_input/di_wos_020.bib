@inproceedings{ ISI:000302328400024,
Author = {Graham, Gary and Kerrigan, Finola and Mehmood, Rashid and Rahman,
   Mustafizur},
Editor = {{Mehmood, R and Cerqueira, E and Piesiewicz, R and Chlamtac, I}},
Title = {{The Interaction of Production and Consumption in the News Media Social
   Space}},
Booktitle = {{COMMUNICATIONS INFRASTRUCTURE: SYSTEMS AND APPLICATIONS IN EUROPE}},
