@inproceedings{ ISI:000266322100093,
Author = {Weber, C. and Hoehne, G. and Husung, S.},
Book-Group-Author = {{Kaunas Univ of Technology}},
Title = {{Modeling of Acoustical Product Properties for Technical Systems in VR}},
Booktitle = {{MECHANIKA 2009 - PROCEEDINGS OF THE 14TH INTERNATIONAL CONFERENCE}},
Series = {{Mechanika Kaunas University of Technology}},
Year = {{2009}},
Pages = {{456-460}},
Note = {{14th International Conference on Mechanika, Kaunas Univ Technol, Kaunas,
   LITHUANIA, APR 02-03, 2009}},
Organization = {{Lithuanian Acad Sci; IFToMM Natl Committee Lithuania; Baltic Assoc
   Mechan Engn}},
Abstract = {{Virtual Prototyping is an important tool to verify and optimise product
   properties in the early design phases. By using digital product models
   many simulations of the system behaviour can be carried out. Today there
   are still deficits in modelling and evaluating the acoustical behaviour
   of technical products which is important for many industries.
   A flexible audio-visual projection system was configured which combines
   stereoscopic visualisation in a CAVE with a new spatial acoustic
   reproduction method: wavc-field-synthesis. For this, a scene
   representation is needed that provides the sound reproduction
   synchronously with the visualisation of the technical systems.
   This paper describes the goals of the new audio-visual Virtual Reality
   system and its use as an innovative tool for a realistic representation
   and presentation of audio-visual product properties, even in early
   design phases. The paper is also focused on methods to describe
   acoustical product properties of technical systems for an interactive
