@article{ ISI:000269677100004,
Author = {Snead, Malcolm L. and Slavkin, Harold C.},
Title = {{Science is the fuel for the engine of technology and clinical practice}},
Journal = {{JOURNAL OF THE AMERICAN DENTAL ASSOCIATION}},
