@article{ ISI:000264136500011,
Author = {Wang, Jia-Chang and Hsieh, Ming-Zhe},
Title = {{Applying the one-column, many pencil local scanning maskless lithography
   technology to micro-RP system}},
Journal = {{INTERNATIONAL JOURNAL OF ADVANCED MANUFACTURING TECHNOLOGY}},
