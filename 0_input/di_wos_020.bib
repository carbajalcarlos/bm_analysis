Author = {Ji, Yunfeng},
Editor = {{Tomizuka, M and Yun, CB and Giurgiutiu, V and Lynch, JP}},
Title = {{A Computer Vision-based Approach for Structural Displacement Measurement}},
Booktitle = {{SENSORS AND SMART STRUCTURES TECHNOLOGIES FOR CIVIL, MECHANICAL, AND
   AEROSPACE SYSTEMS 2010}},
Series = {{Proceedings of SPIE-The International Society for Optical Engineering}},
