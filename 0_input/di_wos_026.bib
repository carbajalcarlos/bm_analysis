@article{ ISI:A1994PZ30700007,
Author = {KITAWAKI, N and MISHIMA, H},
Title = {{PSI-CELP SPEECH CODING DOUBLES THE CAPACITY OF RADIO-WAVES}},
Journal = {{NTT REVIEW}},
Year = {{1994}},
Volume = {{6}},
Number = {{6}},
Pages = {{46-52}},
Month = {{NOV}},
Abstract = {{The digital cellular mobile radio system introduced in Japan in March
   1993 (a full bit-rate system) is likely to run short of radio bandwidth
   within a few years because the number of subscribers is quickly
   increasing. We have therefore developed a speech coding method that
   operates at half the bit-rate but still has almost the same quality as
   the full bit-rate system. This new method is called PSI-CELP (pitch
   synchronous innovation code excited linear prediction); it has been
   adopted as the half-rate PDC (personal digital cellular) standard in
   Japan. The PSI-CELP system is expected to be used in the next digital
   cellular mobile radio system.}},
