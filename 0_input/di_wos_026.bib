@inproceedings{ ISI:A1996BJ47X00085,
Author = {Zhang, GS and Brown, A},
Book-Group-Author = {{INST NAVIGAT}},
Title = {{TIDGET-T: A low cost GPS time sensor that maintains precise time during
   extended periods of GPS signal loss}},
Booktitle = {{PROCEEDINGS OF THE 52ND ANNUAL MEETING - THE INSTITUTE OF NAVIGATION:
   NAVIGATIONAL TECHNOLOGY FOR THE 3RD MILLENNIUM}},
Year = {{1996}},
Pages = {{749-752}},
Note = {{52nd Annual Meeting of the Institute-of-Navigation on Navigational
   Technology for the 3rd Millenium, CAMBRIDGE, MA, JUN 19-21, 1996}},
Organization = {{Inst Navigat}},
Abstract = {{Modern power and digital mobile communication reworks require precise
   time for efficient operation. The Global Positioning System (GPS)
   provides a highly precise time reference and is becoming the preeminent
   time source for many applications. However, significant problems occur
   when GPS operation is attempted in a high Electro Magnetic Interference
   (EMI) environment that affects the reliability and performance of the
   GPS reference.
   NAVSYS Corporation has developed the TIDGET-T time reference which
   combines low cost commercial GPS technology with a robust and reliable
   architecture to provide precision timing performance in challenging,
   high EMI environments for a GPS signal.
   The innovative SmartClock algorithm implemented in this system is
   designed to optimally calibrate the reference clock error during periods
   when GPS satellites are in view, and maintain precise time when GPS is
   not available. This significantly improves the reliability and
   performance of the timing receiver.}},
