@inproceedings{ ISI:000076041700096,
Author = {Claytonsmith, H},
Editor = {{Janiszewski, JM and Moron, W and Sega, W}},
Title = {{Residential broadband networking and concomitant EMC problems}},
Booktitle = {{ELECTROMAGNETIC COMPATIBILITY 1998}},
Year = {{1998}},
