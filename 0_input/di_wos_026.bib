@article{ ISI:A1994PV31200004,
Author = {ARGIRO, VJ and NICKLAW, C and WALSH, M},
Title = {{COMPUTER-AIDED ENGINEERING TECHNOLOGY FOR THE DESIGN OF ELECTRONIC
   SYSTEMS}},
Journal = {{NAVAL ENGINEERS JOURNAL}},
Year = {{1994}},
Volume = {{106}},
Number = {{6}},
Pages = {{21-30}},
Month = {{NOV}},
Abstract = {{The integration of system and architecture of the design process is key
   to reducing design cycle time and cost in developing complex hardware
   and software systems.  In this paper, we present an Integrated
   Engineering Environment.  It is a design, analysis, verification, and
   management environment for the system and architectural levels of
   complex systems, a system synthesis framework.  Discussion is included
   about Computer-Aided Engineering software simulation tools which offer
   the opportunity to gain confidence in the performance of sophisticated
   digital, analog, and hybrid electronic system designs before making a
   commitment to hardware.  This approach should be considered in today's
   environment, where it is important to find innovative ways to control
   development and operational costs.  With the concern for the loss of our
   defense industrial base, and when the original design team may no longer
   be available, using this technique makes it possible to re-engineer
   existing designs, converting them to current technology.}},
