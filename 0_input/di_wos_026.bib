@article{ ISI:A1997YJ27200010,
Author = {Lo, HY},
Title = {{A unidirectional cordic algorithm for angle computation and rotation}},
Journal = {{JOURNAL OF THE CHINESE INSTITUTE OF ENGINEERS}},
