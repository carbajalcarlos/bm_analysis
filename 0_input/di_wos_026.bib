@inproceedings{ ISI:000075897100008,
Author = {Gibbs, KM and Berger, H and Jones, TS and Davis, M},
Editor = {{Crane, RL and Achenbach, JD and Shah, SP and Matikas, TE and KhuriYakub, PT and Gilmore, RS}},
Title = {{Electronic X-ray systems for field aircraft inspection}},
Booktitle = {{NONDESTRUCTIVE CHARACTERIZATION OF MATERIALS IN AGING SYSTEMS}},
Series = {{MATERIALS RESEARCH SOCIETY SYMPOSIUM PROCEEDINGS}},
