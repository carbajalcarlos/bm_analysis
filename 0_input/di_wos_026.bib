@article{ ISI:A1993KV83600010,
Author = {JOCTEUR, R},
Title = {{FROM GUTTA-PERCHA TO SILICA - SUBMARINE CABLES AND THEIR TECHNICAL
   EVOLUTION}},
Journal = {{ONDE ELECTRIQUE}},
Year = {{1993}},
Volume = {{73}},
Number = {{2}},
Pages = {{49-56}},
Month = {{MAR-APR}},
Abstract = {{A long tradition of quality and reliability has characterized submarine
   telecommunication systems. The transition from analog transmission
   technique on coaxial cables to digital transmission technique on optical
   fibre cables, has been accomplished in preserving this established fact.
   The article describes the correlative evolution of the submarine cables
   technology and especially justifies the choices made by
   Alcatel-Submarcom to conciliate the respect of tradition and the
   innovative character connected with the introduction of optical fibres.
   The submarine cable used at present in Alcatel S.560 systems with
   regenerators {[}1], will have to evolute in the next years to take into
   account a better adaptation to repeaterless links in a market in full
   expansion and to long-haul links with optical amplification {[}2], the
   first applications of which are planned in 1995 and will constitute more
   than 50\% of the world market in the second part of the decade.}},
