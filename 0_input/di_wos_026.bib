@inproceedings{ ISI:A1996BH02Y00014,
Author = {DeVeuster, C and Slangen, P and Renotte, Y and Berwart, L and Lion, Y},
Editor = {{Dausmann, GJ}},
Title = {{In-line phase-shifter calibration and drift measurement and compensation
   for digital speckle pattern interferometry (DSPI)}},
Booktitle = {{HOLOGRAPHIC AND DIFFRACTIVE TECHNIQUES}},
