@article{ ISI:A1991FF58300010,
Author = {HECKEN, RP and GIGER, AJ},
Title = {{INNOVATION AND NEW TECHNOLOGIES FOR DIGITAL TRANSMISSION OVER
   TERRESTRIAL POINT-TO-POINT MICROWAVE LINKS}},
Journal = {{ARCHIV FUR ELEKTROTECHNIK}},
Year = {{1991}},
Volume = {{74}},
Number = {{3}},
Pages = {{239-251}},
Abstract = {{The paper traces the development of terrestrial digital radio relay
   systems from the beginning in the early 1970s with special emphasis on
   work done in the United States.  Parallel to the growth of digital
   services in the telephone network microwave radio systems were developed
   that are able to carry these new signals efficiently, reliably and at
   competitive cost.  Much work has been done to increase spectrum
   efficiency, the capability to transmit more and more information bits
   per hertz of bandwidth.  Dramatic improvements were also obtained over
   the years in the ability of these systems to withstand the effects of
   multipath fading.  As a result modern digital radio systems can meet
   international standards for error performance with margins to spare. 
   These advances have been made possible by a long list of technological
   innovations such as:  error correction, errorless protection switching,
   robust receiver techniques, digital transversal equalizers, adaptive
   digital linearizers and methods to counteract external interference. 
   Also very helpful has been a better understanding of the physical
