@article{ ISI:A1992JL22800002,
Author = {RISSET, JC},
Title = {{DIGITAL RECORDING OF SOUND - ACOUSTICS WITHOUT MECHANICAL CONSTRAINTS}},
Journal = {{JOURNAL DE PHYSIQUE IV}},
Year = {{1992}},
Volume = {{2}},
Number = {{C1, 1}},
