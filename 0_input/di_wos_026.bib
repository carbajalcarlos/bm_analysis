@article{ ISI:000074569500002,
Author = {Felder, MD and Mason, JC and Evans, BL},
Title = {{Efficient dual-tone multifrequency detection using the nonuniform
   discrete Fourier transform}},
Journal = {{IEEE SIGNAL PROCESSING LETTERS}},
Year = {{1998}},
Volume = {{5}},
Number = {{7}},
Pages = {{160-163}},
Month = {{JUL}},
Abstract = {{The International Telecommunication Union (ITU) recommendations for
   dual-tone multifrequency (DTMF) signaling: are mot met by conventional
   DTMF detectors. We present an efficient DTMF detection algorithm based
   ore the nonuniform discrete Fourier transform that meets all of the ITU
   recommendations. The key innovations are the use of two sliding windows
   and development of sophisticated timing tests. Oar algorithm requires no
   buffering of input samples. To perform DTMF detection on n telephone
