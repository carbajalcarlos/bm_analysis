@inproceedings{ ISI:000076167500014,
Author = {Zontul, H},
Editor = {{Corsi, C and Kudrya, S}},
Title = {{Academic computer networks, technological opportunities for late comers:
   Turkish case}},
Booktitle = {{GLOBALIZATION OF SCIENCE AND TECHNOLOGY: A WAY FOR C.I.S. COUNTRIES TO
   NEW MARKETS}},
Series = {{NATO ADVANCED SCIENCE INSTITUTES SERIES, SUB-SERIES 4, SCIENCE AND
   TECHNOLOGY POLICY}},
