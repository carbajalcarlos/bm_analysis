@incollection{ ISI:000419663200016,
Author = {Audije-Gil, Julia and Cambra-Moo, Oscar and Garcia Gil, Orosia and
   Gonzalez Martin, Armando},
Editor = {{Quintana, AG and Tonicher, PC and BarrosoBarcenilla, F}},
Title = {{Mapping the anatomy}},
Booktitle = {{MAPAS DE LA NATURALEZA OS MAPAS DA NATUREZA}},
