@inproceedings{ ISI:000416965000110,
Author = {Prawata, Albertus Galih},
Editor = {{Saloma and Borgan, WR and Buntoro, F and Victor}},
Title = {{Rethinking the Architectural Design Concept in the Digital Culture (in
   Architecture's Practice Perspective)}},
Booktitle = {{3RD INTERNATIONAL CONFERENCE ON CONSTRUCTION AND BUILDING ENGINEERING
   (ICONBUILD 2017)}},
Series = {{AIP Conference Proceedings}},
Year = {{2017}},
Volume = {{1903}},
Note = {{3rd International Conference on Construction and Building Engineering
   (ICONBUILD), Palembang, INDONESIA, AUG 14-17, 2017}},
Abstract = {{The architectural design stages in architectural practices or in
   architectural design studio consist of many aspects. One of them is
   during the early phases of the design process, where the architects or
   designers try to interpret the project brief into the design concept.
   This paper is a report of the procedure of digital tools in the early
   design process in an architectural practice in Jakarta. It targets
   principally the use of BIM and digital modeling to generate information
   and transform them into conceptual forms, which is not very common in
   Indonesian architectural practices. Traditionally, the project brief is
   transformed into conceptual forms by using sketches, drawings, and
   physical model. The new method using digital tools shows that it is
   possible to do the same thing during the initial stage of the design
   process to create early architectural design forms. Architect's
   traditional tools and methods begin to be replaced effectively by
   digital tools, which would drive bigger opportunities for innovation.}},
