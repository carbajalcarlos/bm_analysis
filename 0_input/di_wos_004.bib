Editor = {{Misra, S and Matthews, VO and Adewumi, A}},
Title = {{Conceptual Model of Social-Private Partnership for E-Skill Delivery}},
Booktitle = {{PROCEEDINGS OF THE IEEE INTERNATIONAL CONFERENCE ON COMPUTING NETWORKING
   AND INFORMATICS (ICCNI 2017)}},
