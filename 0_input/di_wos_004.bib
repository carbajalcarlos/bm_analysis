@article{ ISI:000423972300004,
Author = {Troitsky, Yu.},
Title = {{News as Literature: About One Experimental Practice}},
Journal = {{NOVYI FILOLOGICHESKII VESTNIK-NEW PHILOLOGICAL BULLETIN}},
Year = {{2017}},
Number = {{42}},
Pages = {{52-58}},
Abstract = {{The article attempts to present innovate practice of news discourse
   aestheticization as the way to contextual understanding. According to
   the author's observations, the actual segmentation of news sentences,
   the asymmetry of the topic-comment relations revealed in the direction
   of the predominance of the comment. It allows and even forces readers
   and news interpreters to constantly supplement and complete their
   content, including using elements of the artistic language. In some
   cases (for example, in the experiments by D.L. Bykov) hybrid types of
   texts can arise in this case. The work introduces some new concepts
   (metonymic news modality, news scaling, meta-comment perspective) and
   analytical procedures (analysis of news understanding contexts). The
   possibility of considering a news narrative as a protoliterary
   phenomenon substantiated. An assumption made about the use of digital
   technologies for news analytics.}},
