@inproceedings{ ISI:000424741600252,
Author = {Karetnikov, V. and Ratner, E. and Ageeva, A.},
Editor = {{Cornish, DR}},
Title = {{Introduction of the automated hydrographic survey systems for creation
   of electronic nautical charts of internal waterways in Southern Africa}},
Booktitle = {{2017 IEEE AFRICON}},
