@inproceedings{ ISI:000424890100059,
Author = {Tuan Minh Vo and Samori, Carlo and Lacaita, Andrea L. and Levantino,
   Salvatore},
Book-Group-Author = {{IEEE}},
Title = {{A Novel Segmentation Scheme for DTC-based Delta Sigma Fractional-N PLL}},
Booktitle = {{2017 IEEE INTERNATIONAL SYMPOSIUM ON CIRCUITS AND SYSTEMS (ISCAS)}},
Series = {{IEEE International Symposium on Circuits and Systems}},
Year = {{2017}},
Pages = {{242-245}},
Note = {{IEEE International Symposium on Circuits and Systems (ISCAS), Baltimore,
   MD, MAY 28-31, 2017}},
Organization = {{IEEE; IEEE Circuits \& Syst Soc}},
Abstract = {{The adoption of the digital/time converter (DTC) circuit has improved
   the performance of Delta Sigma fractional-N phase-locked loops (PLLs).
   Accurate cancellation of Delta Sigma quantization error via the DTC
   requires an automatic calibration made by an LMS loop. A high-order
   Delta Sigma speeds up calibration convergence and improves PLL spectral
   purity, though at the price of larger quantization error and wider DTC
   range. To overcome this problem, we propose an innovative parallel
   segmentation scheme which reduces the range of quantization error
   without compromising spectral purity and convergence speed. The
   effectiveness of the proposed segmentation scheme is demonstrated via
   behavioral-level simulations of a digital PLL and compared to the
   conventional cascaded segmentation scheme.}},
