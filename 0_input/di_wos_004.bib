@inproceedings{ ISI:000426794000247,
Author = {Alonso, Diego and Pastor, Juan and Alvarez, Barbara and Suarez, Tanya
   and Tasic, Igor},
Book-Group-Author = {{IEEE}},
Title = {{Improving the Learning Experience and Outcomes in Entrepreneurial
   Courses}},
Booktitle = {{2017 IEEE 26TH INTERNATIONAL SYMPOSIUM ON INDUSTRIAL ELECTRONICS (ISIE)}},
Series = {{Proceedings of the IEEE International Symposium on Industrial
   Electronics}},
Year = {{2017}},
Pages = {{1581-1586}},
Note = {{26th IEEE International Symposium on Industrial Electronics (ISIE),
   Edinburgh, SCOTLAND, JUN 18-21, 2017}},
Organization = {{Inst Elect \& Elect Engineers; IEEE Ind Elect Soc; Anglia Ruskin Univ}},
Abstract = {{Entrepreneurship has been gaining momentum is Europe over the last years
   as a way to reduce unemployment levels, and in line with the take-off of
   the digital economy in the world. Entrepreneurship education is still an
   open research area, with many courses being launched in almost all
   universities. We describe our experience in running a summer school and
   mentoring the teams created in the context of the EU-XCeL project, and
   ICT focused project funded by European Union's Horizon 2020 Research and
   Innovation Programme. From this experience, we extract some conclusions
   and recommendations for preparing better courses and learning
