@inproceedings{ ISI:000425916100018,
Author = {Petri, Giani and von Wangenheim, Christiane Gresse and Borgatto, Adriano
   Ferreti},
Book-Group-Author = {{IEEE}},
Title = {{Quality of Games for Teaching Software Engineering: An Analysis of
   Empirical Evidences of Digital and Non-digital Games}},
Booktitle = {{2017 IEEE/ACM 39TH INTERNATIONAL CONFERENCE ON SOFTWARE ENGINEERING:
   SOFTWARE ENGINEERING EDUCATION AND TRAINING TRACK (ICSE-SEET 2017)}},
Year = {{2017}},
