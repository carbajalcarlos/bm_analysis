@inproceedings{ ISI:000426884800083,
Author = {Lu, Xiao and Dounas, Theodoros and Spaeth, Benjamin and Bissoonauth,
   Chitraj and Galobardes, Isaac},
Editor = {{Janssen, P and Loh, P and Raonic, A and Schnabel, MA}},
Title = {{ROBOTIC SIMULATION OF TEXTILE AS CONCRETE REINFORCEMENT AND FORMWORK}},
Booktitle = {{PROCEEDINGS OF THE 22ND INTERNATIONAL CONFERENCE ON COMPUTER-AIDED
   ARCHITECTURAL DESIGN RESEARCH IN ASIA (CAADRIA 2017): PROTOCOLS, FLOWS
   AND GLITCHES}},
Year = {{2017}},
Pages = {{863-872}},
Note = {{22nd CAADRIA Annual International Conference on Computer-Aided
   Architectural Design Research in Asia (CAADRIA), Xian Jiaotong Liverpool
   Univ, Dept Architecture, Suzhou, PEOPLES R CHINA, APR 05-08, 2017}},
Organization = {{Assoc Comp Aided Architectural Design Res Asia}},
Abstract = {{New possibilities of concrete constructions in architecture, the
   traditional formwork can be gradually replaced by the use of flexible
   textile. At the same time textile reinforcement combined with fabric
   formwork, introduces an innovative integrated solution in the
   fabrication of concrete. Based on a simple understanding of the textile
   weaving and knitting techniques, this project concentrates on the
   architectural production and the structural optimization of the textile
   as both concrete reinforcement and formwork. Furthermore, we present a
   robotic simulation of the process that develops using a series of
   computational experiments to research the sequence of weaving and/or
   knitting. Through the computational process and the design simulations,
   the research is firmly rooted in analog and digital exploration of
