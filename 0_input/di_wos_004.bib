@article{ ISI:000422844200004,
Author = {Rola, Rinki and Varghese, Vinod},
Title = {{FINANCIAL RE-ENGINEERING: AN INNOVATIVE INITIATIVE FOR FINANCIAL
   INCLUSION}},
