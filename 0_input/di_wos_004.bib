Title = {{What Concerns Game Developers? A Study on Game Development Processes,
   Sustainability and Metrics}},
Booktitle = {{2017 IEEE/ACM 8TH WORKSHOP ON EMERGING TRENDS IN SOFTWARE METRICS
   (WETSOM)}},
Series = {{International Workshop on Emerging Trends in Software Metrics}},
Year = {{2017}},
