@article{ ISI:000415223600007,
Author = {Madhavan, Sundararajan and Yamuna, Govindarajan},
Title = {{Optimisation for video watermarking using ABC algorithm}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL SCIENCE AND ENGINEERING}},
Year = {{2017}},
Volume = {{15}},
Number = {{3-4, SI}},
Pages = {{226-235}},
Abstract = {{Video watermarking is relatively an innovative tool that has been
   proposed to solve the problem of illegal manipulation and sharing of
   digital video. It is the process of embedding copyright information into
   video watermarking. In this paper, artificial bee colony algorithm is
   utilised for finding the frame and location for embedding the grey-scale
   image into video watermarking and then searches a scene and location
   into which a particular part of watermarking is best to embed. The
   number of shot frames and locations is identified using ABC algorithm.
   Once the best frame and locations are identified, the embedding and
   extraction procedure is carried out. The performance of the proposed
   algorithm is analysed with the existing technique using PSNR and NC.
   This technique is tested against different attacks and the results
   obtained are encouraging.}},
