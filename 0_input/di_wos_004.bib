@inproceedings{ ISI:000419843900185,
Author = {Schuh, Guenther and Kreutzer, Ramon and Patzwald, Marc},
Editor = {{Kocaoglu, DF and Anderson, TR and Daim, TU and Kozanoglu, DC and Niwa, K and Perman, G and Steenhuis, HJ}},
Title = {{Assessing the Value of Data An Approach to Evaluate the Technology
   Driven Benefits of Smart Product Data}},
Booktitle = {{2017 PORTLAND INTERNATIONAL CONFERENCE ON MANAGEMENT OF ENGINEERING AND
   TECHNOLOGY (PICMET)}},
