@inproceedings{ ISI:000177818100025,
Author = {Nosenzo, G and Whelan, MP and Dalton, T},
Editor = {{Tomasini, EP}},
Title = {{Structural integrity analysis of vibrating composite structures based on
   optical fibre strain sensing and finite element model updating}},
Booktitle = {{FIFTH INTERNATIONAL CONFERENCE ON VIBRATION MEASUREMENTS BY LASER
   TECHNIQUES: ADVANCES AND APPLICATIONS}},
