@article{ ISI:000079727800010,
Author = {Cowling, T and Bondurant, PD and Lalumandier, S},
Title = {{Innovative furnace tube inspection system}},
Journal = {{MATERIALS EVALUATION}},
Year = {{1999}},
Volume = {{57}},
Number = {{4}},
Pages = {{447-449}},
Month = {{APR}},
Abstract = {{A new and innovative furnace tube inspection system (FTIS) has been
   developed that combines laser profilometry and ultrasonic thickness
   technologies. The system has been designed specifically to inspect the
   serpen-tine tubing networks typical of petrochemical refinery furnaces.
   The FTIS is an autonomous instrument, which is propelled at 0.61 m/s (2
   ft/s) by a column of clean water As the instrument passes through the
   furnace tubing, it optically profiles the inner tubing surface with a
   high speed rotating laser profiler The laser profiler provides high
   density inner radius measurements, while an ultrasonic module
   simultaneously measures tubing wall thickness and records redundant
   radial measurements at a lower sampling density. FTIS employs on board
   state of the art digital electronics to store the recorded data as they
   are collected. After completing the inspection run, the data are then
   downloaded to a portable computer for analysis and display. FTIS is
   currently being used in the field and is targeted to replace current
   conventional furnace inspection practices.}},
