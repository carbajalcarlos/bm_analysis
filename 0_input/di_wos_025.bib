@article{ ISI:000168285400005,
Author = {Villegier, JC and Hadacek, N and Monso, S and Delaet, B and Roussy, A
   and Febvre, P and Lamura, G and Laval, JY},
Title = {{NbN multilayer technology on R-plane sapphire}},
