@article{ ISI:000167346300016,
Author = {Richir, S and Taravel, B and Samier, H},
Title = {{Information networks and technological innovation for industrial
   products}},
Journal = {{INTERNATIONAL JOURNAL OF TECHNOLOGY MANAGEMENT}},
Year = {{2001}},
Volume = {{21}},
Number = {{3-4}},
Pages = {{420-427}},
Abstract = {{This article is illustrated by factual industrial cases and shows how
   influential the generalization of information digital exchange is on the
   innovation process regarding companies, from engineering design to
   commercialization of industrial products.
   Computer networks, either internal (Intranet), `private' external
   (Extranet) or `public' external (Internet) make a `digital chain' of
