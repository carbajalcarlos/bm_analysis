@article{ ISI:000082199200060,
Author = {Baum, G and Birsa, R and Bradamante, F and Bressan, A and Colavita, A
   and Crespo, M and Costa, S and Dalla Torre, S and Fauland, P and Finger,
   M and Fratnik, F and Giorgi, M and Gobbo, B and Grasso, A and Lamanna, M
   and Martin, A and Menon, G and Panzieri, D and Schiavon, P and
   Tessarotto, F and Zanetti, AM},
Title = {{BORA: a front end board, with local intelligence, for the RICH detector
   of the Compass Collaboration}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
Year = {{1999}},
Volume = {{433}},
