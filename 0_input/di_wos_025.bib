@inproceedings{ ISI:000177816900002,
Author = {Colosi, F and Gabrielli, R},
Editor = {{Stancic, Z and Veljanovski, T}},
Title = {{Integrated use of DGPS and the total station for the survey of
   archaeological sites: The case of Colle Breccioso}},
Booktitle = {{COMPUTING ARCHAEOLOGY FOR UNDERSTANDING THE PAST, PROCEEDINGS: COMPUTER
   APPLICATIONS AND QUANTITATIVE METHODS IN ARCHAEOLOGY}},
