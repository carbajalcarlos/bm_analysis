@inproceedings{ ISI:000169435500006,
Author = {Krikelis, A},
Editor = {{Panchanathan, S and Bove, VM and Sudharsanan, SI}},
Title = {{VASP-4096: A very high-performance programmable device for digital media
   processing applications}},
Booktitle = {{MEDIA PROCESSORS 2001}},
