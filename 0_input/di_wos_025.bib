@inproceedings{ ISI:000088436600042,
Author = {Lemke, J},
Editor = {{Aktan, EM and Gosselin, SR}},
Title = {{A remote vibration monitoring system using wireless Internet data
   transfer}},
Booktitle = {{NONDESTRUCTIVE EVALUATION OF HIGHWAYS, UTILITIES, AND PIPELINES IV}},
Series = {{PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)}},
Year = {{2000}},
