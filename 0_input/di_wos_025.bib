@inproceedings{ ISI:000169103300030,
Author = {Gandsas, A and Montgomery, K and McIntire, K and Altrudi, R},
Editor = {{Westwood, JD and Hoffman, HM and Mogel, GT and Stredney, D and Robb, RA}},
Title = {{Wireless vital sign telemetry to hand held computers}},
Booktitle = {{MEDICINE MEETS VIRTUAL REALITY 2001: OUTER SPACE, INNER SPACE, VIRTUAL
   SPACE}},
