@inproceedings{ ISI:000180411500022,
Author = {Yuan, C and Zhong, YZ and Yang, SQ},
Editor = {{Tescher, AG and Vasudev, B and Bove, VM and Divakaran, A}},
Title = {{An innovative hyperchaotic encryption algorithm for compressed video}},
Booktitle = {{MULTIMEDIA SYSTEMS AND APPLICATIONS V}},
Series = {{PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)}},
Year = {{2002}},
Volume = {{4861}},
Pages = {{203-215}},
Note = {{Conference on Multimedia Systems and Applications V, BOSTON, MA, JUL
   29-30, 2002}},
Organization = {{SPIE}},
Abstract = {{It is accepted that stream cryptosystem, which implements encryption by
   selecting a few parts of the block data and header information of the
   compressed video stream, can achieve a good real-time performance and a
   high flexibility. Chaotic random number generator, for example Logistics
   Map, is a comparatively promising substitute, but it is vulnerable to
   attacks by nonlinear dynamic forecasting and geometric information
   extracting. In this paper, a hyperchaotic cryptography scheme is
   presented to encrypt the compressed video, which integrates Logistics
   Map with Z(2(32)-1) field linear congruential algorithm to strengthen
   the security of the mono-chaotic cryptography, and meanwhile maintains
   the real-time performance and flexibility of the chaotic sequence
   cryptography. The scheme also integrates with the dissymmetrical
   public-key cryptography and implements encryption and identity
   authentification on control parameters at. the initialization phase. In
   accordance with the importance of data in compressed video stream,
   encryption is performed in layered scheme. In the innovative
   hyperchaotic cryptography, the value and the updating frequency of
   control parameters can be changed online to satisfy the requirements of
   the network quality and processor capability, as well as security
   requirements. The innovative hyperchaotic cryprography guarantees robust
   security by cryptoanalysis, and proves a good real-time performance and
   a high implementation flexibility through the arithmetic evaluation and
   test.}},
