@article{ ISI:000328882800001,
Author = {Gu, Jiang and Taylor, Clive R.},
Title = {{Practicing Pathology in the Era of Big Data and Personalized Medicine}},
Journal = {{APPLIED IMMUNOHISTOCHEMISTRY \& MOLECULAR MORPHOLOGY}},
