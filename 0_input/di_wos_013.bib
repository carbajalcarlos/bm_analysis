@inproceedings{ ISI:000348561600126,
Author = {Noordin, M. N. A. and Hudzari, R. M. and Sapuan, S. M. and Ssomad, M. A.
   H. A.},
Editor = {{Zulkefli, ZA and Salit, MS and Tahir, SM and ElShekeil, Y and Ishak, MR and Hairuddin, AA and Asarry, A and Bakar, KFA and Khalid, R}},
Title = {{Design, Analysis and Fabrication of Conceptual Innovative Hand Tool}},
