@inproceedings{ ISI:000361384700065,
Author = {Di Mascio, Danilo and Maver, Tom},
Editor = {{Thompson, EM}},
Title = {{Investigating a narrative architecture Mackintosh's Glasgow School of
   Art}},
Booktitle = {{FUSION: DATA INTEGRATION AT ITS BEST, VOL 1}},
Year = {{2014}},
