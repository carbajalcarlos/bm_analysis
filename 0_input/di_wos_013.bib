@inproceedings{ ISI:000351435200033,
Author = {Ketelhut, Diane Jass and Nelson, Brian and Bergey, Bradley and Ryu,
   Minjung},
Editor = {{Busch, C}},
Title = {{Design and Gender in Immersive Learning Environments}},
Booktitle = {{PROCEEDINGS OF THE 8TH EUROPEAN CONFERENCE ON GAMES BASED LEARNING
   (ECGBL 2014), VOLS 1 AND 2}},
