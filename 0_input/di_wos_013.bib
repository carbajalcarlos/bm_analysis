Title = {{A new 3D tracking method for Cell Mechanics investigation exploiting the
   capabilities of Digital Holography in Microscopy}},
Booktitle = {{IMAGING, MANIPULATION, AND ANALYSIS OF BIOMOLECULES, CELLS, AND TISSUES
   XII}},
Series = {{Proceedings of SPIE}},
