@inproceedings{ ISI:000337169400038,
Author = {Hempel, Jon and McBratney, Alex B. and Arrouays, Dominique and McKenzie,
   Neil and Hartemink, Alfred E. and Grundy, Mike and Greve, Mogens and
   Hong, Suk-Young and Lelyk, Glenn and Libohova, Zamir},
