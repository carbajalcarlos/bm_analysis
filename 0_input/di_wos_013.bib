@article{ ISI:000344985600006,
Author = {Thomas, Peter},
Title = {{A Case Study of Using a Hypermedia Pedagogy in a Secondary English
   Classroom}},
Journal = {{ENGLISH IN AUSTRALIA}},
Year = {{2014}},
Volume = {{49}},
Number = {{1}},
Pages = {{53-61}},
Abstract = {{This paper is about the classroom use of digital literacies. It
   describes using a hypermedia-authoring program with middle-years
   students at an Australian secondary school. Classroom environments were
   created in which collaborative and innovative group work built on
   technical facilities and expertise developed by students outside of
   school. The teaching of non-linear hypermedia stories and instruction in
   software use to create such stories are represented as a case study. The
   author argues that the three-part model for understanding literacy as
   having an operational, a cultural and a critical dimension can be an
   effective way to deal with the occasionally intimidating experience of
   digital literacies in education.}},
