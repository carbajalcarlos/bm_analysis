@article{ ISI:000332474000031,
Author = {Bentley, R. Alexander and O'Brien, Michael J. and Brock, William A.},
Title = {{Mapping collective behavior in the big-data era}},
Journal = {{BEHAVIORAL AND BRAIN SCIENCES}},
Year = {{2014}},
Volume = {{37}},
