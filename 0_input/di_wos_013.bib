@inproceedings{ ISI:000355276500053,
Author = {Chen, Kai and Min, Jia and Han, Xianghui and Yan, Xiao and Duan, Yan and
   Zhang, Long and Feng, Zhiyong},
Book-Group-Author = {{IEEE}},
Title = {{Spectrum Survey for TV Band in Beijing}},
Booktitle = {{2014 21ST INTERNATIONAL CONFERENCE ON TELECOMMUNICATIONS (ICT)}},
Year = {{2014}},
Pages = {{267-271}},
Note = {{21st International Conference on Telecommunications (ICT), Lisbon,
   PORTUGAL, MAY 04-07, 2014}},
Organization = {{IEEE Advancing Technol Human; IEEE Commun Soc; IEEE VTS Portugal
   Chapter; Fundacao Ciencia Technologia; Inst Telecomunicacoes; ISCTE IUL;
   UNINOVA; IST; UBI; APDC; Rohde \& Schwarz; Meru Networks \& INDRA; Ordem
   Engn}},
Abstract = {{The rapid development of wireless communication calls for new spectrum
   resources. TV band is considered the most promising band for spectrum
   reallocation and spectrum efficient technologies such as cognitive
   radio. To facilitate spectrum innovation in this band, we conducted both
   fixed and mobile measurements in Beijing, China. Unlike previous
   spectrum surveys which mainly focused on average spectrum occupancy, we
   analyze the usage of TV band from a comprehensive view. Multiple factors
   including frequency, time, space domain and specific TV standards are
   all considered. We find the actual utilization in Beijing is 7\% higher
   than reported in previous study. Moreover, the time and geographical
   utilization peak of TV coincides with current communication systems,
   which results in limited time and space domain spectrum opportunity for
   cognitive radio. We strongly suggest that China accelerate the
   transition from analog television to digital television to leave more
   available spectrum resources for other services.}},
