@inproceedings{ ISI:000337300500156,
Author = {Lin, Bor-Shen and Tsai, Shang-Te},
Editor = {{Meen, TH and Prior, SD and Lam, ADKT}},
Title = {{Pattern search and style analysis for the design of comic figures}},
