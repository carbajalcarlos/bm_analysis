@article{ ISI:000430326600007,
Author = {Spadafora, Natalie and Marini, Zopito},
Title = {{Self-Regulation and ``Time Off ``: Evaluations and Reflections on the
   Development of a Blended Course}},
Journal = {{CANADIAN JOURNAL FOR THE SCHOLARSHIP OF TEACHING AND LEARNING}},
Year = {{2018}},
Volume = {{9}},
Number = {{1}},
Abstract = {{Changes in both the landscape of education and digital technology are
   giving rise to interesting, innovative, and potentially effective
   pedagogical possibilities. As educators of the 21st century, we are
   witnessing continuous changes in the way we teach as well as the way
   students learn. This study is a part of an ongoing research program
   aimed at evaluating the pedagogical effectiveness of blended teaching.
   More specifically, this paper aims to describe some of our reflections
   as we developed, delivered, and carried out an evaluation of a
   university course taught using a blended format. Undergraduate students
   (n=109) in a fourth-year capstone course participated in this study. We
   report on the students' perceptions of the effectiveness of the delivery
   method of the course, while focusing on the struggles they encountered,
   including difficulties keeping up with the course content, particularly
   on the weeks where the lecture was online, as well as a struggle to stay
   organized and connected with their peers and their assignments.
   Self-regulation turned out to be a major determinant of success in this
   type of course. We conclude with suggestions to improve students' social
   and academic experiences as they navigate learning in a digital world.}},
