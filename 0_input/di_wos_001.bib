@article{ ISI:000424793900005,
Author = {Wang, Xiang-yang and Xu, Huan and Zhang, Si-yu and Liang, Lin-lin and
   Niu, Pan-pan and Yang, Hong-ying},
Title = {{A Color Image Watermarking Approach Based on Synchronization Correction}},
