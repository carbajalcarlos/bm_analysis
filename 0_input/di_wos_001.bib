@article{ ISI:000428706500009,
Author = {Zamani, Efpraxia D. and Giaglis, George M.},
Title = {{With a little help from the miners: distributed ledger technology and
   market disintermediation}},
Journal = {{INDUSTRIAL MANAGEMENT \& DATA SYSTEMS}},
