@article{ ISI:000426491100006,
Author = {Chen, Fulong and You, Jiangbin and Tang, Panpan and Zhou, Wei and
   Masini, Nicola and Lasaponara, Rosa},
Title = {{Unique performance of spaceborne SAR remote sensing in cultural heritage
   applications: Overviews and perspectives}},
Journal = {{ARCHAEOLOGICAL PROSPECTION}},
