@inproceedings{ ISI:000432607700057,
Author = {Sykownik, Philipp and Emmerich, Katharina and Masuch, Maic},
Editor = {{Cheok, AD and Inami, M and Romao, T}},
Title = {{Exploring Patterns of Shared Control in Digital Multiplayer Games}},
