@article{ ISI:000427510300078,
Author = {Verde, Sebastiano and Pretto, Niccolo and Milani, Simone and Canazza,
   Sergio},
Title = {{Stay True to the Sound of History: Philology, Phylogenetics and
   Information Engineering in Musicology}},
Journal = {{APPLIED SCIENCES-BASEL}},
Year = {{2018}},
Volume = {{8}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{This work investigates computational musicology for the study of tape
   music works tackling the problems concerning stemmatics. These
   philological problems have been analyzed with an innovative approach
   considering the peculiarities of audio tape recordings. The paper
   presents a phylogenetic reconstruction strategy that relies on
   digitizing the analyzed tapes and then converting each audio track into
   a two-dimensional spectrogram. This conversion allows adopting a set of
   computer vision tools to align and equalize different tracks in order to
   infer the most likely transformation that converts one track into
   another. In the presented approach, the main editing techniques,
   intentional and unintentional alterations and different configurations
   of a tape recorded are estimated in phylogeny analysis. The proposed
   solution presents a satisfying robustness to the adoption of the wrong
   reading setup together with a good reconstruction accuracy of the
   phylogenetic tree. The reconstructed dependencies proved to be correct
   or plausible in 90\% of the experimental cases.}},
