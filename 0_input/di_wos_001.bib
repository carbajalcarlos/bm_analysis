@article{ ISI:000430768500019,
Author = {Hofierka, Jaroslav and Gallay, Michal and Bandura, Peter and Sasak, Jan},
Title = {{Identification Identification of karst sinkholes in a forested karst
   landscape using airborne laser scanning data and water flow analysis}},
Journal = {{GEOMORPHOLOGY}},
Year = {{2018}},
